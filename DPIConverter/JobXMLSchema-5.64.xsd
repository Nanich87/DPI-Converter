  <!-- ******************************************************************** -->
  <!-- ************************  Type declarations  *********************** -->
  <!-- ******************************************************************** -->

  <xsd:simpleType name="angleType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file angle values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0"/>
      <xsd:maxExclusive value="360"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="deltaAngleType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file angle values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minExclusive value="-360"/>
      <xsd:maxExclusive value="360"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="latitudeType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file latitude values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="-90"/>
      <xsd:maxInclusive value="90"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="longitudeType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file longitude values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="-180"/>
      <xsd:maxInclusive value="180"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="distType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file distance/measurement values are in metres</xsd:documentation>
    </xsd:annotation>
  <!-- All distance values (including prism constants) and antenna/target/instrument height values are output in metres. -->
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="coordType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file coordinate values are in metres</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="gradeType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file grades values (gradients) are percentages</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="areaType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file area values are in square metres</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="volumeType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file volume values are in cubic metres</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="timeIntervalType">
    <xsd:annotation>
      <xsd:documentation>All JobXML time interval values are in seconds</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:simpleType name="timeOffsetType">
    <xsd:annotation>
      <xsd:documentation>All JobXML time offset values are in hours (and decimals to allow for half hour time zones)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="-13"/>
      <xsd:maxInclusive value="12"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="percentType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>A percentage value (in percent). E.g. 0.0, 20.5, -45.6, 100.0</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="refractionCoefficientType">  <!-- New type for V5.2 schema -->
    <xsd:annotation>
      <xsd:documentation>Refraction coefficients are double values between -1.0 and 1.0</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="-1"/>
      <xsd:maxInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="booleanType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="true"/>
      <xsd:enumeration value="false"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="idType">
  <!-- Used for all record ID's and references. -->
    <xsd:restriction base="xsd:hexBinary"/>
  </xsd:simpleType>

  <xsd:simpleType name="nameType">       <!-- New type for V5.1 schema - used for entity names -->
    <xsd:annotation>
      <xsd:documentation>Names must consist of at least 1 alphanumeric character (not a space)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:whiteSpace value="collapse"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="timeStampType">
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="emptyString">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- The following types are defined to allow for cases in JobXML files   -->
  <!-- where a value can be empty (null).  The types are created by setting -->
  <!-- up a union with the emptyString type                                 -->

  <!-- Discovered that in validation using a Web site schema the union     -->
  <!-- definitions don't work if they refer to types defined within the    -->
  <!-- schema itself, so the unions are now all based directly on standard -->
  <!-- schema base types.                                                  --> 

  <xsd:simpleType name="doubleOrNullType">
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="integerOrNullType">
    <xsd:union memberTypes="xsd:integer">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="idOrNullType">
    <xsd:union memberTypes="xsd:hexBinary">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="angleOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file angle values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minInclusive value="0"/>
          <xsd:maxExclusive value="360"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="deltaAngleOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file angle values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minExclusive value="-360"/>
          <xsd:maxExclusive value="360"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="distOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file distance/measurement values are in metres</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="coordOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file coordinate values are in metres</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="gradeOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file grade values (gradients) are percentages</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="areaOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file area values are in square metres</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="volumeOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file volume values are in cubic metres</xsd:documentation>
    </xsd:annotation>
    <xsd:union memberTypes="xsd:double">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="latitudeOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file latitude values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minInclusive value="-90"/>
          <xsd:maxInclusive value="90"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="longitudeOrNullType">
    <xsd:annotation>
      <xsd:documentation>All JobXML file longitude values are in decimal degrees</xsd:documentation>
    </xsd:annotation>
    <xsd:union>
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minInclusive value="-180"/>
          <xsd:maxInclusive value="180"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value=""/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="lineMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid line methods are TwoPoints or BearingAndDistance</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TwoPoints"/>
      <xsd:enumeration value="BearingAndDistance"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="arcMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid arc methods are TwoPointsAndRadius, ArcLengthAndRadius, DeltaAngleAndRadius, 
                         IntersectionPointAndTangents, TwoPointsAndCenterPoint or ThreePoints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TwoPointsAndRadius"/>
      <xsd:enumeration value="ArcLengthAndRadius"/>
      <xsd:enumeration value="DeltaAngleAndRadius"/>
      <xsd:enumeration value="IntersectionPointAndTangents"/>
      <xsd:enumeration value="TwoPointsAndCenterPoint"/>
      <xsd:enumeration value="ThreePoints"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="directionType">
    <xsd:annotation>
      <xsd:documentation>Valid directions are Right or Left</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Right"/>
      <xsd:enumeration value="Left"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="directionAlongLineType">
    <xsd:annotation>
      <xsd:documentation>Valid directions along a line are InFromStart, OutFromStart, InFromEnd or OutFromEnd</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="InFromStart"/>
      <xsd:enumeration value="OutFromStart"/>
      <xsd:enumeration value="InFromEnd"/>
      <xsd:enumeration value="OutFromEnd"/>
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="pointCreationType">
    <xsd:annotation>
      <xsd:documentation>This field indicates the method used for the point creation</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Unknown"/>
      <xsd:enumeration value="Coordinates"/>
      <xsd:enumeration value="FourPointIntersection"/>
      <xsd:enumeration value="FromABaseline"/>
      <xsd:enumeration value="BearingAndDistanceFromAPoint"/>
      <xsd:enumeration value="BearingDistancePointIntersection"/>
      <xsd:enumeration value="BearingVerticalPlaneIntersection"/>
      <xsd:enumeration value="BearingDistanceIntersection"/>
      <xsd:enumeration value="BearingBearingIntersection"/>
      <xsd:enumeration value="DistanceDistanceIntersection"/>
      <xsd:enumeration value="ProjectPointToArc"/>              <!-- New method for the V5.4 schema -->
      <xsd:enumeration value="GpsStaticObservation"/>
      <xsd:enumeration value="GpsFastStaticObservation"/>
      <xsd:enumeration value="GpsRTKControlPoint"/>
      <xsd:enumeration value="GpsRTKQuickMark"/>
      <xsd:enumeration value="Gps2PointOffset"/>
      <xsd:enumeration value="Gps4PointIntersection"/>
      <xsd:enumeration value="GpsCheckShot"/>
      <xsd:enumeration value="GpsLoggingInitialisation"/>
      <xsd:enumeration value="GpsStakeOutObservation"/>
      <xsd:enumeration value="GpsCalibrationPoint"/>
      <xsd:enumeration value="GpsSingleAutonomousPoint"/>
      <xsd:enumeration value="GpsAverageAutonomousPoint"/>
      <xsd:enumeration value="GpsContinuousDistancePoint"/>
      <xsd:enumeration value="GpsContinuousTimePoint"/>
      <xsd:enumeration value="GpsContinuousDistanceAndTimePoint"/>
      <xsd:enumeration value="GpsContinuousDistanceOrTimePoint"/>
      <xsd:enumeration value="GpsContinuousStopAndGoPt"/>       <!-- New method added in Survey Controller v11.30 -->
      <xsd:enumeration value="GpsContinuousOffsetPointOne"/>    <!-- New method name from v11.20 on -->
      <xsd:enumeration value="GpsContinuousOffsetPointTwo"/>    <!-- New method name from v11.20 on -->
      <xsd:enumeration value="GpsContinuousOffsetLeftPoint"/>   <!-- Redundant name for method output by v11.10 and earlier systems -->
      <xsd:enumeration value="GpsContinuousOffsetRightPoint"/>  <!-- Redundant name for method output by v11.10 and earlier systems -->
      <xsd:enumeration value="CogoCalculated"/>
      <xsd:enumeration value="AlignmentOffsetCalculated"/>      <!-- New method for the V5.1 schema -->
      <xsd:enumeration value="AzimuthOnly"/>
      <xsd:enumeration value="AngleOnly"/>
      <xsd:enumeration value="HorizontalAngleOnly"/>
      <xsd:enumeration value="DirectReading"/>
      <xsd:enumeration value="AverageMeasurements"/>
      <xsd:enumeration value="AngleOffset"/>
      <xsd:enumeration value="HorizontalAngleOffset"/>
      <xsd:enumeration value="VerticalAngleOffset"/>
      <xsd:enumeration value="DistanceOffset"/>
      <xsd:enumeration value="DualPrismOffset"/>
      <xsd:enumeration value="CircularObject"/>
      <xsd:enumeration value="RemoteObject"/>
      <xsd:enumeration value="RemoteObjectBasePoint"/>          <!-- New method for the V5.2 schema -->
      <xsd:enumeration value="MatchedPair"/>
      <xsd:enumeration value="MatchedPairHidden"/>
      <xsd:enumeration value="MeanTurnedAngle"/>
      <xsd:enumeration value="MeanTurnedAngleHidden"/>
      <xsd:enumeration value="Resection"/>
      <xsd:enumeration value="LaserOffset"/>
      <xsd:enumeration value="CopiedPoint"/>
      <xsd:enumeration value="FromBase"/>
      <xsd:enumeration value="TraverseAdjusted"/>
      <xsd:enumeration value="TurnedAngleAndDistance"/>
      <xsd:enumeration value="ReflineSetup"/>
      <xsd:enumeration value="LevelObservation"/>        <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
      <xsd:enumeration value="ReducedLevelObservation"/> <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
      <xsd:enumeration value="FromABaselineWithErrorDistribution"/> <!-- New in Version 5.0 of schema - for Trimble Survey Manager --> 
      <xsd:enumeration value="ProjectPointToLine"/> <!-- New in Version 5.0 of schema - for Trimble Survey Manager --> 
      <xsd:enumeration value="ProjectPointToLineDefinedByAzimuth"/> <!-- New in Version 5.0 of schema - for Trimble Survey Manager --> 
      <xsd:enumeration value="PhotogrammetryIntersection"/>  <!-- New in Version 5.6 of schema -->
      <xsd:enumeration value="GpsTiltCompensatedPoint"/>     <!-- New in Version 5.61 of schema -->
      <xsd:enumeration value="PointOnPlane"/>                <!-- New in Version 5.62 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="surveyMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid survey methods are KeyedIn, Autonomous, Float, Fix, Copied, Code, WAAS, Inertial, NetworkFloat, 
                         NetworkFix, SingleWireLeveling, ThreeWireLeveling, ElectronicLevel, TrigLeveling, OmniSTARHP,
                         OmniSTARVBS, xFillFloat, xFillFix, RTK, NetworkRTK, RTKxFill, RTX, RTXFast, Leveling and xFillX</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="KeyedIn"/>
      <xsd:enumeration value="Autonomous"/>
      <xsd:enumeration value="Float"/>
      <xsd:enumeration value="Fix"/>
      <xsd:enumeration value="Copied"/>
      <xsd:enumeration value="Code"/>
      <xsd:enumeration value="WAAS"/>
      <xsd:enumeration value="Inertial"/>
      <xsd:enumeration value="NetworkFloat"/>
      <xsd:enumeration value="NetworkFix"/>
      <xsd:enumeration value="SingleWireLeveling"/> <!-- New in Version 4.0 of schema - for TDS SurveyPro level data (effectively replaced by levelMethodType - plan to deprecate in future) -->
      <xsd:enumeration value="ThreeWireLeveling"/>  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data (effectively replaced by levelMethodType - plan to deprecate in future) -->
      <xsd:enumeration value="ElectronicLevel"/>    <!-- New in Version 4.0 of schema - for TDS SurveyPro level data (effectively replaced by levelMethodType - plan to deprecate in future) -->
      <xsd:enumeration value="TrigLeveling"/>       <!-- New in Version 4.0 of schema - for TDS SurveyPro level data (effectively replaced by levelMethodType - plan to deprecate in future) -->
      <xsd:enumeration value="OmniSTARHP"/>         <!-- New in Version 5.55 of schema -->
      <xsd:enumeration value="OmniSTARVBS"/>        <!-- New in Version 5.55 of schema -->
      <xsd:enumeration value="xFillFloat"/>         <!-- New in Version 5.56 of schema -->
      <xsd:enumeration value="xFillFix"/>           <!-- New in Version 5.56 of schema -->
      <xsd:enumeration value="RTK"/>                <!-- New in Version 5.56 of schema -->
      <xsd:enumeration value="NetworkRTK"/>         <!-- New in Version 5.56 of schema -->
      <xsd:enumeration value="RTKxFill"/>           <!-- New in Version 5.56 of schema -->
      <xsd:enumeration value="RTX"/>                <!-- New in Version 5.57 of schema -->
      <xsd:enumeration value="RTXFast"/>            <!-- New in Version 5.57 of schema -->
      <xsd:enumeration value="Leveling"/>           <!-- New in Version 5.57 of schema - for TDS SurveyPro level data covering all leveling methods -->
      <xsd:enumeration value="xFillX"/>             <!-- New in Version 5.64 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="classificationType">
    <xsd:annotation>
      <xsd:documentation>Valid classifications are Check, BackSight, Stakeout, Construction, Normal, MTA, Adjusted, 
                         Averaged, Control, ForeSight, SideShot, NetworkAdjusted or None</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="Check"/>
      <xsd:enumeration value="BackSight"/>
      <xsd:enumeration value="Stakeout"/>
      <xsd:enumeration value="Construction"/>
      <xsd:enumeration value="Normal"/>
      <xsd:enumeration value="MTA"/>
      <xsd:enumeration value="Adjusted"/>
      <xsd:enumeration value="Averaged"/>
      <xsd:enumeration value="Control"/>
      <xsd:enumeration value="ForeSight"/>        <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
      <xsd:enumeration value="SideShot"/>         <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
      <xsd:enumeration value="NetworkAdjusted"/>  <!-- New in Version 5.0 of schema - to allow identification of network adjusted points from the TBC system -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="levelMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid level method types are SingleWireLeveling, ThreeWireLeveling, ElectronicLevel or TrigLeveling</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SingleWireLeveling"/>
      <xsd:enumeration value="ThreeWireLeveling"/>
      <xsd:enumeration value="ElectronicLevel"/>
      <xsd:enumeration value="TrigLeveling"/>
    </xsd:restriction>
  </xsd:simpleType>
	
  <xsd:simpleType name="sightType">
    <xsd:annotation>
      <xsd:documentation>Valid sight types are BackSight and ForeSight</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BackSight"/>
      <xsd:enumeration value="ForeSight"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="levelPointType">   <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>Valid level point types are TurningPoint, BenchMark, SideShot, PegTest, Stakeout, ControlBenchMark or UndefinedPointType</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UndefinedPointType"/>
      <xsd:enumeration value="TurningPoint"/>
      <xsd:enumeration value="BenchMark"/>
      <xsd:enumeration value="SideShot"/>
      <xsd:enumeration value="PegTest"/>
      <xsd:enumeration value="Stakeout"/>
      <xsd:enumeration value="ControlBenchMark"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="monitorStatusType">
    <xsd:annotation>
      <xsd:documentation>Valid monitor status values are NotMonitored, MonitorFailed or MonitorOK</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotMonitored"/>
      <xsd:enumeration value="MonitorFailed"/>
      <xsd:enumeration value="MonitorOK"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="faceType">
    <xsd:annotation>
      <xsd:documentation>Valid values are Face1, Face2 or FaceNull</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="FaceNull"/>
      <xsd:enumeration value="Face1"/>
      <xsd:enumeration value="Face2"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="entityType">
    <xsd:annotation>
      <xsd:documentation>Valid entities are Line, Arc, Road, Alignment or Tunnel</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Line"/>
      <xsd:enumeration value="Arc"/>
      <xsd:enumeration value="Road"/>
      <xsd:enumeration value="Alignment"/>      <!-- New type for the V5.1 schema -->
      <xsd:enumeration value="Tunnel"/>         <!-- New type for the V5.55 schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="azimuthDisplayType">
    <xsd:annotation>
      <xsd:documentation>Valid azimuth display types are Grid, Magnetic, Local or WGS84</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Grid"/>
      <xsd:enumeration value="Magnetic"/>
      <xsd:enumeration value="Local"/>
      <xsd:enumeration value="WGS84"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="distDisplayType">
    <xsd:annotation>
      <xsd:documentation>Valid distance display types are Grid, Ground or Ellipsoid</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Grid"/>
      <xsd:enumeration value="Ground"/>
      <xsd:enumeration value="Ellipsoid"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="offsetDirectionType">
    <xsd:annotation>
      <xsd:documentation>Valid direction types are Right, Left, Out or In</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Right"/>
      <xsd:enumeration value="Left"/>
      <xsd:enumeration value="Out"/>
      <xsd:enumeration value="In"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- The following prismType is a new type included from Survey Controller v11.30 on.  -->
  <!-- It is used in the required element PrismType in the TargetRecord element.         -->
  <xsd:simpleType name="prismType">
    <xsd:annotation>
      <xsd:documentation>
          Valid prism types are SSeries360Prism, SSeriesTraversePrism, Small318mmTiltablePrism, Large635mmTiltablePrism,
          MiniPrism, SuperPrism, CustomPrism, VXSSeriesMultiTrack, DRTarget, R10_360Prism, ActiveTrack360 or UnknownPrism</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SSeries360Prism"/>
      <xsd:enumeration value="SSeriesTraversePrism"/>
      <xsd:enumeration value="Small318mmTiltablePrism"/>
      <xsd:enumeration value="Large635mmTiltablePrism"/>
      <xsd:enumeration value="MiniPrism"/>
      <xsd:enumeration value="SuperPrism"/>
      <xsd:enumeration value="CustomPrism"/>
      <xsd:enumeration value="VXSSeriesMultiTrack"/>
      <xsd:enumeration value="DRTarget"/>
      <xsd:enumeration value="R10_360Prism"/>
      <xsd:enumeration value="ActiveTrack360"/>
      <xsd:enumeration value="UnknownPrism"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="remoteTargetIDType">  <!-- New type for the V5.2 schema -->
    <xsd:annotation>
      <xsd:documentation>Valid Remote Target ID types are None, 0, 1, 2, 3, 4, 5, 6, 7, or 8</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="0"/>
      <xsd:enumeration value="1"/>
      <xsd:enumeration value="2"/>
      <xsd:enumeration value="3"/>
      <xsd:enumeration value="4"/>
      <xsd:enumeration value="5"/>
      <xsd:enumeration value="6"/>
      <xsd:enumeration value="7"/>
      <xsd:enumeration value="8"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="onOffType">
    <xsd:annotation>
      <xsd:documentation>Valid values are On or Off</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="On"/>
      <xsd:enumeration value="Off"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="measurementModeType">
    <xsd:annotation>
      <xsd:documentation>Valid measurement modes are Fine, Coarse, Track, 5mm, 10mm, 20mm, Default, AveragedTrack or Null</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Fine"/>
      <xsd:enumeration value="Coarse"/>
      <xsd:enumeration value="Track"/>
      <xsd:enumeration value="5mm"/>
      <xsd:enumeration value="10mm"/>
      <xsd:enumeration value="20mm"/>
      <xsd:enumeration value="Default"/>
      <xsd:enumeration value="AveragedTrack"/>  <!-- New enumeration for V5.62 schema -->
      <xsd:enumeration value="Null"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="setHorizCircleModeType">
    <xsd:annotation>
      <xsd:documentation>Valid horizontal circle setting modes are DoNothing, Zero, SetToAzimuth or SetHorizontalNull</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DoNothing"/>
      <xsd:enumeration value="Zero"/>
      <xsd:enumeration value="SetToAzimuth"/>
      <xsd:enumeration value="SetHorizontalNull"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="servoType">
    <xsd:annotation>
      <xsd:documentation>Valid servo types are HorizontalAngleOnly, HorizontalAndVerticalAngle, NoServo or VerticalAngleOnly</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="HorizontalAngleOnly"/>
      <xsd:enumeration value="HorizontalAndVerticalAngle"/>
      <xsd:enumeration value="NoServo"/>
      <xsd:enumeration value="VerticalAngleOnly"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="pressureDetailsFromType"> <!-- Added for v11.20 -->
    <xsd:annotation>
      <xsd:documentation>Valid pressure value sources are KeyedIn or ReadFromInstrument</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="KeyedIn"/>
      <xsd:enumeration value="ReadFromInstrument"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="scaleType">
    <xsd:annotation>
      <xsd:documentation>Valid scale types are Fixed or Free</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Fixed"/>
      <xsd:enumeration value="Free"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stationType">
    <xsd:annotation>
      <xsd:documentation>Valid station types are StationSetup, StationSetupPlus, StandardResection, 
                         HelmertResection, ReflineStationSetup, StationElevationSetup or Unknown</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Unknown"/>
      <xsd:enumeration value="StationSetup"/>
      <xsd:enumeration value="StationSetupPlus"/>
      <xsd:enumeration value="StandardResection"/>
      <xsd:enumeration value="HelmertResection"/>
      <xsd:enumeration value="ReflineStationSetup"/>
      <xsd:enumeration value="StationElevationSetup"/>    <!-- New enumeration for V5.51 schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="datumType">
    <xsd:annotation>
      <xsd:documentation>Valid datum types are NoDatum, SevenParameter, ThreeParameter or GridDatum</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoDatum"/>
      <xsd:enumeration value="SevenParameter"/>
      <xsd:enumeration value="ThreeParameter"/>
      <xsd:enumeration value="GridDatum"/>
      <xsd:enumeration value="RTCMGrid"/>   <!-- New enumeration for V5.2 schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="projectionType">
    <xsd:annotation>
      <xsd:documentation>Valid projection types are NoProjection, ScaleOnly, Plane, Mercator, TransverseMercator, UniversalTransverseMercator, 
                         Lambert1Parallel, Lambert2Parallel, ObliqueMercatorAngle, ObliqueConformalCylindrical, ObliqueStereographic, 
                         PolarStereographic, RDStereographic, AlbersEqualAreaConic, CassiniSoldner, Krovak, NewZealandMapGrid, 
                         UnitedKingdomNationalGrid, Denmark, HungarianEOV, StereographicDouble, RSO, UPSNorth, UPSSouth, ProjectionGrid, 
                         SnakeGrid or SCAPICustom</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoProjection"/>
      <xsd:enumeration value="ScaleOnly"/>
      <xsd:enumeration value="Plane"/>
      <xsd:enumeration value="Mercator"/>
      <xsd:enumeration value="TransverseMercator"/>
      <xsd:enumeration value="UniversalTransverseMercator"/>
      <xsd:enumeration value="Lambert1Parallel"/>
      <xsd:enumeration value="Lambert2Parallel"/>
      <xsd:enumeration value="ObliqueMercatorAngle"/>
      <xsd:enumeration value="ObliqueConformalCylindrical"/>
      <xsd:enumeration value="ObliqueStereographic"/>
      <xsd:enumeration value="PolarStereographic"/>
      <xsd:enumeration value="RDStereographic"/>
      <xsd:enumeration value="AlbersEqualAreaConic"/>
      <xsd:enumeration value="CassiniSoldner"/>
      <xsd:enumeration value="Krovak"/>
      <xsd:enumeration value="NewZealandMapGrid"/>
      <xsd:enumeration value="UnitedKingdomNationalGrid"/>
      <xsd:enumeration value="Denmark"/>
      <xsd:enumeration value="HungarianEOV"/>
      <xsd:enumeration value="StereographicDouble"/>
      <xsd:enumeration value="RSO"/>
      <xsd:enumeration value="UPSNorth"/>
      <xsd:enumeration value="UPSSouth"/>
      <xsd:enumeration value="ProjectionGrid"/>
      <xsd:enumeration value="SnakeGrid"/>
      <xsd:enumeration value="SCAPICustom"/>   <!-- New enumeration for V5.61 schema - indicates a custom projection definition set up by a Survey Core Plug-In applicaiton--> 
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="azimuthOriginAtType">
  <!-- Used for settings in the definition of Oblique Mercator (RSO) projections -->
    <xsd:annotation>
      <xsd:documentation>Valid values are Equator or ProjectionCentre</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Equator"/>
      <xsd:enumeration value="ProjectionCentre"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="denmarkAreaType">
  <!-- Areas specific to the Denmark projection type -->
    <xsd:annotation>
      <xsd:documentation>Valid Denmark projection zones are Zeeland, Jutland, Bornholm or None</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="Zeeland"/>
      <xsd:enumeration value="Jutland"/>
      <xsd:enumeration value="Bornholm"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="gridOrientType">
    <xsd:annotation>
      <xsd:documentation>Valid grid orientations are IncreasingNorthEast, IncreasingSouthWest, IncreasingNorthWest or IncreasingSouthEast</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IncreasingNorthEast"/>
      <xsd:enumeration value="IncreasingSouthWest"/>
      <xsd:enumeration value="IncreasingNorthWest"/>
      <xsd:enumeration value="IncreasingSouthEast"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="horizAdjType">
    <xsd:annotation>
      <xsd:documentation>Valid horizontal adjustment types are PlaneAdjustment or NoAdjustment</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoAdjustment"/>
      <xsd:enumeration value="PlaneAdjustment"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="vertAdjType">
    <xsd:annotation>
      <xsd:documentation>Valid vertical adjustment types are InclinedPlane, GeoidModel, GeoidModelAndInclinedPlane or NoAdjustment</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoAdjustment"/>
      <xsd:enumeration value="InclinedPlane"/>
      <xsd:enumeration value="GeoidModel"/>
      <xsd:enumeration value="GeoidModelAndInclinedPlane"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="localSiteType">
    <xsd:annotation>
      <xsd:documentation>Valid local site types are Grid, KeyedInGroundScaleFactor or CalculatedGroundScaleFactor</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Grid"/>
      <xsd:enumeration value="KeyedInGroundScaleFactor"/>
      <xsd:enumeration value="CalculatedGroundScaleFactor"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="distanceDisplayType">
    <xsd:annotation>
      <xsd:documentation>Valid distance display types are GridDistance, GroundDistance or EllipsoidDistance</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="GridDistance"/>
      <xsd:enumeration value="GroundDistance"/>
      <xsd:enumeration value="EllipsoidDistance"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="calibDimensionType">
    <xsd:annotation>
      <xsd:documentation>Valid calibration point pair dimensions are 0D (disabled), 1D, 2D or 3D</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0D"/>
      <xsd:enumeration value="1D"/>
      <xsd:enumeration value="2D"/>
      <xsd:enumeration value="3D"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="magneticSensorCalibDimensionType">
    <xsd:annotation>
      <xsd:documentation>Valid magnetic sensor calibration dimensions are 2D or 3D</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="2D"/>
      <xsd:enumeration value="3D"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="antennaMeasType">
    <xsd:annotation>
      <xsd:documentation>Valid antenna measurement types are Raw or Corrected</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Corrected"/>
      <xsd:enumeration value="Raw"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="surveyEventType">
    <xsd:annotation>
      <xsd:documentation>Valid survey event types are StartBaseSurvey, StartRoverSurvey, AbortSurvey, EndSurvey, BaseStarted, 
                         ReceiverModeSetError, CommunicationsError, InfillSystemAdopted, RealtimeSystemAdopted, StartContinuous or EndContinuous</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="StartBaseSurvey"/>
      <xsd:enumeration value="StartRoverSurvey"/>
      <xsd:enumeration value="AbortSurvey"/>
      <xsd:enumeration value="EndSurvey"/>
      <xsd:enumeration value="BaseStarted"/>
      <xsd:enumeration value="ReceiverModeSetError"/>
      <xsd:enumeration value="CommunicationsError"/>
      <xsd:enumeration value="InfillSystemAdopted"/>
      <xsd:enumeration value="RealtimeSystemAdopted"/>
      <xsd:enumeration value="StartContinuous"/>
      <xsd:enumeration value="EndContinuous"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="contTriggerMethodType">
  <!-- Continuous survey setting -->
    <xsd:annotation>
      <xsd:documentation>Valid continuous survey trigger methods are Distance, Time, DistanceAndTime, DistanceOrTime or StopAndGo</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Distance"/>
      <xsd:enumeration value="Time"/>
      <xsd:enumeration value="DistanceAndTime"/>
      <xsd:enumeration value="DistanceOrTime"/>
      <xsd:enumeration value="StopAndGo"/> <!-- New method available from Survey Controller v11.30 on -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="contOffsetType">
  <!-- Continuous survey setting -->
    <xsd:annotation>
      <xsd:documentation>Valid continuous survey offset types are None, One or Two</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="One"/>
      <xsd:enumeration value="Two"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="constOffsetAppliedType">
  <!-- Construction offset (roading) setting -->
    <xsd:annotation>
      <xsd:documentation>Valid construction offset application types are Horizontal, SlopePrevious or SlopeNext</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Horizontal"/>
      <xsd:enumeration value="Slope"/>          <!-- This item is redundant from Survey Controller v11.30 on -->
      <xsd:enumeration value="SlopePrevious"/>  <!-- New enumeration for Survey Controller v11.30 -->
      <xsd:enumeration value="SlopeNext"/>      <!-- New enumeration for Survey Controller v11.30 -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="constVertOffsetAppliedType"> <!-- This type is new for the Survey COntroller v11.30 -->
  <!-- Vertical construction offset (roading) setting -->
    <xsd:annotation>
      <xsd:documentation>Valid vertical offset application types are PerpendicularToElement or Vertically</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PerpendicularToElement"/>
      <xsd:enumeration value="Vertically"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="initEventType">
    <xsd:annotation>
      <xsd:documentation>Valid initialisation event types are GainedInitialisation, LostInitialisation, FailedInitialisation, 
                         BadInitialisation, HighRMS, GoodRMS, UserCancelled, RTKInitialised or RTKNotInitialised</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="GainedInitialisation"/>
      <xsd:enumeration value="LostInitialisation"/>
      <xsd:enumeration value="FailedInitialisation"/>
      <xsd:enumeration value="BadInitialisation"/>
      <xsd:enumeration value="HighRMS"/>
      <xsd:enumeration value="GoodRMS"/>
      <xsd:enumeration value="UserCancelled"/>
      <xsd:enumeration value="RTKInitialised"/>    <!-- New enumeration for the V5.56 schema -->
      <xsd:enumeration value="RTKNotInitialised"/> <!-- New enumeration for the V5.56 schema -->
      <xsd:enumeration value="SucceededInit"/>     <!-- New enumeration for the V5.56 schema -->
      <xsd:enumeration value="RTXConverged"/>      <!-- New enumeration for the V5.61 schema -->
      <xsd:enumeration value="RTXNotConverged"/>   <!-- New enumeration for the V5.61 schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="initialisationType">
    <xsd:annotation>
      <xsd:documentation>Valid initialisation types are Plate, KnownPoint, NewPoint, OnTheFly, ResetRTK, ResetSVTracking or Null</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Plate"/>
      <xsd:enumeration value="KnownPoint"/>
      <xsd:enumeration value="NewPoint"/>
      <xsd:enumeration value="OnTheFly"/>
      <xsd:enumeration value="ResetRTK"/>
      <xsd:enumeration value="ResetSVTracking"/>
      <xsd:enumeration value="Null"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="gpsSurveyType">
    <xsd:annotation>
      <xsd:documentation>Valid GPS survey types are RealTime, PostProcess or Infill</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RealTime"/>
      <xsd:enumeration value="PostProcess"/>
      <xsd:enumeration value="Infill"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid stakeout methods are ToThePoint, FromAPoint, FromStartPosition, FromLastPointStaked, PointRelativeToAzimuth, ToTheLine, ToTheArc, 
                         PointOffsetFromLine, PointOffsetFromArc, OffsetLine, OffsetArc, PointOnLine, PointOnArc, IntersectionPointOfArc, 
                         CentrePointOfArc, SlopeFromLine, SlopeFromArc, StationAndOffset, PositionOnRoad, SideSlopeFromAlignment, 
                         StationOnString, NearestOffset, StationAlongString, ToTheDTM, StationOnAlignment or PositionFromFile</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ToThePoint"/>
      <xsd:enumeration value="FromAPoint"/>
      <xsd:enumeration value="FromStartPosition"/>
      <xsd:enumeration value="FromLastPointStaked"/>
      <xsd:enumeration value="PointRelativeToAzimuth"/>
      <xsd:enumeration value="ToTheLine"/>
      <xsd:enumeration value="ToTheArc"/>
      <xsd:enumeration value="PointOffsetFromLine"/>
      <xsd:enumeration value="PointOffsetFromArc"/>
      <xsd:enumeration value="OffsetLine"/>
      <xsd:enumeration value="OffsetArc"/>
      <xsd:enumeration value="PointOnLine"/>
      <xsd:enumeration value="PointOnArc"/>
      <xsd:enumeration value="IntersectionPointOfArc"/>
      <xsd:enumeration value="CentrePointOfArc"/>
      <xsd:enumeration value="SlopeFromLine"/>
      <xsd:enumeration value="SlopeFromArc"/>
      <xsd:enumeration value="StationAndOffset"/>
      <xsd:enumeration value="PositionOnRoad"/>
      <xsd:enumeration value="SideSlopeFromAlignment"/>
      <xsd:enumeration value="StationOnString"/>
      <xsd:enumeration value="NearestOffset"/>
      <xsd:enumeration value="StationAlongString"/>    <!-- New method for the V5.1 schema -->
      <xsd:enumeration value="ToTheDTM"/>
      <xsd:enumeration value="StationOnAlignment"/>    <!-- New method for the V5.1 schema -->
      <xsd:enumeration value="PositionFromFile"/>      <!-- New method for the V5.56 schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="horizontalOffsetMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid horizontal offset methods are Tangential to master or Tangential to substring</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Tangential to master"/>
      <xsd:enumeration value="Tangential to substring"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="eachStationClassType">
    <xsd:annotation>
      <xsd:documentation>Valid station classes are StraightToSpiral, SpiralToArc, ArcToSpiral, SpiralToStraight, SpiralToSpiral, StraightToArc, 
                         ArcToStraight, IntersectionPoint, CrossSection, VerticalCurveStart, VerticalCurveEnd, VerticalPoint, RoadStart, RoadEnd, 
                         TemplateAssignment, SuperelevationStart, WideningStart, WideningEnd, SuperelevationEnd, WideningMaximum, SuperelevationMaximum, 
                         SuperelevationEqual, SuperelevationInterest, VerticalSagPoint, VerticalSummitPoint or StationEquation</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="StraightToSpiral"/>
      <xsd:enumeration value="SpiralToArc"/>
      <xsd:enumeration value="ArcToSpiral"/>
      <xsd:enumeration value="SpiralToStraight"/>
      <xsd:enumeration value="SpiralToSpiral"/>
      <xsd:enumeration value="StraightToArc"/>
      <xsd:enumeration value="ArcToStraight"/>
      <xsd:enumeration value="IntersectionPoint"/>
      <xsd:enumeration value="CrossSection"/>
      <xsd:enumeration value="VerticalCurveStart"/>
      <xsd:enumeration value="VerticalCurveEnd"/>
      <xsd:enumeration value="VerticalPoint"/>
      <xsd:enumeration value="RoadStart"/>
      <xsd:enumeration value="RoadEnd"/>
      <xsd:enumeration value="TemplateAssignment"/>
      <xsd:enumeration value="SuperelevationStart"/>
      <xsd:enumeration value="WideningStart"/>
      <xsd:enumeration value="WideningEnd"/>
      <xsd:enumeration value="SuperelevationEnd"/>
      <xsd:enumeration value="WideningMaxium"/>          <!-- Old enumeration with spelling mistake kept for backward compatibility -->
      <xsd:enumeration value="WideningMaximum"/>         <!-- Correct spelling mistake from pre v12.20 -->
      <xsd:enumeration value="SuperelevationMaximum"/>
      <xsd:enumeration value="SuperelevationEqual"/>     <!-- New in Survey Controller v11.30 -->
      <xsd:enumeration value="SuperelevationInterest"/>  <!-- New in Survey Controller v11.30 -->
      <xsd:enumeration value="VerticalSagPoint"/>
      <xsd:enumeration value="VerticalSummitPoint"/>
      <xsd:enumeration value="StationEquation"/>         <!-- New in Survey Controller v12.20 -->
      <xsd:enumeration value="NoneToArc"/>               <!-- New for V5.2 of schema -->
      <xsd:enumeration value="NoneToStraight"/>          <!-- New for V5.2 of schema -->
      <xsd:enumeration value="ArcToArcCompound"/>        <!-- New for V5.2 of schema -->
      <xsd:enumeration value="ArcToArcReverse"/>         <!-- New for V5.2 of schema -->
      <xsd:enumeration value="ArcToNone"/>               <!-- New for V5.2 of schema -->
      <xsd:enumeration value="StraightToNone"/>          <!-- New for V5.2 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stationClassType">
  <!-- The station class for a staked point may have more than one
       enumeration that applies so it is presented as a space separated list -->
    <xsd:list itemType="eachStationClassType"/>
  </xsd:simpleType>

  <xsd:simpleType name="sideSlopeFromAlignMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid side slope from alignment methods are OffsetAndElevation, OffsetAndGrade or OffsetAndVerticalDistance</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="OffsetAndElevation"/>
      <xsd:enumeration value="OffsetAndGrade"/>
      <xsd:enumeration value="OffsetAndVerticalDistance"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="sideSlopeType">
    <xsd:annotation>
      <xsd:documentation>Valid side slope types are HorizontalAndVerticalDistance, GradeAndSlopeDistance or GradeAndHorizontalDistance</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="HorizontalAndVerticalDistance"/>
      <xsd:enumeration value="GradeAndSlopeDistance"/>
      <xsd:enumeration value="GradeAndHorizontalDistance"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="adjustmentMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid traverse adjustment methods are Compass or Transit</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Compass"/>
      <xsd:enumeration value="Transit"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="errorDistributionType">
    <xsd:annotation>
      <xsd:documentation>Valid traverse angular and elevation adjustment methods are ProportionalToDistance, EqualProportions or None</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ProportionalToDistance"/>
      <xsd:enumeration value="EqualProportions"/>
      <xsd:enumeration value="None"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="closureStatusType">
    <xsd:annotation>
      <xsd:documentation>Valid traverse closure status values are NotAdjusted, Closed or Adjusted</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotAdjusted"/>
      <xsd:enumeration value="Closed"/>
      <xsd:enumeration value="Adjusted"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="distUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid distance units (for display) are Metres, InternationalFeet or USSurveyFeet</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Metres"/>
      <xsd:enumeration value="InternationalFeet"/>
      <xsd:enumeration value="USSurveyFeet"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="angleUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid angle units (for display) are DMSDegrees, DecimalDegrees, Gons, Mils or QuadrantBearings</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DMSDegrees"/>
      <xsd:enumeration value="DecimalDegrees"/>
      <xsd:enumeration value="Gons"/>
      <xsd:enumeration value="Mils"/>
      <xsd:enumeration value="QuadrantBearings"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="azimuthFormatType">
    <xsd:annotation>
      <xsd:documentation>Valid azimuth formats (for display) are Azimuth or QuadrantBearings</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Azimuth"/>
      <xsd:enumeration value="QuadrantBearings"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="latLongUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid latitude and longitude units (for display) are DMSDegrees or DecimalDegrees</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DMSDegrees"/>
      <xsd:enumeration value="DecimalDegrees"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="coordOrderType">
    <xsd:annotation>
      <xsd:documentation>Valid coordinate order settings (for display) are North-East-Elevation, East-North-Elevation, 
                         Y-X-Z (Y = North) or X-Y-Z (Y = North)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="North-East-Elevation"/>
      <xsd:enumeration value="East-North-Elevation"/>
      <xsd:enumeration value="Y-X-Z"/>
      <xsd:enumeration value="X-Y-Z"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="tempUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid temperature units (for display) are Celsius or Fahrenheit</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Celsius"/>
      <xsd:enumeration value="Fahrenheit"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="pressUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid pressure units (for display) are MilliBar, InchHg or mmHg</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="MilliBar"/>
      <xsd:enumeration value="InchHg"/>
      <xsd:enumeration value="mmHg"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="gradeUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid grade (gradient) display settings are Percentage, RatioRiseRun, RatioRunRise or Angle</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Percentage"/>
      <xsd:enumeration value="RatioRiseRun"/>
      <xsd:enumeration value="RatioRunRise"/>
      <xsd:enumeration value="Angle"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="areaUnitsType">
    <xsd:annotation>
      <xsd:documentation>Valid area units (for display) are SquareMetres, SquareMiles, SquareFeet, SquareUSSurveyFeet, Acres or Hectares</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SquareMetres"/>
      <xsd:enumeration value="SquareMiles"/>
      <xsd:enumeration value="SquareFeet"/>
      <xsd:enumeration value="SquareUSSurveyFeet"/>  <!-- New for V5.2 of schema -->
      <xsd:enumeration value="Acres"/>
      <xsd:enumeration value="Hectares"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="volumeUnitsType">  <!-- New for V5.52 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid volume units (for display) are CubicMetres, CubicFeet, CubicUSSurveyFeet, 
                         CubicYards, CubicUSSurveyYards, AcreFeet or USAcreFeet</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CubicMetres"/>
      <xsd:enumeration value="CubicFeet"/>
      <xsd:enumeration value="CubicUSSurveyFeet"/>
      <xsd:enumeration value="CubicYards"/>
      <xsd:enumeration value="CubicUSSurveyYards"/>
      <xsd:enumeration value="AcreFeet"/>    <!-- New for V5.56 of schema -->
      <xsd:enumeration value="USAcreFeet"/>  <!-- New for V5.56 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stationingFormatType">
    <xsd:annotation>
      <xsd:documentation>Valid stationing formats (for display) are 1000.0, 10+00.0, 1+000.0 or StationIndex</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1000.0"/>
      <xsd:enumeration value="10+00.0"/>
      <xsd:enumeration value="1+000.0"/>
      <xsd:enumeration value="StationIndex"/>  <!-- New for V5.57 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="azimuthType">
    <xsd:annotation>
      <xsd:documentation>Valid azimuth types are Grid, Magnetic, Local, WGS84, Sun or empty (null)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Grid"/>
      <xsd:enumeration value="Magnetic"/>
      <xsd:enumeration value="Local"/>
      <xsd:enumeration value="WGS84"/>
      <xsd:enumeration value="Sun"/>
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="distanceType">
    <xsd:annotation>
      <xsd:documentation>Valid distance types are Grid, Ground or Ellipsoid</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Grid"/>
      <xsd:enumeration value="Ground"/>
      <xsd:enumeration value="Ellipsoid"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="attributeType">
    <xsd:annotation>
      <xsd:documentation>Valid attribute types are Numeric, Text, Menu, Date, Time, File, Integer or Photo</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Numeric"/>
      <xsd:enumeration value="Text"/>
      <xsd:enumeration value="Menu"/>
      <xsd:enumeration value="Date"/>
      <xsd:enumeration value="Time"/>
      <xsd:enumeration value="File"/>
      <xsd:enumeration value="Integer"/>  <!-- New for V5.2 of schema -->
      <xsd:enumeration value="Photo"/>    <!-- New for V5.2 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="computeAzimuthMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid computed azimuth methods are BetweenTwoPoints, BisectedAzimuths, 
                         BisectedCorner, AzimuthPlusTurnedAngle or AzimuthToLineOffset</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BetweenTwoPoints"/>
      <xsd:enumeration value="BisectedAzimuths"/>
      <xsd:enumeration value="BisectedCorner"/>
      <xsd:enumeration value="AzimuthPlusTurnedAngle"/>
      <xsd:enumeration value="AzimuthToLineOffset"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="computeDistMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid computed distance methods are BetweenTwoPoints, BetweenPointAndLine or BetweenPointAndArc</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BetweenTwoPoints"/>
      <xsd:enumeration value="BetweenPointAndLine"/>
      <xsd:enumeration value="BetweenPointAndArc"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="cogoTranslationMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid cogo translation methods are EnteredDeltas or TwoPoints</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="EnteredDeltas"/>
      <xsd:enumeration value="TwoPoints"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="contOffsetDirectionType">
  <!-- Continuous survey setting -->
    <xsd:annotation>
      <xsd:documentation>A continuous survey offset element Horizontal and Vertical distance component elements</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Horizontal" type="distType"/>
      <xsd:element name="Vertical" type="distType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="ellipsoidRecType"> 
    <xsd:annotation>
      <xsd:documentation>An ellipsoid element contains EarthRadius (distance) and Flattening (ratio) elements</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="EarthRadius" type="distOrNullType"/>
      <xsd:element name="Flattening" type="doubleOrNullType"/>
      <!-- Flattening is specified as a decimal value eg. 1/297 recorded as 0.00336700... -->
      <xsd:element ref="Notes" minOccurs="0"/>  <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="ellipsoidRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>An ellipsoid element contains EarthRadius (distance) and Flattening (ratio) elements</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ellipsoidRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="datumRecType"> 
    <xsd:annotation>
      <xsd:documentation>An datum element contains the datum type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="datumType"/>
      <xsd:element name="Direction" type="xsd:string" fixed="WGS84ToLocal" minOccurs="0"/> <!-- Present when Type = SevenParameter | ThreeParameter | GridDatum -->
      <xsd:element name="GridName" type="xsd:string" minOccurs="0"/>                       <!-- Present when Type = GridDatum | RTCMGrid. Note: the combined datum grid file name is written without the .cdg file extension and the RTCM transformation grid file name without the .rtd file extension --> 
      <xsd:element name="EarthRadius" type="distOrNullType" minOccurs="0"/>                <!-- Present when Type = SevenParameter | ThreeParameter -->
      <xsd:element name="Flattening" type="doubleOrNullType" minOccurs="0"/>               <!-- Present when Type = SevenParameter | ThreeParameter -->
      <!-- Flattening is specified as a decimal value eg. 1/297 recorded as 0.00336700... -->
      <xsd:element name="RotationX" type="deltaAngleType" minOccurs="0"/>                  <!-- Present when Type = SevenParameter -->
      <xsd:element name="RotationY" type="deltaAngleType" minOccurs="0"/>                  <!-- Present when Type = SevenParameter -->
      <xsd:element name="RotationZ" type="deltaAngleType" minOccurs="0"/>                  <!-- Present when Type = SevenParameter -->
      <xsd:element name="TranslationX" type="distType" minOccurs="0"/>                     <!-- Present when Type = SevenParameter | ThreeParameter -->
      <xsd:element name="TranslationY" type="distType" minOccurs="0"/>                     <!-- Present when Type = SevenParameter | ThreeParameter -->
      <xsd:element name="TranslationZ" type="distType" minOccurs="0"/>                     <!-- Present when Type = SevenParameter | ThreeParameter -->
      <xsd:element name="Scale" type="xsd:double" minOccurs="0"/>                          <!-- Present when Type = SevenParameter -->
      <xsd:element ref="Notes" minOccurs="0"/>                                             <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="datumRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>An datum element contains the datum type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="datumRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="horizAdjRecType">
    <xsd:annotation>
      <xsd:documentation>A horizontal adjustment element contains the adjustment type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="horizAdjType"/>
      <xsd:element name="OriginNorth" type="coordType" minOccurs="0"/>     <!-- Not present if Type = NoAdjustment -->
      <xsd:element name="OriginEast" type="coordType" minOccurs="0"/>      <!-- Not present if Type = NoAdjustment -->
      <xsd:element name="TranslationNorth" type="distType" minOccurs="0"/> <!-- Not present if Type = NoAdjustment -->
      <xsd:element name="TranslationEast" type="distType" minOccurs="0"/>  <!-- Not present if Type = NoAdjustment -->
      <xsd:element name="Rotation" type="deltaAngleType" minOccurs="0"/>   <!-- Not present if Type = NoAdjustment -->
      <xsd:element name="ScaleFactor" type="xsd:double" minOccurs="0"/>    <!-- Not present if Type = NoAdjustment -->
      <xsd:element ref="Notes" minOccurs="0"/>                             <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="horizAdjRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>A horizontal adjustment element contains the adjustment type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="horizAdjRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="vertAdjRecType">
    <xsd:annotation>
      <xsd:documentation>A vertical adjustment element contains the adjustment type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="vertAdjType"/>
      <xsd:element name="OriginNorth" type="coordType" minOccurs="0"/>        <!-- Present if Type = InclinedPlane | GeoidModelAndInclinedPlane -->
      <xsd:element name="OriginEast" type="coordType" minOccurs="0"/>         <!-- Present if Type = InclinedPlane | GeoidModelAndInclinedPlane -->
      <xsd:element name="SlopeNorthPerUnit" type="xsd:double" minOccurs="0"/> <!-- Present if Type = InclinedPlane | GeoidModelAndInclinedPlane -->
      <xsd:element name="SlopeEastPerUnit" type="xsd:double" minOccurs="0"/>  <!-- Present if Type = InclinedPlane | GeoidModelAndInclinedPlane -->
      <xsd:element name="ConstantAdjustment" type="distType" minOccurs="0"/>  <!-- Present if Type = InclinedPlane | GeoidModelAndInclinedPlane -->
      <xsd:element name="GeoidName" type="xsd:string" minOccurs="0"/>         <!-- Present if Type = GeoidModel | GeoidModelAndInclinedPlane. Note: the geoid grid file name is written without the .ggf file extension -->
      <xsd:element ref="Notes" minOccurs="0"/>                                <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="vertAdjRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>A vertical adjustment element contains the adjustment type and its associated parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="vertAdjRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="unitsRecType"> 
    <xsd:annotation>
      <xsd:documentation>The Environment section DisplaySettings element records the display units that were set in the job at the time of export</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="DistanceUnits" type="distUnitsType"/>
      <xsd:element name="HeightUnits" type="distUnitsType"/>
      <xsd:element name="AngleUnits" type="angleUnitsType"/>
      <xsd:element name="AzimuthFormat" type="azimuthFormatType"/>
      <xsd:element name="LatitudeLongitudeUnits" type="latLongUnitsType"/>
      <xsd:element name="CoordinateOrder" type="coordOrderType"/>
      <xsd:element name="TemperatureUnits" type="tempUnitsType"/>
      <xsd:element name="PressureUnits" type="pressUnitsType"/>
      <xsd:element name="GradeUnits" type="gradeUnitsType"/>
      <xsd:element name="AreaUnits" type="areaUnitsType"/>
      <xsd:element name="VolumeUnits" type="volumeUnitsType"/>  <!-- New for V5.52 of schema -->
      <xsd:element name="StationingFormat" type="stationingFormatType"/>
      <xsd:element name="StationIndexIncrement" type="distOrNullType"/>     <!-- New for V5.57 of schema -->
      <xsd:element ref="Notes" minOccurs="0"/>                              <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="unitsRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>The UnitsRecord element records the display units that were set in the job at this time in the field work</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="unitsRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="timeZoneRecType">
    <xsd:annotation>
      <xsd:documentation>The Environment section TimeZoneRecord element records the time zone name and 
                         offset (in hours) that applied at the time of export</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="ZoneName" type="xsd:string"/>
      <xsd:element name="HoursToUTC" type="timeOffsetType"/>
      <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="timeZoneRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>The TimeZoneRecord element records the time zone name and offset (in hours) that applies at this 
                         time in the field work (and will be output to the job again if it changes during the course of the job)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="timeZoneRecType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="surveyOptionsType">
    <xsd:annotation>
      <xsd:documentation>This element records base or rover GPS receiver elevation and PDOP mask settings</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="ElevationMask" type="xsd:double"/>
      <xsd:element name="PDOPMask" type="xsd:double"/>
      <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
    <xsd:attribute name="ID" type="idType" use="required"/>
    <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="GPSTimeType">
    <xsd:annotation>
      <xsd:documentation>This element records the GPS time as the GPS week and the seconds within the week</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="GPSWeek" type="xsd:integer"/>
      <xsd:element name="Seconds" type="xsd:double"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="lineArcDesignType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides the design details for a line or arc stakeout element</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Name" type="nameType"/>
      <xsd:element name="Code" type="xsd:string"/>
      <xsd:element name="StakeoutMethod" type="stakeoutMethodType"/>
      <xsd:element name="Station" type="coordType"/>
      <xsd:element name="Offset" type="coordOrNullType"/>
      <xsd:element name="Elevation" type="coordOrNullType"/>
      <xsd:element name="VerticalOffset" type="coordType"/>                 <!-- New for V5.2 of schema -->
      <xsd:element name="StakedFromElementStartPoint" type="booleanType"/>  <!-- New for V5.0 schema - indicates direction of staking with respect to the original line/arc definition -->
      <xsd:element name="ElementLength" type="distType"/>                   <!-- New for V5.0 schema - total length of line/arc -->
      <xsd:element name="OriginalDesignElevation" type="coordOrNullType" minOccurs="0"/> <!-- Present if the design elevation has been edited in Trimble Access prior to staking out -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="leftRightSlopeDesignType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides the design details for a staked slope position stakeout element</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="sideSlopeType"/>
      <xsd:element name="HorizontalDistance" type="distType"/>
      <xsd:element name="VerticalDistance" type="distType"/>
      <xsd:element name="SlopeDistance" type="distType"/>
      <xsd:element name="Grade" type="gradeType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="polarType"> 
    <xsd:annotation>
      <xsd:documentation>A Polar element that provides the azimuth, horizontal distance and vertical distance between two positions</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Azimuth" type="angleOrNullType"/>
      <xsd:element name="HorizontalDistance" type="distOrNullType"/>
      <xsd:element name="VerticalDistance" type="distOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="itemType">
    <xsd:simpleContent>
      <xsd:extension base='xsd:string'>
        <xsd:attribute name="n" type="xsd:integer" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="traverseLegType">
    <xsd:annotation>
      <xsd:documentation>A traverse leg element records names of the from and to points for the leg and a Polar element that 
                         provides the azimuth (grid), horizontal distance (ground distance) and vertical distance for the leg</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="FromPoint" type="nameType"/>
      <xsd:element name="ToPoint" type="nameType"/>
      <!-- A polar element is used to record the Azimuth, HorizontalDistance and VerticalDistance -->
      <!-- values for the traverse leg.  In all cases the Azimuth values are grid azimuths and    -->
      <!-- HorizontalDistance values are ground distances (Ellipsoid and projection scale factor  -->
      <!-- corrections are applied as the adjusted coordinates are computed) -->
      <xsd:element name="Polar" type="polarType"/>
    </xsd:all>
    <xsd:attribute name="n" type="xsd:integer" use="required"/>
  </xsd:complexType>

  <xsd:simpleType name="instTargetMeasurementMethodType">
    <xsd:annotation>
      <xsd:documentation>Valid instrument and target measurement methods are TrueHeight, BottomNotch, BottomOfV10 or LeverOfV10Extension</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TrueHeight"/>
      <xsd:enumeration value="BottomNotch"/>
      <xsd:enumeration value="BottomOfV10"/> <!-- New in Version 5.61 of schema -->
      <xsd:enumeration value="LeverOfV10Extension"/> <!-- New in Version 5.61 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="rawInstTargetHeightType">
    <xsd:annotation>
      <xsd:documentation>The raw instrument and target height elements record the measurement method along with the measurement and the offsets that apply</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="MeasurementMethod" type="instTargetMeasurementMethodType"/>
      <xsd:element name="MeasuredHeight" type="distOrNullType"/>
      <xsd:element name="HorizontalOffset" type="distType"/>
      <xsd:element name="VerticalOffset" type="distType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="circleType">
    <xsd:annotation>
      <xsd:documentation>Circle elements record horizontal and vertical circle angle readings and the measured EDM distance. 
                         The instrument face and the angular and distance standard errors for the single observation are also recorded.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="HorizontalCircle" type="angleOrNullType"/> <!-- May be null if a horizontal angle not recorded -->
      <xsd:element name="VerticalCircle" type="angleOrNullType"/>   <!-- May be null if a vertical angle not recorded -->
      <xsd:element name="EDMDistance" type="distOrNullType"/>       <!-- May be null if a distance not measured -->
      <xsd:element name="Face" type="faceType"/>
      <xsd:element name="HorizontalCircleStandardError" type="angleOrNullType"/>
      <xsd:element name="VerticalCircleStandardError" type="angleOrNullType"/>
      <xsd:element name="EDMDistanceStandardError" type="distOrNullType"/>
      <xsd:element name="EDMMeasurementMode" type="measurementModeType" minOccurs="0"/>
      <xsd:element name="RawVerticalCircle" type="angleType" minOccurs="0"/>   <!-- New for V5.2 of schema --> <!-- When a prism type requiring an internal vertical angle correction has been used (S Series 360deg, VX/S Series MultiTrack, R10 360 or Active Track 360 prism) this element provides the original measured vertical angle without the internal correction -->
      <xsd:element name="RawEDMDistance" type="distOrNullType" minOccurs="0"/> <!-- New for V5.2 of schema --> <!-- When a prism type requiring an internal EDM distance correction has been used (S Series 360deg, VX/S Series MultiTrack, R10 360 or Active Track 360 prism) this element provides the original measured EDM distance without the internal correction -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="gridPosType"> 
    <xsd:annotation>
      <xsd:documentation>A grid coordinate position providing North, East and Elevation values for a point</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="North" type="coordOrNullType"/>
      <xsd:element name="East" type="coordOrNullType"/>
      <xsd:element name="Elevation" type="coordOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="localGridPosType"> 
    <xsd:annotation>
      <xsd:documentation>A local grid coordinate position providing the local grid North, East and Elevation 
                         values for a point along with the ID of the transformation that that will transform
                         the local position to or from a database grid position.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="North" type="coordOrNullType"/>
      <xsd:element name="East" type="coordOrNullType"/>
      <xsd:element name="Elevation" type="coordOrNullType"/>
      <xsd:element name="ReferenceTransformation" type="xsd:string"/>  <!-- The transformation is referenced by name in the job as the transformation may be updated -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="gridDeltaType"> 
    <xsd:annotation>
      <xsd:documentation>Grid deltas providing the North, East and Elevation coordinate differences 
                        (typically this element will provide the deltas from a measured position to a design position)</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="DeltaNorth" type="distOrNullType"/>
      <xsd:element name="DeltaEast" type="distOrNullType"/>
      <xsd:element name="DeltaElevation" type="distOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="gridStdDeviationsType">  <!-- Added for v11.20 -->
    <xsd:annotation>
      <xsd:documentation>This element provides the grid standard deviations for an averaged point</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="NorthStandardDeviation" type="distOrNullType"/>
      <xsd:element name="EastStandardDeviation" type="distOrNullType"/>
      <xsd:element name="ElevationStandardDeviation" type="distOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="rotationDefnType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides the rotation definition for a cogo transformation operation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="RotationOriginPoint" type="nameType"/>
      <xsd:element name="RotationOriginGrid" type="gridPosType"/>
      <xsd:element name="RotationAngle" type="deltaAngleType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="scaleDefnType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides the scaling definition for a cogo transformation operation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="ScaleOriginPoint" type="nameType"/>
      <xsd:element name="ScaleOriginGrid" type="gridPosType"/>
      <xsd:element name="ScaleFactor" type="xsd:double"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="translationDefnType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides the translation definition for a cogo transformation operation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="TranslationDefinitionMethod" type="cogoTranslationMethodType"/>
      <xsd:element name="TranslationFromPoint" type="nameType" minOccurs="0"/> <!-- Present if TranslationDefinitionMethod = TwoPoints -->
      <xsd:element name="TranslationToPoint" type="nameType" minOccurs="0"/>   <!-- Present if TranslationDefinitionMethod = TwoPoints -->
      <xsd:element name="TranslationDeltas" type="gridDeltaType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="crossSectionPointType"> 
    <xsd:annotation>
      <xsd:documentation>This is a point in a design cross-section definition - provides the cross-section point code (if any) and 
                         the point's offset and elevation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Code" type="xsd:string"/>
      <xsd:element name="Offset" type="coordType"/>
      <xsd:element name="Elevation" type="coordOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="crossSectionSideSlopeType"> 
    <xsd:annotation>
      <xsd:documentation>This is a side slope point in a design cross-section definition - provides the cross-section point code (if any) 
                         along with the cut and fill grades to be applied and ditch width</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Code" type="xsd:string"/>
      <xsd:element name="CutGrade" type="gradeOrNullType"/>
      <xsd:element name="FillGrade" type="gradeOrNullType"/>
      <xsd:element name="DitchWidth" type="distType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="crossSectionElementsType"> 
    <xsd:annotation>
      <xsd:documentation>This element provides all the points that define one side of a design cross-section report</xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="Point" type="crossSectionPointType" minOccurs="0" maxOccurs="unbounded"/> <!-- Depending on the template definition there may be no cross-section points (unlikely) or multiple points -->
      <xsd:element name="SideSlope" type="crossSectionSideSlopeType" minOccurs="0" maxOccurs="1"/> <!-- Depending on the template definition there may be no side slope definition or a single side slope -->
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="levelLoopStartEndPointType">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>This element provides the details for the starting or ending point of a level loop</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Name" type="nameType"/>
      <xsd:element name="ID" type="idType"/>
      <xsd:element name="Elevation" type="coordType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="levelTestReadingsType">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>This element records the test readings for level test operation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Point1VerticalReading" type="distType"/>
      <xsd:element name="Point2VerticalReading" type="distType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="pointDescType" mixed="true">
    <xsd:annotation>
      <xsd:documentation>Point descriptions are extra optional text fields that (like codes) that can be assigned to points</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="Name" type="nameType" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:simpleType name="scanType">
    <xsd:annotation>
      <xsd:documentation>Valid scan types are ScanFullCircle, ScanRectangle or ScanPolygon</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ScanFullCircle"/>
      <xsd:enumeration value="ScanRectangle"/>
      <xsd:enumeration value="ScanPolygon"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="precisionsSourceType">
    <xsd:annotation>
      <xsd:documentation>Valid instrument precisions sources are UserEntered or FromInstrument</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UserEntered"/>
      <xsd:enumeration value="FromInstrument"/>
    </xsd:restriction>
  </xsd:simpleType>
   
  <xsd:simpleType name="levelLoopMethodType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid level loop methods are BF (backsight/foresight) or BFFB (backsight/foresight/foresight/backsight)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BF"/>
      <xsd:enumeration value="BFFB"/>
    </xsd:restriction>
  </xsd:simpleType>
   
  <xsd:complexType name="associatedFileType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The associatedFileType is used in the LinkedFiles and ActiveMapFiles elements to present the file names of the associated files</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base='xsd:string'>
        <xsd:attribute name="Selectable" type="booleanType" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:simpleType name="stationSetupBacksightObservationUseType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid observation uses are NotUsed, VerticalOnly, HorizontalOnly or HorizontalAndVertical. This indicates 
                         how a backsight observation is used in a Station Setup Plus or Resection setup.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotUsed"/>
      <xsd:enumeration value="VerticalOnly"/>
      <xsd:enumeration value="HorizontalOnly"/>
      <xsd:enumeration value="HorizontalAndVertical"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="areaEntityElementType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid area entity element types are Line and Arc</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Line"/>
      <xsd:enumeration value="Arc"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="gridPosWithNameType">  <!-- New for V5.2 of schema --> 
    <xsd:annotation>
      <xsd:documentation>A grid coordinate position providing North, East and Elevation values for a point along with the point name if applicable</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Name" type="xsd:string"/>  <!-- Allowed to be a null name so don't use nameType -->
      <xsd:element name="North" type="coordOrNullType"/>
      <xsd:element name="East" type="coordOrNullType"/>
      <xsd:element name="Elevation" type="coordOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="areaEntityType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>An areaEntityType element records the details of an individual line or arc entity used in an area computation</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="areaEntityElementType"/>
      <xsd:element name="Name" type="xsd:string"/>  <!-- Don't use nameType - some elements may be constructed as part of an area subdivision and so not have a name -->
      <xsd:element name="Start" type="gridPosWithNameType"/>
      <xsd:element name="End" type="gridPosWithNameType"/>
      <xsd:element name="Center" type="gridPosWithNameType" minOccurs="0"/>  <!-- Only used for arc elements -->
      <xsd:element name="ScribedAngle" type="angleType" minOccurs="0"/>      <!-- Only used for arc elements -->
      <xsd:element name="Direction" type="directionType" minOccurs="0"/>     <!-- Only used for arc elements -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="entityListType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>An entityListType element lists all the entities used in an area computation</xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="Entity" type="areaEntityType" maxOccurs="unbounded"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:simpleType name="transformationMethodType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid 'Grid (local)' transformation methods are Line and Helmert</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Line"/>
      <xsd:enumeration value="Helmert"/>
      <xsd:enumeration value="SevenParameter"/>  <!-- New enumeration for V5.3 of schema -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="transformationVerticalAdjustmentMethodType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid 'Grid (local)' vertical adjustment methods are NoAdjustment, ConstantAdjustment and InclinedPlane</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoAdjustment"/>
      <xsd:enumeration value="ConstantAdjustment"/>
      <xsd:enumeration value="InclinedPlane"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="horizGridPosType">  <!-- New for V5.2 of schema --> 
    <xsd:annotation>
      <xsd:documentation>A horizontal only grid coordinate position providing North and East values</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="North" type="coordOrNullType"/>
      <xsd:element name="East" type="coordOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="scaleApplicationMethodType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid scale application methods are Free, FixedToOne and AlongLocalNorthingAxisOnly</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Free"/>
      <xsd:enumeration value="FixedToOne"/>
      <xsd:enumeration value="AlongLocalNorthingAxisOnly"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="gridPointType">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The gridPointType is used for the Point1 and Point2 elements that hold the position details 
                         for the points used to define the 3D axis in the Measure 3D axes measurement option</xsd:documentation>
    </xsd:annotation> 
    <xsd:all>
      <xsd:element name="Name" type="nameType"/>
      <xsd:element name="Code" type="xsd:string"/>
      <xsd:element name="Grid" type="gridPosType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="triangleSolutionMethodType">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid triangle solution methods are Side-Side-Side, Angle-Side-Angle, Side-Angle-Angle,
                         Side-Angle-Side and Side-Side-Angle</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Side-Side-Side"/>
      <xsd:enumeration value="Angle-Side-Angle"/>
      <xsd:enumeration value="Side-Angle-Angle"/>
      <xsd:enumeration value="Side-Angle-Side"/>
      <xsd:enumeration value="Side-Side-Angle"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:complexType name="calcEnteredDistanceType">  <!-- New for V5.4 of schema -->
    <xsd:simpleContent>
      <xsd:extension base='distType'>
        <xsd:attribute name="Calculated" type="booleanType" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="calcEnteredAngleType">  <!-- New for V5.4 of schema -->
    <xsd:simpleContent>
      <xsd:extension base='angleType'>
        <xsd:attribute name="Calculated" type="booleanType" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:simpleType name="arcSolutionParamOneMethodType">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid arc solution parameter one methods are Radius, 
                         Delta, DegreeArc and DegreeChord</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Radius"/>
      <xsd:enumeration value="Delta"/>
      <xsd:enumeration value="DegreeArc"/>
      <xsd:enumeration value="DegreeChord"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="arcSolutionParamTwoMethodType">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid arc solution parameter two methods are Delta, 
                         Length, Chord, Tangent, External and MidOrdinate</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Delta"/>
      <xsd:enumeration value="Length"/>
      <xsd:enumeration value="Chord"/>
      <xsd:enumeration value="Tangent"/>
      <xsd:enumeration value="External"/>
      <xsd:enumeration value="MidOrdinate"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="arcLayoutMethodType">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid arc layout methods are PCDeflection, 
                         PIDeflection, TangentOffset and ChordOffset</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PCDeflection"/>
      <xsd:enumeration value="PIDeflection"/>
      <xsd:enumeration value="TangentOffset"/>
      <xsd:enumeration value="ChordOffset"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="miningAutostakeMethodType">  <!-- New for V5.5 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid mining auto stake methods are AutostakeCenterline, AutostakeGradeline
                         AutostakeLaserline, AutostakeBlasthole, AutostakePivotpoint, AutostakeProjectline and AutostakeLaserlineFromCenterline</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="AutostakeCenterline"/>
      <xsd:enumeration value="AutostakeGradeline"/>
      <xsd:enumeration value="AutostakeLaserline"/>
      <xsd:enumeration value="AutostakeBlasthole"/>
      <xsd:enumeration value="AutostakePivotpoint"/>
      <xsd:enumeration value="AutostakeProjectline"/>   <!-- New for V5.56 of schema -->
      <xsd:enumeration value="AutostakeLaserlineFromCenterline"/>   <!-- New for V5.58 of schema -->
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="volumeComputationMethodType">  <!-- New for V5.52 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid volume computation methods are AboveAnElevation, VoidVolume
                         SurfaceToElevation, SurfaceToSurface and StockpileDepression</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="AboveAnElevation"/>
      <xsd:enumeration value="VoidVolume"/>
      <xsd:enumeration value="SurfaceToElevation"/>
      <xsd:enumeration value="SurfaceToSurface"/>
      <xsd:enumeration value="StockpileDepression"/>
      <xsd:enumeration value="SurfaceArea"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:complexType name="volumeResultsType">  <!-- New for V5.52 of schema -->
    <xsd:annotation>
      <xsd:documentation>An element containing the computed cut and/or fill volumes and the cut/fill balance</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="CutVolume" type="volumeOrNullType"/>
      <xsd:element name="FillVolume" type="volumeOrNullType"/>
      <xsd:element name="CutFillBalance" type="volumeOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="biVector3DType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Defines a 3D biVector for rotation.
        For biVectors,
        XX represents the quantity of the U2U3 basis area
        YY represents the quantity of the U3U1 basis area
        ZZ represents the quantity of the U1U2 basis area
      </xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="XX" type="doubleOrNullType"/>
      <xsd:element name="YY" type="doubleOrNullType"/>
      <xsd:element name="ZZ" type="doubleOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="deviceAttitudeType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Defines the attitude of a device in terms of pitch, roll and yaw</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Pitch" type="doubleOrNullType"/>
      <xsd:element name="Roll" type="doubleOrNullType"/>
      <xsd:element name="Yaw" type="doubleOrNullType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:group name="deviceAxisBiVectorGroup">
    <xsd:sequence>  <!-- Need to use sequence definition here as the schema validation will not work with choice referencing groups defined with all -->
      <xsd:element name="BiVector" type="biVector3DType" />
      <xsd:element name="StandardDeviation" type="biVector3DType" minOccurs="0"/>  <!-- New for V5.60 of schema -->
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="deviceAttitudeGroup">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Defines the azimuth type and pitch, roll and yaw values</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>  <!-- Need to use sequence definition here as the schema validation will not work with choice referencing groups defined with all -->
      <xsd:element name="Attitude" type="deviceAttitudeType"/>
      <xsd:element name="AttitudeStandardDeviation" type="deviceAttitudeType" minOccurs="0"/>
      <xsd:element name="MagneticDirectionDisturbance" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="MagneticStengthDisturbance" type="xsd:integer" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>

  <xsd:complexType name="deviceAxisOrientationBaseType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>A base definition for the following deviceAxisOrientationType definition</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="AzimuthType" type="azimuthType"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="deviceAxisOrientationType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Provides details of a device axis orientation by means of a choice of biVector or device 
        attitude pitch, roll and yaw values.  The deviceAxisBiVectorGroup choice has an optional 
        StandardDeviation biVector element to allow the recording of the standard deviation of the 
        axis orientation components. The deviceAttitudeGroup choice has an options AttitudeStandardDeviation
        element to allow the recording of the standard deviations of the pitch, roll and yaw values as well as 
        optional Magnetic Direction and Strength Disturbance values.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="deviceAxisOrientationBaseType">
        <xsd:choice minOccurs="1" maxOccurs="1">
          <xsd:group ref="deviceAxisBiVectorGroup"/>
          <xsd:group ref="deviceAttitudeGroup"/>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:simpleType name="qualityWarningType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Valid quality warning values are NotSupported, NotCalibrated, NotChecked, Yes and No
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotSupported"/>
      <xsd:enumeration value="NotCalibrated"/>
      <xsd:enumeration value="NotChecked"/>
      <xsd:enumeration value="Yes"/>
      <xsd:enumeration value="No"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="tiltEventType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Valid tilt events are Calibration and StartSurvey. These indicate the reason
        for the recording of a TiltSensorCalibrationRecord: Calibration = a tilt sensor 
        calibration has been carried out, StartSurvey: a record showing the status of the
        tilt sensor calibration when a survey is started.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Calibration"/>
      <xsd:enumeration value="StartSurvey"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="photoObsType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Defines a photogrammetry observation in terms of the image used and the pixel coordinates from that image.
        These values are converted to a ray start position and ray direction in the world's frame of reference. The intersection
        of two or more such rays is used to compute the point's position.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="ImageID" type="idType"/>
      <!-- Specifies the image used -->
      <xsd:element name="RowPixel" type="xsd:double"/>
      <xsd:element name="ColumnPixel" type="xsd:double"/>
      <xsd:element name="PixelStdDeviation" minOccurs="0" type="xsd:double"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="tiltSensorStatusType">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Valid tilt sensor status types are NotInitialised, Faulty, NotCalibrated, CalibratedOk,
        Cancelled, CalibrationFailed, CalibrationExpired or StateUnrecognized.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotInitialised"/>
      <xsd:enumeration value="Faulty"/>
      <xsd:enumeration value="NotCalibrated"/>
      <xsd:enumeration value="CalibratedOk"/>
      <xsd:enumeration value="Cancelled"/>
      <xsd:enumeration value="CalibrationFailed"/>
      <xsd:enumeration value="CalibrationExpired"/>
      <xsd:enumeration value="StateUnrecognized"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="linkedFilesType">  <!-- New for V5.2 of schema (type definition rearranged for V5.57 of schema) -->
    <xsd:annotation>
      <xsd:documentation>The LinkedFiles element provides a list of the external  
                         files linked with the job at the time of export</xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="File" type="associatedFileType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="linkedFilesRecWithIDType">  <!-- New for V5.57 of schema -->
    <xsd:annotation>
      <xsd:documentation>The type definition for the LinkedFilesRecord elements that 
                         are recorded in the FieldBook section of the JobXML file</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="linkedFilesType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="activeMapFilesType">  <!-- New for V5.2 of schema (type definition rearranged for V5.57 of schema) -->
    <xsd:annotation>
      <xsd:documentation>The ActiveMapFiles element provides a list of the external  
                         files selected for viewing in the map at the time of export</xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="File" type="associatedFileType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="activeMapFilesRecWithIDType">  <!-- New for V5.57 of schema -->
    <xsd:annotation>
      <xsd:documentation>The type definition for the ActiveMapFilesRecord elements that 
                         are recorded in the FieldBook section of the JobXML file</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="activeMapFilesType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:simpleType name="circularObjectMethodType">  <!-- New for V5.58 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Valid circular object method types are BisectTangents and CenterPlusTangent.
        The appropriate method type can be chosen when measuring a circular object.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BisectTangents"/>
      <xsd:enumeration value="CenterPlusTangent"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="photoStationType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Valid station types are RawSensorValues, AzimuthOnly, FullOrientation and StandardResection
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RawSensorValues"/>
      <xsd:enumeration value="AzimuthOnly"/>
      <xsd:enumeration value="FullOrientation"/>
      <xsd:enumeration value="StandardResection"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="symmetrical3DMatrixType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>Defines the diagonal and upper-right corner of a symmetrical matrix. I.e. the lower left corner is symmetrical with the upper right.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="A00" type="xsd:double" />
      <xsd:element name="A01" type="xsd:double" />
      <xsd:element name="A02" type="xsd:double" />
      <xsd:element name="A11" type="xsd:double" />
      <xsd:element name="A12" type="xsd:double" />
      <xsd:element name="A22" type="xsd:double" />
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="full3DMatrixType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>Defines all elements of a 3x3 matrix. I.e. it isn't symmetrical.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="A00" type="xsd:double" />
      <xsd:element name="A01" type="xsd:double" />
      <xsd:element name="A02" type="xsd:double" />
      <xsd:element name="A10" type="xsd:double" />
      <xsd:element name="A11" type="xsd:double" />
      <xsd:element name="A12" type="xsd:double" />
      <xsd:element name="A20" type="xsd:double" />
      <xsd:element name="A21" type="xsd:double" />
      <xsd:element name="A22" type="xsd:double" />
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="lensModelType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>Defines the lens model used by a camera</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Rectilinear" />
      <xsd:enumeration value="FTheta" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="vector3DType">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Defines a 3D vector for translation.
        X represents the quantity of the U1 basis vector
        Y represents the quantity of the U2 basis vector
        Z represents the quantity of the U3 basis vector
      </xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="X" type="xsd:double" />
      <xsd:element name="Y" type="xsd:double" />
      <xsd:element name="Z" type="xsd:double" />
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="instrumentType">  <!-- New for V5.60 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Type of surveying instrument
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Terrestrial"/>
      <xsd:enumeration value="Aerial"/>
      <xsd:enumeration value="V10 Camera"/>
    </xsd:restriction>
  </xsd:simpleType>

    <!-- New for V5.6 of schema -->
  <xsd:complexType name="flightBlockType">
    <xsd:annotation>
      <xsd:documentation>
        A flight block represents one series of images along with their camera positions and orientations taken during a single
        continuous flight covering a single polygonal area. Alone or combined with other flight blocks, it defines an executed
        flight mission.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="FlightPlan" type="flightPlanType"/>
      <xsd:element name="StationList">
        <xsd:complexType>
          <xsd:annotation>
            <xsd:documentation>
              A series of ordered station Ids corresponding to the stations each image was taken from during the flight.
            </xsd:documentation>
          </xsd:annotation>
          <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="StationID" type="idType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="name" type="nameType" use="required" />
    <xsd:attribute name="startTime" type="timeStampType" use="required"/>
    <xsd:attribute name="endTime" type="timeStampType" use="required"/>
  </xsd:complexType>

  <!-- New for V5.6 of schema -->
  <xsd:complexType name="flightPlanType">
    <xsd:annotation>
      <xsd:documentation>
        A flight plan represents a designated single area to be surveyed with a UAV. It is used to plan a flight before
        any data is collected. One single continuous flight may end up covering several flight plans (without having to land).
        It is a series of ordered latitude and longitude pairs defining the closed outline of the area to be flown.
        The last pair connect to the first one.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence minOccurs="3" maxOccurs="unbounded">
      <xsd:element name ="Node">
        <xsd:complexType>
          <xsd:annotation>
            <xsd:documentation>
              Latitude and longitude coordinates
            </xsd:documentation>
          </xsd:annotation>
          <xsd:attribute name="latitude" type="latitudeType" use="required"/>
          <xsd:attribute name="longitude" type="longitudeType" use="required"/>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="height" type="distType" use="optional" />
    <xsd:attribute name="percentForwardOverlap" type="percentType" use="optional" />
    <xsd:attribute name="percentLateralOverlap" type="percentType" use="optional" />
  </xsd:complexType>

  <xsd:simpleType name="magneticCalibrationDeviceType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid magnetic calibration device types are V10ImagingRover or GNSSReceiver</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="V10ImagingRover"/>
      <xsd:enumeration value="GNSSReceiver"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="surveyStyleDeviceType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid survey style device types are None, GNSS, Conventional or Integrated</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="GNSS"/>
      <xsd:enumeration value="Conventional"/>
      <xsd:enumeration value="Integrated"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="roverBaseOptionsSurveyType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid rover and base options survey types are RTK, RTKAndInfill, RTKAndDataLogging,
                         FastStatic, PPKinematic, RTDifferential, RTDifferentialAndInfill, 
                         RTDifferentialAndDataLogging, PPDifferential or NoGPS</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RTK"/>
      <xsd:enumeration value="RTKAndInfill"/>
      <xsd:enumeration value="RTKAndDataLogging"/>
      <xsd:enumeration value="FastStatic"/>
      <xsd:enumeration value="PPKinematic"/>
      <xsd:enumeration value="RTDifferential"/>
      <xsd:enumeration value="RTDifferentialAndInfill"/>
      <xsd:enumeration value="RTDifferentialAndDataLogging"/>
      <xsd:enumeration value="PPDifferential"/>
      <xsd:enumeration value="NoGPS"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="roverBaseOptionsRecordType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid rover and base options survey types are RTCMCode, SBASOnly, OmniSTAROnly,
                         RTCMCtrl, RTCMCtrl23, RTCMCtrl30, CMR2Ctrl, CMR2Plus, CMR2Plus5Hz, CMRX,
                         NetworkADV, NetworkVRS, NetworkCMR, NetworkCMRNet, NetworkDiff, MultiStationCMR,
                         MultiStationRTCM, RTCMNet, RTXviaNTRIP or RTXviaSV</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RTCMCode"/>
      <xsd:enumeration value="SBASOnly"/>
      <xsd:enumeration value="OmniSTAROnly"/>
      <xsd:enumeration value="RTCMCtrl"/>
      <xsd:enumeration value="RTCMCtrl23"/>
      <xsd:enumeration value="RTCMCtrl30"/>
      <xsd:enumeration value="CMR2Ctrl"/>
      <xsd:enumeration value="CMR2Plus"/>
      <xsd:enumeration value="CMR2Plus5Hz"/>
      <xsd:enumeration value="CMRX"/>
      <xsd:enumeration value="NetworkADV"/>
      <xsd:enumeration value="NetworkVRS"/>
      <xsd:enumeration value="NetworkCMR"/>
      <xsd:enumeration value="NetworkCMRNet"/>
      <xsd:enumeration value="NetworkDiff"/>
      <xsd:enumeration value="MultiStationCMR"/>
      <xsd:enumeration value="MultiStationRTCM"/>
      <xsd:enumeration value="RTCMNet"/>
      <xsd:enumeration value="RTXviaNTRIP"/>
      <xsd:enumeration value="RTXviaSV"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="loggingDeviceType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid position storage types are HandHeld or Receiver</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="HandHeld"/>
      <xsd:enumeration value="Receiver"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="positionStoreType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid position storage types are StoreAsPositions or StoreAsVectors</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="StoreAsPositions"/>
      <xsd:enumeration value="StoreAsVectors"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="satelliteDifferentialCorrectionType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid satellite differential correction types are NoCorrection, SBAS or OMNISTAR</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoCorrection"/>
      <xsd:enumeration value="SBAS"/>
      <xsd:enumeration value="OMNISTAR"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutPointLabelsType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid stakeout point labels types are DesignName, AutoPointName, DesignCode, 
                         LastCodeUsed or DesignStationAndOffset</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DesignName"/>
      <xsd:enumeration value="AutoPointName"/>
      <xsd:enumeration value="DesignCode"/>
      <xsd:enumeration value="LastCodeUsed"/>
      <xsd:enumeration value="DesignStationAndOffset"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutOrientToType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid stakeout orient to types are OrientToHeading, OrientToNorth or OrientToReferenceAzimuth</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="OrientToHeading"/>
      <xsd:enumeration value="OrientToNorth"/>
      <xsd:enumeration value="OrientToReferenceAzimuth"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutTotalStationDeltasType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid stakeout total station deltas types are DistanceDeltas, AngleDistanceDeltas, 
                         AnglesSlope, AzimuthDistance, GridDeltas or StationOffset</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DistanceDeltas"/>
      <xsd:enumeration value="AngleDistanceDeltas"/>
      <xsd:enumeration value="AnglesSlope"/>
      <xsd:enumeration value="AzimuthDistance"/>
      <xsd:enumeration value="GridDeltas"/>
      <xsd:enumeration value="StationOffset"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutMapModeType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid stakeout orient to types are MovingMap or MovingCursor</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="MovingMap"/>
      <xsd:enumeration value="MovingCursor"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stakeoutTotalStationViewModeType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid stakeout total station view mode types are InOutAndLeftRightView or DirectionAndDistanceView</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="InOutAndLeftRightView"/>
      <xsd:enumeration value="DirectionAndDistanceView"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="verticalAdjustmentType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid vertical adjustment types are NoAdjustment, ConstantAdjustment or InclinedPlane</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NoAdjustment"/>
      <xsd:enumeration value="ConstantAdjustment"/>
      <xsd:enumeration value="InclinedPlane"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="calibrationObservationType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid calibration observation types are TopoPoint, KinematicPoint or RapidPoint</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TopoPoint"/>
      <xsd:enumeration value="KinematicPoint"/>
      <xsd:enumeration value="RapidPoint"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="coordinateType">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid coordinate types are Null, WGS84, Local, UncalibratedLocal,
                         Grid, LocalTransformedGrid, GridDeltas, ECEF, ECEFGround, ECEFDeltas, 
                         ECEFGroundDeltas, Polar, Ground, Unturned, UnorientedPolar, Oriented,
                         Circle, Corrected, LaserOffset, StationOffset, MTA, MGRS or AsStored</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Null"/>
      <xsd:enumeration value="WGS84"/>                 <!-- WGS-84 (Global) ellipsoid latitude, longitude and height (in terms of RTK reference realisation) -->
      <xsd:enumeration value="Local"/>                 <!-- Local ellipsoid latitude, longitude and height -->
      <xsd:enumeration value="UncalibratedLocal"/>     <!-- Internal use only and never displayed - Local ellipsoid latitude, longitude and height without GNSS calibration applied -->
      <xsd:enumeration value="Grid"/>                  <!-- Display: "Grid" -->
      <xsd:enumeration value="LocalTransformedGrid"/>  <!-- Display: "Grid (local)" - database coordinate with local transformation applied -->
      <xsd:enumeration value="GridDeltas"/>            <!-- Display: "delta Grid" -->
      <xsd:enumeration value="ECEF"/>                  <!-- Earth Centred Earth Fixed (X,Y,Z) coordinate -->
      <xsd:enumeration value="ECEFGround"/>            <!-- Earth Centred Earth Fixed (X,Y,Z) ground coordinate -->
      <xsd:enumeration value="ECEFDeltas"/>            <!-- Deltas (in X,Y,Z) between 2 Earth Centred Earth Fixed coordinates -->
      <xsd:enumeration value="ECEFGroundDeltas"/>      <!-- Deltas (in X,Y,Z) between 2 Earth Centred Earth Fixed ground coordinates -->
      <xsd:enumeration value="Polar"/>                 <!-- Display: "Az HD VD" - has corrections for ppm, collimation, prism constant and curvature and refraction applied -->
      <xsd:enumeration value="Ground"/>                <!-- Ground coordinate position -->
      <xsd:enumeration value="UnorientedPolar"/>       <!-- HA HD VD - has corrections for ppm, collimation, prism constant and curvature and refraction applied -->
      <xsd:enumeration value="Oriented"/>              <!-- Display: "Az VA SD" - has corrections for ppm, collimation, prism constant and curvature and refraction applied -->
      <xsd:enumeration value="RawCircle"/>             <!-- Display: "HA VA SD (raw)" -->
      <xsd:enumeration value="Corrected"/>             <!-- Display: "HA VA SD" - has corrections for ppm, collimation, prism constant and curvature and refraction applied -->
      <xsd:enumeration value="LaserOffset"/>           <!-- Laser offset observation -->
      <xsd:enumeration value="StationOffset"/>         <!-- Display: "Station and offset" - with reference to a linear entity -->
      <xsd:enumeration value="MTA"/>                   <!-- Mean Turned Angle -->
      <xsd:enumeration value="MGRS"/>                  <!-- Map Grid Reference System -->
      <xsd:enumeration value="AsStored"/>              <!-- Display of a point position based on the way the point has been stored -->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="fullSurveyOptionsType">
    <xsd:annotation>
      <xsd:documentation>This element type records the full base or rover GNSS receiver settings</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="SurveyType" type="roverBaseOptionsSurveyType"/>
      <xsd:element name="LoggingDevice" type="loggingDeviceType"/>
      <xsd:element name="TiltEnabled" type="booleanType"/>
      <xsd:element name="RecordType" type="roverBaseOptionsRecordType"/>
      <xsd:element name="PositionStoreType" type="positionStoreType"/>
      <xsd:element name="RovingAutoPrecisions" type="booleanType"/>
      <xsd:element name="RovingHorizontalPrecision" type="xsd:double"/>
      <xsd:element name="RovingVerticalPrecision" type="xsd:double"/>
      <xsd:element name="Satellites">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="SatelliteDifferentialCorrection" type="satelliteDifferentialCorrectionType"/>
            <xsd:element name="ElevationMask" type="xsd:double"/>
            <xsd:element name="PDOPMask" type="xsd:double"/>
            <xsd:element name="L2CEnabled" type="booleanType"/>
            <xsd:element name="GlonassEnabled" type="booleanType"/>
            <xsd:element name="GalileoEnabled" type="booleanType"/>
            <xsd:element name="QZSSEnabled" type="booleanType"/>
            <xsd:element name="BeiDouEnabled" type="booleanType"/>
            <xsd:element name="L5Enabled" type="booleanType"/>
            <xsd:element name="L2eEnabled" type="booleanType"/>
            <xsd:element name="G1PTracked" type="booleanType"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Antenna">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="AntennaNumber" type="xsd:integer"/>
            <xsd:element name="AntennaMethod" type="xsd:integer"/>
            <xsd:element name="MeasuredHeight" type="distOrNullType"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="quickMarkAndContinuousSettingsType">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The quickMarkAndSettingsType definition provides the settings details for
                         rapid points and GNSS continuous points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Measurement">
        <xsd:complexType> 
          <xsd:all>
            <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
            <xsd:element name="VerticalPrecision" type="distOrNullType"/>
            <xsd:element name="AutoPrecisions" type="booleanType"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Tilt">
        <xsd:complexType> 
          <xsd:all>
            <xsd:element name="TiltWarnings" type="booleanType"/>
            <xsd:element name="TiltLimit" type="distOrNullType"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="instrumentRecordType">
    <xsd:annotation>
      <xsd:documentation>The Instrument element provides details on the instrument being used for the following observations</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="Type" type="xsd:string">  <!-- Changed in V5 of schema to be a string rather than the previous instrumentType enumeration -->
        <xsd:annotation>
          <xsd:documentation>A text field providing the instrument type</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RefractiveIndex" type="doubleOrNullType">
        <xsd:annotation>
          <xsd:documentation>The refractive index constant specific to this instrument</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CarrierWavelength" type="doubleOrNullType">
        <xsd:annotation>
          <xsd:documentation>The carrier wavelength constant (not the actual carrier wavelength) specific to 
                             this instrument (for use in the correction formula with pressure values in millibars)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EDMMeasurementMode" type="measurementModeType"/>
      <xsd:element name="SetHorizontalCircleMode" type="setHorizCircleModeType"/>
      <xsd:element name="Servo" type="servoType"/>
      <xsd:element name="AnglesOnlyRate" type="doubleOrNullType"/>
      <xsd:element name="TurnToZero" type="booleanType"/>
      <xsd:element name="AverageMeasurements" type="xsd:integer"/>
      <xsd:element name="HorizontalAnglePrecision" type="angleOrNullType"/>
      <xsd:element name="VerticalAnglePrecision" type="angleOrNullType"/>
      <xsd:element name="EDMPrecision" type="distOrNullType"/>
      <xsd:element name="EDMppm" type="integerOrNullType"/>
      <xsd:element name="InstrumentPrecisionsSource" type="precisionsSourceType"/>  <!-- A new record for the Survey Controller v11.40 - indicates whether the instrument precisions have been manually entered or have come from the instrument -->
      <xsd:element name="CenteringError" type="distType"/>
      <xsd:element name="BacksightCenteringError" type="distType"/> <!-- New for V5.57 of schema - holds the estimated centering error for backsight points -->
      <!-- The following elements are only present when Type = Trimble3600GDM | Trimble5600 | TrimbleSSeries -->
      <xsd:element name="Model" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A text field providing the instrument model name - only present for Trimble instruments</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Serial" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A text field providing the instrument serial number - only present for Trimble instruments</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FirmwareVersion" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A text field providing the instrument firmware version - only present for Trimble instruments</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="UserDefinedName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A text field providing any user defined name assigned to the - only present for Trimble instruments</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="InstrumentAppliedHorizontalCollimation" type="deltaAngleOrNullType" minOccurs="0"/>
      <xsd:element name="InstrumentAppliedVerticalCollimation" type="deltaAngleOrNullType" minOccurs="0"/>
      <xsd:element name="InstrumentAppliedAutolockHorizontalCollimation" type="deltaAngleOrNullType" minOccurs="0"/>
      <xsd:element name="InstrumentAppliedAutolockVerticalCollimation" type="deltaAngleOrNullType" minOccurs="0"/>
      <xsd:element name="TrunionAxisTiltCorrection" type="deltaAngleOrNullType" minOccurs="0"/>
      <xsd:element name="InstrumentAppliedEDMConstant" type="doubleOrNullType" minOccurs="0"/>
      <xsd:element name="AtmosphericCorrectionCoefficient" type="doubleOrNullType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The AtmosphericCorrectionCoefficient value is a correction coefficient specific to the Trimble VX/S Series instruments 
                             (it effectively combines the RefractiveIndex and CarrierWavelength values)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LevelAdditiveConstant" type="doubleOrNullType" minOccurs="0"/>  <!-- New for V5.2 schema - allows recording of level instrument additive constant -->
      <xsd:element ref="Notes" minOccurs="0"/>    <!-- Present when note added to this item in 'Review current job'  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="instrumentRecWithIDType"> 
    <xsd:annotation>
      <xsd:documentation>An instrument record that can be used at the top level in the FieldBook element.
                         The instrumentRecordType is also used in the SurveyStyleRecord that can be obtained
                         when a Plug In Survey Core application requests a JobXML output of a specific record.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="instrumentRecordType">
        <xsd:attribute name="ID" type="idType" use="required"/>
        <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="calibrationImageDetailsType">  <!-- New for V5.62 of schema -->
    <xsd:annotation>
      <xsd:documentation>A record that provides the camera record ID, file name and dimensions for an image file 
                         in an ImagePairRecord recorded as part of a camera calibration check on a V10 Imaging Rover</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="CameraRecord2ID" type="idType"/>
      <xsd:element name="FileName" type="xsd:string"/>
      <xsd:element name="Width" type="xsd:integer"/>
      <xsd:element name="Height" type="xsd:integer"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="planeResidualType">  <!-- New for V5.62 of schema -->
    <xsd:annotation>
      <xsd:documentation>An record that provides the point name and delta to the defined plane for
                         a point used in the definition of a plane.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="PointName" type="nameType"/>
      <xsd:element name="Delta" type="distType"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="tectonicPlateNameType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid tectonic plate name types are Unknown, Aegean Sea, Altiplano, Amurian, Anatolia, Antarctica,
                         Arabia, Australia, Balmoral Reef, Banda Sea, Birds Head, Burma, Capricorn, Caribbean, Caroline,
                         Cocos, Conway Reef, Easter, Eurasia, Futuna, Galapagos, India, Juan de Fuca, Juan Fernandez, Kermadec,
                         Lwandle, Macquarie, Manus, Maoke, Mariana, Molucca Sea, Nazca, New Hebrides, Niuafoou, North America,
                         North Andes, North Bismarck, Nubia, Okhotsk, Okinawa, Pacific, Panama, Philippine Sea, Rivera,
                         Sandwich, Scotia, Shetland, Solomon Sea, Somalia, South America, South Bismarck, Sunda, Sur, Timor,
                         Tonga, Woodlark or Yangtze</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Unknown"/>
      <xsd:enumeration value="Aegean Sea"/>
      <xsd:enumeration value="Altiplano"/>
      <xsd:enumeration value="Amurian"/>
      <xsd:enumeration value="Anatolia"/>
      <xsd:enumeration value="Antarctica"/>
      <xsd:enumeration value="Arabia"/>
      <xsd:enumeration value="Australia"/>
      <xsd:enumeration value="Balmoral Reef"/>
      <xsd:enumeration value="Banda Sea"/>
      <xsd:enumeration value="Birds Head"/>
      <xsd:enumeration value="Burma"/>
      <xsd:enumeration value="Capricorn"/>
      <xsd:enumeration value="Caribbean"/>
      <xsd:enumeration value="Caroline"/>
      <xsd:enumeration value="Cocos"/>
      <xsd:enumeration value="Conway Reef"/>
      <xsd:enumeration value="Easter"/>
      <xsd:enumeration value="Eurasia"/>
      <xsd:enumeration value="Futuna"/>
      <xsd:enumeration value="Galapagos"/>
      <xsd:enumeration value="India"/>
      <xsd:enumeration value="Juan de Fuca"/>
      <xsd:enumeration value="Juan Fernandez"/>
      <xsd:enumeration value="Kermadec"/>
      <xsd:enumeration value="Lwandle"/>
      <xsd:enumeration value="Macquarie"/>
      <xsd:enumeration value="Manus"/>
      <xsd:enumeration value="Maoke"/>
      <xsd:enumeration value="Mariana"/>
      <xsd:enumeration value="Molucca Sea"/>
      <xsd:enumeration value="Nazca"/>
      <xsd:enumeration value="New Hebrides"/>
      <xsd:enumeration value="Niuafoou"/>
      <xsd:enumeration value="North America"/>
      <xsd:enumeration value="North Andes"/>
      <xsd:enumeration value="North Bismarck"/>
      <xsd:enumeration value="Nubia"/>
      <xsd:enumeration value="Okhotsk"/>
      <xsd:enumeration value="Okinawa"/>
      <xsd:enumeration value="Pacific"/>
      <xsd:enumeration value="Panama"/>
      <xsd:enumeration value="Philippine Sea"/>
      <xsd:enumeration value="Rivera"/>
      <xsd:enumeration value="Sandwich"/>
      <xsd:enumeration value="Scotia"/>
      <xsd:enumeration value="Shetland"/>
      <xsd:enumeration value="Solomon Sea"/>
      <xsd:enumeration value="Somalia"/>
      <xsd:enumeration value="South America"/>
      <xsd:enumeration value="South Bismarck"/>
