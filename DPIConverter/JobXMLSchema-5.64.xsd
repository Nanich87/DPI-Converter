        <xsd:element ref="LineRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ArcRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NoteRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CalibrationPointRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="GPSEquipmentRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AntennaRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="RoverSurveyOptionsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="BaseSurveyOptionsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SurveyEventRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContinuousParametersRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="InitialisationRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ReferenceRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PostProcessRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="RoundsMeanTurnedAngleResiduals" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TraverseDefinitionRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TraverseAdjustmentRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TraverseClosureRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AreaRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ComputeAreaRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.2 of schema -->
        <xsd:element ref="SubdivideAreaRecord" minOccurs="0" maxOccurs="unbounded"/>  <!-- New for V5.2 of schema -->
        <xsd:element ref="StartRoundsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="StartRoundRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="EndRoundsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="EndRoundRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ComputeAzimuthRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ComputeAngleRecord" minOccurs="0" maxOccurs="unbounded"/>  <!-- New for v5.0 schema -->
        <xsd:element ref="ComputeDistanceRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="InverseRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CogoTransformationRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="LevelLoopOpenRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="LevelLoopCloseRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="LevelTestResultsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="TimeZoneRecord" type="timeZoneRecWithIDType" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ScanningRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ImageRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MediaFileRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CameraRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TunnelSettingsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TunnelCrossSectionRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TunnelSetoutPointsRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NTRIPDataSourceRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ImportDataBeginRecord" minOccurs="0" maxOccurs="unbounded"/>  <!-- New for V5.2 of schema -->
        <xsd:element ref="ImportDataEndRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.2 of schema -->
        <xsd:element ref="LocalTransformationRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.2 of schema -->
        <xsd:element ref="TriangleSolutionRecord" minOccurs="0" maxOccurs="unbounded"/>       <!-- New for V5.4 of schema -->
        <xsd:element ref="ArcSolutionRecord" minOccurs="0" maxOccurs="unbounded"/>            <!-- New for V5.4 of schema -->
        <xsd:element ref="EchoSounderConfigurationRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.51 of schema -->
        <xsd:element ref="VolumeRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.52 of schema -->
        <xsd:element ref="SBASOffsetRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.55 of schema -->
        <xsd:element ref="TiltSensorCalibrationRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.56 of schema -->
        <xsd:element name="LinkedFilesRecord" type="linkedFilesRecWithIDType" minOccurs="0" maxOccurs="unbounded"/>       <!-- New for V5.57 of schema -->
        <xsd:element name="ActiveMapFilesRecord" type="activeMapFilesRecWithIDType" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.57 of schema -->
        <xsd:element ref="CustomTypeRecord" minOccurs="0" maxOccurs="unbounded"/>   <!-- New for V5.57 of schema -->
        <xsd:element ref="CustomXmlRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.57 of schema -->
        <xsd:element ref="CustomEntityRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.57 of schema -->
        <xsd:element ref="ReducedLevelDataRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.57 of schema -->
        <xsd:element ref="PhotoInstrumentRecord" minOccurs="0" maxOccurs="unbounded"/> <!-- New for V5.6 of schema -->
        <xsd:element ref="PhotoStationRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.6 of schema -->
        <xsd:element ref="CameraDesignRecord" minOccurs="0" maxOccurs="unbounded"/>    <!-- New for V5.6 of schema -->
        <xsd:element ref="CameraRecord2" minOccurs="0" maxOccurs="unbounded"/>         <!-- New for V5.6 of schema -->
        <xsd:element ref="FlightMissionRecord" minOccurs="0" maxOccurs="unbounded"/>   <!-- New for V5.6 of schema -->
        <xsd:element ref="SurveyStyleRecord" minOccurs="0" maxOccurs="unbounded"/>     <!-- New for V5.61 of schema -->
        <xsd:element ref="MagneticSensorCalibrationRecord" minOccurs="0" maxOccurs="unbounded"/>     <!-- New for V5.61 of schema -->
        <xsd:element ref="CalibrationCheckRecord" minOccurs="0" maxOccurs="unbounded"/>              <!-- New for V5.62 of schema -->
        <xsd:element ref="ImagePairRecord" minOccurs="0" maxOccurs="unbounded"/>                     <!-- New for V5.62 of schema -->
        <xsd:element ref="PlaneDefinitionRecord" minOccurs="0" maxOccurs="unbounded"/>               <!-- New for V5.62 of schema -->
        <xsd:element ref="RTXRTKOffsetRecord" minOccurs="0" maxOccurs="unbounded"/>                  <!-- New for V5.64 of schema -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>


  <!-- ******************************************************************** -->
  <!-- *****************  Reductions section declarations  **************** -->
  <!-- ******************************************************************** -->

  <xsd:element name="Point">
    <xsd:annotation>
      <xsd:documentation>The Point element contains the best position (based on Trimble Access's search rules), from a PointRecord 
                         in the FieldBook node, Trimble Access has or can compute for each point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ID" type="idOrNullType"/>                                 <!-- References the ID of the PointRecord in the FieldBook node that has provided these coordinates - allowed to be null for point upload purposes -->
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="Description1" type="pointDescType" minOccurs="0"/>        <!-- Present if the option for extra point descriptions has been switched on -->
        <xsd:element name="Description2" type="pointDescType" minOccurs="0"/>        <!-- Present if the option for extra point descriptions has been switched on -->
        <xsd:element name="SurveyMethod" type="surveyMethodType" minOccurs="0"/>     <!-- Optional SurveyMethod available to pass quality details for point upload purposes -->
        <xsd:element name="Classification" type="classificationType" minOccurs="0"/> <!-- Optional Classification available to pass quality details for point upload purposes -->
        <xsd:element ref="Features" minOccurs="0"/>                                  <!-- This is a copy of the Features node, if present, in the best PointRecord used as the source for this point -->
        <xsd:element name="Grid" type="gridPosType" minOccurs="0"/>                  <!-- Present if grid coordinates are available, or can be computed for the point -->
        <xsd:element ref="WGS84" minOccurs="0"/>                                     <!-- Present if the point is a GPS measured or entered point -->
        <xsd:element ref="Local" minOccurs="0"/>                                     <!-- Present if there were only local ellipsoid coordinates available for the point - typically it would need to be a point with keyed in local coordinates -->
        <xsd:element ref="CustomXmlSubRecord" minOccurs="0"/>                        <!-- New for V5.57 of schema --> <!-- Present when a SCAPI application has added custom data to a PointRecord -->
        <xsd:element ref="Layer" minOccurs="0"/>                                     <!-- New for V5.64 of schema - used by Survey Pro system -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Reductions">
    <xsd:annotation>
      <xsd:documentation>The Reductions element contains a set of Point elements that provide the best position (based 
                         on Trimble Access's search rules) for each surveyed point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="Point" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="error" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>



  <!-- ******************************************************************** -->
  <!-- ****************  Environment section declarations  **************** -->
  <!-- ******************************************************************** -->

  <xsd:element name="Location">  <!-- New definition for the Survey Controller v11.30 system -->
    <xsd:annotation>
      <xsd:documentation>The Location element provides the location the computed Combined Scale Factor is based on</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Latitude" type="latitudeOrNullType"/>
        <xsd:element name="Longitude" type="longitudeOrNullType"/>
        <xsd:element name="Height" type="coordOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CombinedScaleFactor">  <!-- New definition for the Survey Controller v11.30 system -->
    <xsd:annotation>
      <xsd:documentation>The CombinedScaleFactor element provides the Combined Scale Factor for the job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="Location"/>
        <xsd:element name="Scale" type="doubleOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CalibrationPointPair">  <!-- New definition for the Survey Controller v12.20 system -->
    <xsd:annotation>
      <xsd:documentation>The CalibrationPointPair element provides the names for a point pair used in a calibration. The point
                         pair consists of a GPS and a grid point. The Dimension element indicates how the point pair should
                         be used in the calibration computation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="GridPointName" type="nameType"/>
        <xsd:element name="WGS84PointName" type="nameType"/>
        <xsd:element name="Dimension" type="calibDimensionType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CalibrationPointPairs">  <!-- New definition for the Survey Controller v12.20 system -->
    <xsd:annotation>
      <xsd:documentation>The CalibrationPointPairs element provides a collection of calibration point pairs that define
                         the points used to compute the GPS calibration adjustments. When creating a JobXML file for export
                         to a field device, these pairs allow the calibration routine in the field to be primed with the 
                         existing calibration points.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="CalibrationPointPair" minOccurs="1" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TectonicPlate">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Records the tectonic plate selected for the job.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="tectonicPlateNameType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RTXRTKOffset">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Records the RTX to RTK offset defined in the job.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TranslationX" type="distType"/>  <!-- Cartesian X axis translation applied to RTXECEF X coordinate to transform to RTK reference frame -->
        <xsd:element name="TranslationY" type="distType"/>  <!-- Cartesian Y axis translation applied to RTXECEF Y coordinate to transform to RTK reference frame -->
        <xsd:element name="TranslationZ" type="distType"/>  <!-- Cartesian Z axis translation applied to RTXECEF Z coordinate to transform to RTK reference frame -->
        <xsd:element name="SigmaX" type="distType"/>        <!-- Estimated error in X axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="SigmaY" type="distType"/>        <!-- Estimated error in Y axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="SigmaZ" type="distType"/>        <!-- Estimated error in Z axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="ITRFEpoch" type="xsd:string"/>   <!-- ITRF2008 epoch the offset was computed for, and to be applied to -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="CoordinateSystem">
    <xsd:annotation>
      <xsd:documentation>The CoordinateSystem element provides the complete coordinate definition current at the time
                         the job was exported to the JobXML file. It also provides a means of defining the coordinate
                         system in a JobXML file being exported to a field device.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SystemName" type="xsd:string" minOccurs="0"/> <!-- This element doesn't need to be included if there is no name - typically it is included with an empty value. -->
        <xsd:element name="ZoneName" type="xsd:string" minOccurs="0"/>   <!-- This element doesn't need to be included if there is no name - typically it is included with an empty value. -->
        <xsd:element name="DatumName" type="xsd:string" minOccurs="0"/>  <!-- This element doesn't need to be included if there is no name - typically it is included with an empty value. -->
        <xsd:element name="Ellipsoid" type="ellipsoidRecType"/>
        <xsd:element ref="TectonicPlate" minOccurs="0"/>                 <!-- New for V5.64 of schema -->
        <xsd:element ref="RTXRTKOffset" minOccurs="0"/>                  <!-- New for V5.64 of schema -->
        <xsd:element ref="Projection"/>
        <xsd:element ref="LocalSite"/>
        <xsd:element name="Datum" type="datumRecType"/>
        <xsd:element name="HorizontalAdjustment" type="horizAdjRecType"/>
        <xsd:element name="VerticalAdjustment" type="vertAdjRecType"/>
        <xsd:element ref="CombinedScaleFactor"/>                   <!-- New definition for the Survey Controller v11.30 system -->
        <xsd:element ref="CalibrationPointPairs" minOccurs="0"/>   <!-- New definition for the Survey Controller v12.20 system -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="JobSettings">
    <xsd:annotation>
      <xsd:documentation>The JobSettings element provides the status of the Neighbourhood adjustment (a type of adjustment 
                         used in Germany) at the time of export to the JobXML file</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="NeighbourhoodAdjustment"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="JobLineFiles">  <!-- New for V5.55 of schema -->
    <xsd:annotation>
      <xsd:documentation>The JobLineFiles element provides a list of the alignment and polyline files created
                         (as RoadXML format files) by the SurveyPro system based on the line data in a job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="File" type="associatedFileType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AssociatedFiles">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The AssociatedFiles element provides a list of the external  
                         files associated with the job at the time of export</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LinkedFiles" type="linkedFilesType"/>       <!-- Type definition rearranged for V5.57 of schema -->
        <xsd:element name="ActiveMapFiles" type="activeMapFilesType"/> <!-- Type definition rearranged for V5.57 of schema -->
        <xsd:element ref="JobLineFiles" minOccurs="0"/>  <!-- New for V5.55 of schema -->
        <xsd:element name="FeatureCodeLibrary" type="xsd:string" minOccurs="0"/> <!-- New for V5.57 of schema -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="InformationGroup">  <!-- New for V5.3 of schema -->
    <xsd:annotation>
      <xsd:documentation>An InformationGroup provides a grouping for Information elements that hold job specific information.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="Information" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>The Information elements record individual details for the InformationGroup</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="Name"/>
              <xsd:element name="Value"/>
              <xsd:element name="Type" type="attributeType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
      <xsd:attribute name="name"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="JobInformation">  <!-- New for V5.3 of schema -->
    <xsd:annotation>
      <xsd:documentation>The JobInformation element provides an optional location where sets of  
                         InformationGroup elements holding details relevant to the job can be placed.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="InformationGroup" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Environment">
    <xsd:annotation>
      <xsd:documentation>The Environment element contains all the configuration settings that were current at the time
                         the data was exported to the JobXML file. It provides a single location in which latest job
                         settings can be obtained.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all maxOccurs="1" minOccurs="1">
        <xsd:element name="DisplaySettings" type="unitsRecType"/>
        <xsd:element ref="CoordinateSystem"/>
        <xsd:element ref="JobSettings"/>
        <xsd:element ref="AssociatedFiles" minOccurs="0"/>  <!-- New for V5.2 of schema -->
        <xsd:element ref="JobInformation" minOccurs="0"/>   <!-- New for V5.3 of schema -->
        <xsd:element name="TimeZone" type="timeZoneRecType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>



  <!-- ******************************************************************** -->
  <!-- **********************  Main JOBFile Element  ********************** -->
  <!-- ******************************************************************** -->

  <xsd:element name="JOBFile">
    <xsd:annotation>
      <xsd:documentation>The JOBFile element is the 'root' element used to hold all the JobXML file data</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="FieldBook" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="Reductions" minOccurs="1" maxOccurs="1"/>
        <xsd:element ref="Environment" minOccurs="1" maxOccurs="1"/>
      </xsd:all>
      <xsd:attribute name="jobName" type="nameType" use="required"/>
      <xsd:attribute name="version" type="xsd:double" use="required"/>
      <xsd:attribute name="product" type="xsd:string" use="required"/>
      <xsd:attribute name="productVersion" type="xsd:string" use="required"/>
      <xsd:attribute name="productDBVersion" type="xsd:string"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

</xsd:schema>
