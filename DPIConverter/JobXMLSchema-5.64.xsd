  <xsd:simpleType name="checkResultType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid check result types are NotPerformed, InProgress, CompleteOk or CompleteFail</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NotPerformed"/>
      <xsd:enumeration value="InProgress"/>
      <xsd:enumeration value="CompleteOk"/>
      <xsd:enumeration value="CompleteFail"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="RTXRTKoffsetSourceType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid offset source types are ComputedAtRover, UploadedToRover, ComputedAtBase, 
                         UploadedToBase or ComputedInController</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ComputedAtRover"/>
      <xsd:enumeration value="UploadedToRover"/>
      <xsd:enumeration value="ComputedAtBase"/>
      <xsd:enumeration value="UploadedToBase"/>
      <xsd:enumeration value="ComputedInController"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="quickReleaseAdaptorType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid types of Quick Release adaptor are None, OnePiece, TwoPiece or Custom</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="OnePiece"/>
      <xsd:enumeration value="TwoPiece"/>
      <xsd:enumeration value="Custom"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:complexType name="measurementStackItemType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Horizontal and Vertical elements in a MeasurementStack</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="VerticalOffset" type="distType"  minOccurs="0"/>
      <xsd:element name="HorizontalOffset" type="distType"  minOccurs="0"/>  <!-- Only present on the V10Extension element  -->
      <xsd:element name="Type" type="quickReleaseAdaptorType"  minOccurs="0"/>  <!-- Only present on the Adaptor element  -->
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="imageType">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid types of image are Normal, HDRCombined, HDR_Component_Under, HDR HDR_Component_Over or HDR_Component_Under.
                         Either a non-HDR (High Dynamic Range) image, a single, processed, HDR image or one of three component HDR images with varying exposures.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Normal"/>
      <xsd:enumeration value="HDRCombined"/>
      <xsd:enumeration value="HDR_Component_Under"/>
      <xsd:enumeration value="HDR_Component_Normal"/>
      <xsd:enumeration value="HDR_Component_Over"/>
    </xsd:restriction>
  </xsd:simpleType>

<xsd:simpleType name="antennaMeasurementKey">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>Valid types of key are Method0, Method1, Method2, Method3, Method4, Method5, Method6, Method7, Method8, Method9, BottomOfV10, LeverOfV10Extension, APC or Unknown.
                         If present, clarifies the AntennaMethod number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Method0"/>
      <xsd:enumeration value="Method1"/>
      <xsd:enumeration value="Method2"/>
      <xsd:enumeration value="Method3"/>
      <xsd:enumeration value="Method4"/>
      <xsd:enumeration value="Method5"/>
      <xsd:enumeration value="Method6"/>
      <xsd:enumeration value="Method7"/>
      <xsd:enumeration value="Method8"/>
      <xsd:enumeration value="Method9"/>
      <xsd:enumeration value="BottomOfV10"/>
      <xsd:enumeration value="LeverOfV10Extension"/>
      <xsd:enumeration value="APC"/>
      <xsd:enumeration value="Unknown"/>
    </xsd:restriction>
  </xsd:simpleType>











  <!-- ******************************************************************** -->
  <!-- *****************  FieldBook section declarations  ***************** -->
  <!-- ******************************************************************** -->

  <xsd:element name="Notes">
    <xsd:annotation>
      <xsd:documentation>The Notes element contains one or more user entered, or automatically generated, pieces of text</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="1" maxOccurs="unbounded">
        <xsd:element name="Note" type="xsd:string" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="NoteRecord"> 
    <xsd:annotation>
      <xsd:documentation>The NoteRecord element contains user entered, or automatically generated, text</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="Notes" minOccurs="0"/>   
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>    
  </xsd:element>

  <xsd:element name="FeatureCodingRecord">
    <xsd:annotation>
      <xsd:documentation>The FeatureCodingRecord element provides details on the feature coding library used in the job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LibraryName" type="xsd:string"/>
        <xsd:element name="SourceFilename" type="xsd:string"/>
        <xsd:element name="AttributeCheckSum" type="xsd:integer"/>
        <xsd:element name="AttributesStored" type="booleanType"/>
        <xsd:element ref="Notes" minOccurs="0"/>  <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="NeighbourhoodAdjustment">
    <xsd:annotation>
      <xsd:documentation>The NeighbourhoodAdjustment element in a CorrectionsRecord indicates whether Neighbourhood 
                         adjustment (a type of adjustment used in Germany) has been applied to the computed grid 
                         coordinates, and provides the weight exponent value</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Applied" type="booleanType"/>
        <xsd:element name="WeightExponent" type="xsd:double"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelInstrument">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The optional LevelInstrument element in a CorrectionsRecord indicates whether refraction and
                         earth curvature corrections have already been applied to the level observation values in the file</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="RefractionCorrectionApplied" type="booleanType"/>
        <xsd:element name="RefractionCoefficient" type="doubleOrNullType"/>
        <xsd:element name="EarthCurvatureCorrectionApplied" type="booleanType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CorrectionsRecord">
    <xsd:annotation>
      <xsd:documentation>The CorrectionsRecord element shows the current configuration used in the job. The SouthAzimuth setting is true if zero 
                         degrees is grid south. The SouthAzimuth and GridOrientation settings at export time are also available in the Environment section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SouthAzimuth" type="booleanType"/>
        <xsd:element name="GridOrientation" type="gridOrientType"/>
        <xsd:element name="MagneticDeclination" type="deltaAngleType"/>
        <xsd:element name="Distances" type="distanceDisplayType"/>
        <xsd:element name="ApplySeaLevelCorrection" type="booleanType">  <!-- New for V5.2 of schema -->
          <xsd:annotation>
            <xsd:documentation>When the ApplySeaLevelCorrection element is set to true this indicates that sea level corrections should
                               be applied in the computation of coordinates. When set to true the coordinates in the ComputedGrid and 
                               Reductions elements have had sea level corrections applied during their computation.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="NeighbourhoodAdjustment"/>
        <xsd:element ref="LevelInstrument" minOccurs="0"/>  <!-- An optional element added for V5.2 of schema -->
        <xsd:element ref="Notes" minOccurs="0"/>  <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CoordinateSystemRecord">
    <xsd:annotation>
      <xsd:documentation>The CoordinateSystemRecord element provides the names of the coordinate system, 
                         zone and datum for coordinate systems selected from the coordinate system library</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SystemName" type="xsd:string"/>
        <xsd:element name="ZoneName" type="xsd:string"/>
        <xsd:element name="DatumName" type="xsd:string"/>
        <xsd:element ref="Notes" minOccurs="0"/>  <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EllipsoidRecord" type="ellipsoidRecWithIDType"/>

  <xsd:element name="ScapiCustomProjectionData">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>The ScapiCustomProjectionData element provides a means for a Plug-In application to
                         record the parameters relating to a custom projection type.  The appropriate 
                         parameters must be recorded in XML records that are read by the Plug-In application</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="PluginSpecificData">
          <xsd:complexType> 
            <xsd:sequence>
              <xsd:any namespace='##any' minOccurs='0' maxOccurs='unbounded' processContents='skip'/>
            </xsd:sequence>
            <xsd:anyAttribute namespace="##any" processContents='skip'/>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RequiredSupportFiles" type="linkedFilesType" minOccurs="0"/>
      </xsd:all>
      <xsd:attribute name="ProjectionTypeName" type="xsd:string"/>
      <xsd:attribute name="PluginOwnerName" type="xsd:string"/>
      <xsd:attribute name="PluginOwnerGuid" type="xsd:string"/>
      <xsd:attribute name="PluginOwnerVersion" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Projection">
    <xsd:annotation>
      <xsd:documentation>The Projection element provides the type and parameters for the projection currently being used</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Type" type="projectionType"/>
        <xsd:element name="Scale" type="doubleOrNullType" minOccurs="0"/>                <!-- Present if Type = ScaleOnly | Plane | Mercator | TransverseMercator | UniversalTransverseMercator | Lambert1Parallel | ObliqueMercatorAngle | ObliqueConformalCylindrical | ObliqueStereographic | CassiniSoldner | StereographicDouble | RSO -->
        <xsd:element name="ShiftGridFileName" type="xsd:string" minOccurs="0"/>          <!-- If present indicates that a shift grid of corrections at a projection level is being applied on top of the projection definition - the Netherlands RD and RD 2004 and the Belgium Lambert 72 (2005) library definitions use shift grids. Note: the projection shift grid file name is written without the .sgf file extension -->
        <xsd:element name="CentralLatitude" type="latitudeOrNullType" minOccurs="0"/>    <!-- Present if Type = Plane | Mercator | TransverseMercator | UniversalTransverseMercator | Lambert1Parallel | Lambert2Parallel | ObliqueMercatorAngle | ObliqueConformalCylindrical | ObliqueStereographic | AlbersEqualAreaConic | CassiniSoldner | StereographicDouble | RSO -->
        <xsd:element name="CentralLongitude" type="longitudeOrNullType" minOccurs="0"/>  <!-- Present if Type = Plane | Mercator | TransverseMercator | UniversalTransverseMercator | Lambert1Parallel | Lambert2Parallel | ObliqueMercatorAngle | ObliqueConformalCylindrical | ObliqueStereographic | AlbersEqualAreaConic | CassiniSoldner | StereographicDouble | RSO -->
        <xsd:element name="OriginHeight" type="coordType" minOccurs="0"/>                <!-- Present if Type = Plane -->
        <xsd:element name="FalseNorthing" type="coordType" minOccurs="0"/>               <!-- Present if Type = Plane | Mercator | TransverseMercator | UniversalTransverseMercator | Lambert1Parallel | Lambert2Parallel | ObliqueMercatorAngle | ObliqueConformalCylindrical | ObliqueStereographic | AlbersEqualAreaConic | CassiniSoldner | StereographicDouble | RSO -->
        <xsd:element name="FalseEasting" type="coordType" minOccurs="0"/>                <!-- Present if Type = Plane | Mercator | TransverseMercator | UniversalTransverseMercator | Lambert1Parallel | Lambert2Parallel | ObliqueMercatorAngle | ObliqueConformalCylindrical | ObliqueStereographic | AlbersEqualAreaConic | CassiniSoldner | StereographicDouble | RSO -->
        <xsd:element name="OriginElevation" type="coordType" minOccurs="0"/>             <!-- Present if Type = Plane -->
        <xsd:element name="Rotation" type="deltaAngleType" minOccurs="0"/>               <!-- Present if Type = Plane -->
        <xsd:element name="Parallel1" type="xsd:double" minOccurs="0"/>                  <!-- Present if Type = Lambert2Parallel | AlbersEqualAreaConic -->
        <xsd:element name="Parallel2" type="xsd:double" minOccurs="0"/>                  <!-- Present if Type = Lambert2Parallel | AlbersEqualAreaConic -->
        <xsd:element name="Azimuth" type="deltaAngleType" minOccurs="0"/>                <!-- Present if Type = ObliqueMercatorAngle | RSO -->
        <xsd:element name="AzimuthAt" type="azimuthOriginAtType" minOccurs="0"/>         <!-- Present if Type = ObliqueMercatorAngle | RSO -->
        <xsd:element name="OriginAt" type="azimuthOriginAtType" minOccurs="0"/>          <!-- Present if Type = ObliqueMercatorAngle | RSO -->
        <xsd:element name="Rectify" type="booleanType" minOccurs="0"/>                   <!-- Present if Type = ObliqueMercatorAngle | RSO -->
        <xsd:element name="FerroConstant" type="xsd:double" minOccurs="0"/>              <!-- Present if Type = Krovak -->
        <xsd:element name="NorthGridFile" type="xsd:string" minOccurs="0"/>              <!-- Present if Type = UnitedKingdomNationalGrid. Note: the UKNG north grid file name is written without the .pgf file extension -->
        <xsd:element name="EastGridFile" type="xsd:string" minOccurs="0"/>               <!-- Present if Type = UnitedKingdomNationalGrid. Note: the UKNG east grid file name is written without the .pgf file extension -->
        <xsd:element name="Area" type="denmarkAreaType" minOccurs="0"/>                  <!-- Present if Type = Denmark -->
        <xsd:element name="ProjectionGridFile" type="xsd:string" minOccurs="0"/>         <!-- Present if Type = ProjectionGrid. Note: the projection grid file name is written without the .pjg file extension -->
        <xsd:element name="SouthAzimuth" type="booleanType" minOccurs="0"/>              <!-- Always present in the JOBFile/Environment/CoordinateSystem/Projection element but not present in the JOBFile/FieldBook/ProjectionRecord element -->
        <xsd:element name="GridOrientation" type="gridOrientType" minOccurs="0"/>        <!-- Always present in the JOBFile/Environment/CoordinateSystem/Projection element but not present in the JOBFile/FieldBook/ProjectionRecord element -->
        <xsd:element name="ApplySeaLevelCorrection" type="booleanType" minOccurs="0"/>   <!-- New for V5.2 of schema --> <!-- Always present in the JOBFile/Environment/CoordinateSystem/Projection element but not present in the JOBFile/FieldBook/ProjectionRecord element -->
        <xsd:element name="SnakeGridParameterFile" type="xsd:string" minOccurs="0"/>     <!-- New for V5.55 of schema --> <!-- Present if Type = SnakeGrid. Note: the SnakeGrid parameter file name is written without the .dat file extension -->
        <xsd:element ref="ScapiCustomProjectionData" minOccurs="0"/>                     <!-- New for V5.61 of schema --> <!-- Present if Type = SCAPICustom. -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LocalSite">
    <xsd:annotation>
      <xsd:documentation>The LocalSite element indicates if the projection is set up as a ground coordinate system</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Type" type="localSiteType"/>
        <xsd:element name="ProjectLocationLatitude" type="latitudeOrNullType"/>
        <xsd:element name="ProjectLocationLongitude" type="longitudeOrNullType"/>
        <xsd:element name="ProjectLocationHeight" type="coordOrNullType"/>
        <xsd:element name="GroundScaleFactor" type="doubleOrNullType" minOccurs="0"/>            <!-- Present if Type = KeyedInGroundScaleFactor | CalculatedGroundScaleFactor -->
        <xsd:element name="NorthingOffset" type="coordOrNullType" minOccurs="0"/>                <!-- Present if Type = KeyedInGroundScaleFactor | CalculatedGroundScaleFactor -->
        <xsd:element name="EastingOffset" type="coordOrNullType" minOccurs="0"/>                 <!-- Present if Type = KeyedInGroundScaleFactor | CalculatedGroundScaleFactor -->
        <xsd:element name="ProjectionOriginNorthingDelta" type="coordOrNullType" minOccurs="0"/> <!-- Present if Type = KeyedInGroundScaleFactor | CalculatedGroundScaleFactor -->
        <xsd:element name="ProjectionOriginEastingDelta" type="coordOrNullType" minOccurs="0"/>  <!-- Present if Type = KeyedInGroundScaleFactor | CalculatedGroundScaleFactor -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ProjectionRecord">
    <xsd:annotation>
      <xsd:documentation>The ProjectionRecord element combines the Projection and LocalSite elements</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="Projection"/>
        <xsd:element ref="LocalSite"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DatumRecord" type="datumRecWithIDType"/>

  <xsd:element name="HorizontalAdjustmentRecord" type="horizAdjRecWithIDType"/>

  <xsd:element name="VerticalAdjustmentRecord" type="vertAdjRecWithIDType"/>

  <xsd:element name="JobPropertiesRecord">
    <xsd:annotation>
      <xsd:documentation>The JobPropertiesRecord element records any details relating to the job that the user has entered</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Reference" type="xsd:string"/>
        <xsd:element name="Description" type="xsd:string"/>
        <xsd:element name="Operator" type="xsd:string"/>
        <xsd:element name="JobNote" type="xsd:string"/>
        <xsd:element name="TectonicPlate" type="tectonicPlateNameType"/> <!-- New for v5.64 schema -->  
        <xsd:element ref="Notes" minOccurs="0"/>    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AtmosphereRecord">
    <xsd:annotation>
      <xsd:documentation>The AtmosphereRecord provides the current atmosphere details - pressure values are always in hPa (millibars), 
                         temperature values are always in degrees Celsius</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Pressure" type="doubleOrNullType">
          <xsd:annotation>
            <xsd:documentation>The pressure value, entered or read from the instrument, used in the computation of the PPM value</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="Temperature" type="doubleOrNullType">
          <xsd:annotation>
            <xsd:documentation>The entered Temperature value used in the computation of the PPM value</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PPM" type="doubleOrNullType">
          <xsd:annotation>
            <xsd:documentation>The computed or entered PPM value to be used for the atmospheric correction of EDM distances</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ApplyEarthCurvatureCorrection" type="booleanType"> <!-- New for v5.2 schema -->
          <xsd:annotation>
            <xsd:documentation>The ApplyEarthCurvatureCorrection setting indicates whether earth curvature correction needs to be 
                               applied to the VerticalCircle values in any circleType elements during office processing. If set to 
                               'true' then the correction needs to be applied during office processing. When 'true' the ComputedGrid 
                               and Reductions Point elements in the file have had this correction applied during computation.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ApplyRefractionCorrection" type="booleanType"> <!-- New for v5.2 schema -->
          <xsd:annotation>
            <xsd:documentation>The ApplyRefractionCorrection setting indicates whether refraction correction needs to be 
                               applied to the VerticalCircle values in any circleType elements during office processing. If set to 
                               'true' then the correction needs to be applied during office processing. When 'true' the ComputedGrid 
                               and Reductions Point elements in the file have had this correction applied during computation.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="RefractionCoefficient" type="refractionCoefficientType"> <!-- New for v5.2 schema -->
          <xsd:annotation>
            <xsd:documentation>The RefractionCoefficient element provides the refraction coefficient to be used when computing the
                               refraction correction (only carried out if the ApplyRefractionCorrection element is set to 'true')</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PressureInputMethod" type ="pressureDetailsFromType"/>  <!-- Added for v11.20 -->
        <xsd:element name="ApplyPPMToRawDistances" type="booleanType" minOccurs="0"> <!-- New for v5.0 schema -->
          <xsd:annotation>
            <xsd:documentation>The ApplyPPMToRawDistances setting indicates whether atmospheric corrections need to be applied to the 
                               EDMDistance values in any circleType elements during office processing. If set to 'true' then the
                               corrections need to be applied. If set to 'false' they have already been applied to the distances
                               and no atmospheric corrections should be applied during office processing.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StationRecord">
    <xsd:annotation>
      <xsd:documentation>The StationRecord element provides details of the station setup. The InstrumentID provides a link to the 
                         appropriate InstrumentRecord and the AtmosphereID provides a link to the appropriate AtmosphereRecord</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="StationName" type="nameType"/>
        <xsd:element name="TheodoliteHeight" type="distOrNullType"/>
        <xsd:element name="RawTheodoliteHeight" type="rawInstTargetHeightType"/>
        <xsd:element name="InstrumentID" type="idType"/>
        <xsd:element name="AtmosphereID" type="idType"/>
        <xsd:element name="NumberOfBacksightMeasurements" type="xsd:integer"/>
        <xsd:element name="NumberOfBacksightPoints" type="xsd:integer"/>
        <xsd:element name="ScaleFactor" type="xsd:double"/>
        <xsd:element name="ScaleType" type="scaleType"/>
        <xsd:element name="ScaleFactorStandardError" type="doubleOrNullType"/>
        <xsd:element name="StationType" type="stationType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AngleResidual">
    <xsd:annotation>
      <xsd:documentation>The AngleResidual element is used to provide angular and distance residuals as part of a ResidualsRecord</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="HorizontalCircle" type="deltaAngleOrNullType"/>
        <xsd:element name="VerticalCircle" type="deltaAngleOrNullType"/>
        <xsd:element name="SlopeDistance" type="distOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ResidualsRecord">
    <xsd:annotation>
      <xsd:documentation>The ResidualsRecord element holds the residuals details reported in a StationResiduals element</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ObservationID" type="idType"/>
        <xsd:element name="PointName" type="nameType"/>
        <xsd:element name="ObservationUsedInSolutionFor" type="stationSetupBacksightObservationUseType"/>  <!-- New for V5.2 of schema -->
        <xsd:element name="GridResidual" type="gridDeltaType"/>
        <xsd:element ref="AngleResidual"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StationResiduals">
    <xsd:annotation>
      <xsd:documentation>The StationResiduals element provides the residuals details for observations to backsight points 
                         observed as part of a Resection station setup or a Station Setup Plus</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ResidualsRecord" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="BackBearingRecord">
    <xsd:annotation>
      <xsd:documentation>The BackBearingRecord element provides the backsight orientation details that will apply to the following observations</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="StationRecordID" type="idType"/>  <!-- New for the v4.0 schema definition (Survey Controller v11.30) -->
        <xsd:element name="Station" type="nameType"/>
        <xsd:element name="BackSight" type="nameType"/>
        <xsd:element name="Face1HorizontalCircle" type="angleOrNullType"/>
        <xsd:element name="Face2HorizontalCircle" type="angleOrNullType"/>
        <xsd:element name="OrientationCorrection" type="deltaAngleOrNullType"/>
        <xsd:element name="OrientationCorrectionStandardError" type="doubleOrNullType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  
  <xsd:element name="MeasurementStack">  <!-- New for V5.64 of schema -->
    <xsd:annotation>
      <xsd:documentation>The measurement stack lists the horizontal and vertical elements which when added together define the offsets from the measured-to point to either the
      APC or Prism centre.  The stack is only present in an equipment or target record if a V10 measurement method is selected.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Antenna" type="measurementStackItemType" minOccurs="0"/>  <!-- APC to bottom of antenna mount -->
        <xsd:element name="Prism" type="measurementStackItemType" minOccurs="0"/>  <!-- Prism centre to bottom of prism mount -->
        <xsd:element name="Adaptor" type="measurementStackItemType" minOccurs="0"/>  <!-- Height and type of adaptor -->
        <xsd:element name="V10AirGap" type="measurementStackItemType" minOccurs="0"/>  <!-- Gap in Quick Release mount hardware -->
        <xsd:element name="V10CameraHead" type="measurementStackItemType" minOccurs="0"/>  <!-- Bottom of V10 to Quick Release mount -->
        <xsd:element name="V10PowerMount" type="measurementStackItemType" minOccurs="0"/>  <!-- Height of V10 power mount -->
        <xsd:element name="V10Extension" type="measurementStackItemType" minOccurs="0"/> <!-- Height and horizontal offset of V10 prism base with lever -->
      </xsd:all>
    </xsd:complexType>
</xsd:element>

  <xsd:element name="TargetRecord">
    <xsd:annotation>
      <xsd:documentation>The TargetRecord element provides all the details relating to the target being used. A TargetRecord element
                         will be present in the FieldBook section of the JobXML file whenever any target details change. If editing of
                         target details has been carried out after measurements have been made, this may result in a duplicate TargetRecord
                         being inserted. This will be placed at the appropriate location in the FieldBook section to ensure that the TargetRecord 
                         preceding any observation holds the correct details for that observation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="PrismType" type="prismType"/> <!-- This PrismType element is new from Survey Controller v11.30 on and is a required element -->
        <xsd:element name="PrismConstant" type="distType"/>
        <xsd:element name="PrismOffset" type="distType" minOccurs="0"/> <!-- The PrismOffset is not output by the Survey Controller from v11.30 on so it has been changed to be an optional element -->
        <xsd:element name="TargetHeight" type="distOrNullType"/>
        <xsd:element name="RawTargetHeight" type="rawInstTargetHeightType"/>
        <xsd:element name="RemoteTargetID" type="remoteTargetIDType" minOccurs="0"/>   <!-- New for V5.2 of schema -->
        <xsd:element ref="MeasurementStack" minOccurs="0"/>   <!-- New for V5.64 of schema. Present when a V10 MeasurementMethod is selected -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="WGS84">
    <xsd:annotation>
      <xsd:documentation>The WGS84 element provides a position as a latitude, longitude and height with respect to the job's realisation of the WGS-84 ellipsoid.
                         Point elements in the Reductions element that have WGS84 elements will have been measured using GPS equipment.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Latitude" type="latitudeType"/>
        <xsd:element name="Longitude" type="longitudeType"/>
        <xsd:element name="Height" type="coordType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Local">
    <xsd:annotation>
      <xsd:documentation>The Local element provides a position as a latitude, longitude and height with respect to the 
                         specified local ellipsoid. Points with Local positions will typically have been keyed in or uploaded.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all maxOccurs="1" minOccurs="1">
        <xsd:element name="Latitude" type="latitudeType"/>
        <xsd:element name="Longitude" type="longitudeType"/>
        <xsd:element name="Height" type="coordType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ECEF">
    <xsd:annotation>
      <xsd:documentation>The ECEF element provides a position as Cartesian X, Y and Z coordinates with respect to the job's
                   	    "WGS-84" reference frame. Points with ECEF positions will typically have been measured using a network RTK solution.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="X" type="distType"/>
        <xsd:element name="Y" type="distType"/>
        <xsd:element name="Z" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RTXECEF">
    <xsd:annotation>
      <xsd:documentation>The RTXECEF element provides a position as Cartesian X, Y and Z coordinates with respect to the
                         ITRF 2008 (Epoch 2005.0) reference frame. Points with RTXECEF positions will have been measured using an RTX solution.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="X" type="distType"/>
        <xsd:element name="Y" type="distType"/>
        <xsd:element name="Z" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ECEFDeltas">
    <xsd:annotation>
      <xsd:documentation>The ECEFDeltas element holds Cartesian delta (vector) values from the current GPS  
                         base station. Points with ECEFDeltas will typically have been measured using an RTK solution.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DeltaX" type="distType"/>
        <xsd:element name="DeltaY" type="distType"/>
        <xsd:element name="DeltaZ" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StationAndOffset">
    <xsd:annotation>
      <xsd:documentation>The StationAndOffset element holds positions that have been keyed in as station and offset 
                         values with respect to a Line, Arc or Road entity</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="EntityType" type="entityType"/>
        <xsd:element name="EntityName" type="nameType"/>
        <xsd:element name="Station" type="coordType"/>
        <xsd:element ref="EquatedStation" minOccurs="0"/>     <!-- New for Version 12.20 Survey Controller - only present if equated stations are defined for the road -->
        <xsd:element name="Offset" type="coordType"/>
        <xsd:element name="VerticalDistance" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Precision">
    <xsd:annotation>
      <xsd:documentation>The Precision element holds horizontal and vertical precisions for RTK or VRS fixes</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Horizontal" type="distType"/>  <!-- Estimated at the 1-sigma (68%) level -->
        <xsd:element name="Vertical" type="distType"/>    <!-- Estimated at the 1-sigma (68%) level -->
        <xsd:element name="RawHorizontal" type="distType" minOccurs="0"/>  <!-- New for V5.64 of schema - raw horizontal precision for RTX points not combined with offset vector horizontal precision -->
        <xsd:element name="RawVertical" type="distType" minOccurs="0"/>    <!-- New for V5.64 of schema - raw vertical precision for RTX points not combined with offset vector vertical precision -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Ground">
    <xsd:annotation>
      <xsd:documentation>The Ground element holds the details for a ground vector defining the current position</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Azimuth" type="angleType"/>
        <xsd:element name="Zenith" type="angleType"/>
        <xsd:element name="SlopeDistance" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LaserOffset">
    <xsd:annotation>
      <xsd:documentation>The LaserOffset element holds the details for measured laser offset defining the current position</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Azimuth" type="angleType"/>
        <xsd:element name="VerticalAngle" type="deltaAngleType"/>
        <xsd:element name="SlopeDistance" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="MTA">
    <xsd:annotation>
      <xsd:documentation>The MTA element holds the details for a Mean Turned Angle. This is the computed average observation to a 
                         point created when multiple observations have been made to the same point during the same station setup.
                         The computed standard errors and the number of observations contributing to the result are provided.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="HorizontalCircle" type="angleType"/>
        <xsd:element name="VerticalCircle" type="angleOrNullType"/> <!-- May not have been a vertical angle recorded -->
        <xsd:element name="EDMDistance" type="distOrNullType"/>     <!-- May not have been a distance measured -->
        <xsd:element name="HorizontalCircleStandardError" type="doubleOrNullType"/>
        <xsd:element name="VerticalCircleStandardError" type="doubleOrNullType"/>
        <xsd:element name="EDMDistanceStandardError" type="doubleOrNullType"/>
        <xsd:element name="NumberOfHorizontalAngles" type="xsd:integer"/>
        <xsd:element name="NumberOfVerticalAngles" type="xsd:integer"/>
        <xsd:element name="NumberOfDistances" type="xsd:integer"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelObservations">  <!-- New in Version 5.57 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The LevelObservations element contains one or more LevelData elements</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="1" maxOccurs="unbounded">
        <xsd:element ref="LevelData" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelData">  <!-- New in Version 4.0 of schema and updated in V5.57 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The LevelData element contains the raw level measurements for a level point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LoopID" type="idType" minOccurs="0"/>                     <!-- Changed to be optional element in V5.57 of schema - plan to deprecate in future -->
        <xsd:element name="TargetID" type="idType" minOccurs="0"/>                   <!-- New in Version 5.57 of schema -->
        <xsd:element name="LevelMethod" type="levelMethodType"/>                     <!-- New in Version 5.57 of schema -->
        <xsd:element name="UpperWire" type="distType" minOccurs="0"/>                <!-- Only used when LevelMethod = ThreeWireLeveling -->
        <xsd:element name="CentreWire" type="distType" minOccurs="0"/>               <!-- Only used when LevelMethod = ThreeWireLeveling | LevelMethod = SingleWireLeveling -->
        <xsd:element name="LowerWire" type="distType" minOccurs="0"/>                <!-- Only used when LevelMethod = ThreeWireLeveling -->
        <xsd:element name="StadiaConstant" type="xsd:double" minOccurs="0"/>         <!-- Only used when LevelMethod = ThreeWireLeveling -->
        <xsd:element name="VerticalReading" type="distType" minOccurs="0"/>          <!-- Only used when LevelMethod = ElectronicLevel -->
        <xsd:element name="HorizontalDistance" type="distType" minOccurs="0"/>       <!-- Only used when LevelMethod = ElectronicLevel -->
        <xsd:element name="Face1VerticalCircle" type="angleType" minOccurs="0"/>     <!-- Only used when LevelMethod = TrigLeveling -->
        <xsd:element name="Face1EDMDistance" type="angleType" minOccurs="0"/>        <!-- Only used when LevelMethod = TrigLeveling -->
        <xsd:element name="Face2VerticalCircle" type="angleType" minOccurs="0"/>     <!-- Only used when LevelMethod = TrigLeveling -->
        <xsd:element name="Face2EDMDistance" type="angleType" minOccurs="0"/>        <!-- Only used when LevelMethod = TrigLeveling -->
        <xsd:element name="NumberOfMeasurements" type="xsd:integer" minOccurs="0"/>  <!-- New for V5.2 schema - provides number of individual level measurements made -->
        <xsd:element name="StandardDeviation" type="distType" minOccurs="0"/>        <!-- New for V5.2 schema - provides the computed standard deviation for multiple measurements -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ReducedLevelData">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>                     <!-- Now redundant as replaced by ReducedLevelDataRecord - plan to deprecate in future -->
      <xsd:documentation>The ReducedLevelData element contains the reduced level measurements for a level point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LoopID" type="idType"/>
        <xsd:element name="PointType" type="levelPointType"/>                       <!-- Indicates the type of point observed (Control, TurningPoint or SideShot) -->
        <xsd:element name="VerticalReading" type="distType"/>                       <!-- The averaged vertical reading from the preceding observed LevelData records -->
        <xsd:element name="BackSightElevation" type="distType" minOccurs="0"/>      <!-- Only used when Classification = BackSight -->
        <xsd:element name="InstrumentAxisElevation" type="distType" minOccurs="0"/> <!-- Only used when Classification = BackSight -->
        <xsd:element name="ForeSightElevation" type="distType" minOccurs="0"/>      <!-- Only used when Classification = ForeSight | Classification = SideShot -->
        <xsd:element name="HorizontalDistance" type="distOrNullType"/>              <!-- The averaged horizontal distance from the preceding observed LevelData records (will be null when SurveyMethod = SingleWireLeveling) -->
        <xsd:element name="NumberOfMeasurements" type="xsd:integer" minOccurs="0"/> <!-- New for V5.2 schema - provides number of individual level measurements made -->
        <xsd:element name="StandardDeviation" type="distType" minOccurs="0"/>       <!-- New for V5.2 schema - provides the computed standard deviation for multiple measurements -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ReducedLevelDataRecord">  <!-- New in Version 5.57 of schema - recorded in FieldBook element holding the level observations -->
    <xsd:annotation>
      <xsd:documentation>The ReducedLevelDataRecord element contains the reduced level measurements for a level point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LoopID" type="idType"/>
        <xsd:element name="PointObserved" type="nameType"/>                         <!-- The name of the point that the observations were to -->
        <xsd:element name="SightType" type="sightType"/>                            <!-- New for V5.57 schema - indicates backsight or foresight -->
        <xsd:element name="LevelMethod" type="levelMethodType" minOccurs="0"/>      <!-- New for V5.57 schema - indicates the leveling method (SingleWireLeveling, ThreeWireLeveling, ElectronicLevel or TrigLeveling) -->
        <xsd:element name="PointType" type="levelPointType"/>                       <!-- Indicates the type of point observed (Control, TurningPoint or SideShot) -->
        <xsd:element name="VerticalReading" type="distType"/>                       <!-- The averaged vertical reading from the preceding observed LevelData records -->
        <xsd:element name="BackSightElevation" type="distType" minOccurs="0"/>      <!-- Only used when SightType = BackSight -->
        <xsd:element name="InstrumentAxisElevation" type="distType" minOccurs="0"/> <!-- Instrument elevation -->
        <xsd:element name="ForeSightElevation" type="distType" minOccurs="0"/>      <!-- Only used when SightType = ForeSight | PointType = SideShot -->
        <xsd:element ref="LevelObservations" minOccurs="0"/>                        <!-- Collection of the LevelData used for this record -->
        <xsd:element name="HorizontalDistance" type="distOrNullType"/>              <!-- The averaged horizontal distance from the preceding observed LevelData records (will be null when SurveyMethod = SingleWireLeveling) -->
        <xsd:element name="NumberOfMeasurements" type="xsd:integer" minOccurs="0"/> <!-- New for V5.2 schema - provides number of individual level measurements made -->
        <xsd:element name="StandardDeviation" type="distType" minOccurs="0"/>       <!-- New for V5.2 schema - provides the computed standard deviation for multiple measurements -->
        <xsd:element ref="Notes" minOccurs="0"/>                                    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ReducedLevelObsReferences"><!-- New in Version 5.57 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The ReducedLevelObsReferences element contains the ID references for the 
                         backsight and foresight reduced level observations</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="ReducedLevelBacksightId" type="idType"/>                 <!-- The ReducedLevelDataRecord that holds the backsight observations -->
        <xsd:element name="ReducedLevelForesightId" type="idType"/>                 <!-- The ReducedLevelDataRecord that holds the foresight observations -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Warnings">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Provides a set of elements indicating the status of specific warnings displayed 
        during the point measurement and indicating whether the warning states were still 
        present when the point was stored.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ExcessVibrationWarning" type="qualityWarningType" minOccurs="0"/>
        <xsd:element name="ExcessVibrationAtStorage" type="qualityWarningType" minOccurs="0"/>
        <xsd:element name="ExcessTiltWarning" type="qualityWarningType"/>
        <xsd:element name="ExcessTiltAtStorage" type="qualityWarningType"/>
        <xsd:element name="ExcessMovementWarning" type="qualityWarningType"/>
        <xsd:element name="ExcessMovementAtStorage" type="qualityWarningType"/>
        <xsd:element name="PoorPrecisionsWarning" type="qualityWarningType"/>
        <xsd:element name="PoorPrecisionsAtStorage" type="qualityWarningType"/>
        <xsd:element name="MagneticDisturbanceWarning" type="qualityWarningType"/>      <!-- New for V5.61 of schema -->
        <xsd:element name="MagneticDisturbanceAtStorage" type="qualityWarningType"/>    <!-- New for V5.61 of schema -->
        <xsd:element name="BadEnvironmentWarning" type="qualityWarningType"/>           <!-- New for V5.64 of schema -->
        <xsd:element name="BadEnvironmentAtStorage" type="qualityWarningType"/>         <!-- New for V5.64 of schema -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
    
  <xsd:element name="QualityControl1">
    <xsd:annotation>
      <xsd:documentation>The QualityControl1 element contains a set of GPS quality control values relating to the observation.
                         Trimble Access can include just QualityControl1, QualityControl1 plus QualityControl2 or
                         QualityControl1 plus QualityControl3 records with a point.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="MinimumNumberOfSatellites" type="xsd:integer"/>  <!-- New for V5.58 of schema - records the minimum number of satelites observed during the occupation -->
        <xsd:element name="NumberOfSatellites" type="xsd:integer"/>
        <xsd:element name="RelativeDOPs" type="booleanType"/>
        <xsd:element name="PDOP" type="doubleOrNullType"/>  <!-- Changed type definition for V5.64 of schema -->
        <xsd:element name="GDOP" type="doubleOrNullType"/>  <!-- New for V5.56 of schema -->
        <xsd:element name="HDOP" type="doubleOrNullType"/>  <!-- Changed type definition for V5.64 of schema -->
        <xsd:element name="VDOP" type="doubleOrNullType"/>  <!-- Changed type definition for V5.64 of schema -->
        <xsd:element name="PDOP_AtStore" type="xsd:double"/>       <!-- New for V5.58 of schema - records the PDOP value as it was at the time of storage -->
        <xsd:element name="GDOP_AtStore" type="doubleOrNullType"/> <!-- New for V5.58 of schema - records the GDOP value as it was at the time of storage -->
        <xsd:element name="HDOP_AtStore" type="xsd:double"/>       <!-- New for V5.58 of schema - records the HDOP value as it was at the time of storage -->
        <xsd:element name="VDOP_AtStore" type="xsd:double"/>       <!-- New for V5.58 of schema - records the VDOP value as it was at the time of storage -->
        <xsd:element name="RMS" type="doubleOrNullType" minOccurs="0"/>  <!-- Recorded in millicycles (to convert to metres divide by 1000 to get cycles and multiply by 0.19 - the L1 wavelength in metres). Switched to an optional element in V5.56 schema. -->
        <xsd:element name="NumberOfPositionsUsed" type="xsd:integer"/>
        <xsd:element name="HorizontalStandardDeviation" type="doubleOrNullType"/>
        <xsd:element name="VerticalStandardDeviation" type="doubleOrNullType"/>
        <xsd:element name="StartTime" type="GPSTimeType"/>
        <xsd:element name="EndTime" type="GPSTimeType"/>
        <xsd:element name="MonitorStatus" type="monitorStatusType"/>
        <xsd:element name="RTCMAge" type="xsd:double"/> <!-- New for V5.55 schema - records the RTCM message age in seconds -->
        <xsd:element ref="Warnings" minOccurs="0"/> <!-- New for V5.56 schema - records whether or not specific warnings have been issued and accepted during the point measurement -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="QualityControl2">
    <xsd:annotation>
      <xsd:documentation>The QualityControl2 element contains further GPS quality control values relating to the observation.
                         Trimble Access can include just QualityControl1, QualityControl1 plus QualityControl2 or
                         QualityControl1 plus QualityControl3 records with a point.
                         All values are estimated at the 1-sigma (68%) level.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="NumberOfSatellites" type="xsd:integer"/>
        <xsd:element name="ErrorScale" type="xsd:double"/>
        <xsd:element name="VCVxx" type="xsd:double"/>
        <xsd:element name="VCVxy" type="xsd:double"/>
        <xsd:element name="VCVxz" type="xsd:double"/>
        <xsd:element name="VCVyy" type="xsd:double"/>
        <xsd:element name="VCVyz" type="xsd:double"/>
        <xsd:element name="VCVzz" type="xsd:double"/>
        <xsd:element name="UnitVariance" type="xsd:double"/>    <!-- New for V5.2 of schema -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="QualityControl3">
    <xsd:annotation>
      <xsd:documentation>The QualityControl3 element contains yet more GPS quality control values relating to the observation.
                         Trimble Access can include just QualityControl1, QualityControl1 plus QualityControl2 or
                         QualityControl1 plus QualityControl3 records with a point.
                         All values are estimated at the 1-sigma (68%) level.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SigmaNorth" type="xsd:double"/>
        <xsd:element name="SigmaEast" type="xsd:double"/>
        <xsd:element name="SigmaElevation" type="xsd:double"/>
        <xsd:element name="CovarianceNorthEast" type="xsd:double"/>
        <xsd:element name="SemiMajorAxis" type="distType"/>
        <xsd:element name="SemiMinorAxis" type="distType"/>
        <xsd:element name="Orientation" type="deltaAngleType"/>
        <xsd:element name="UnitVariance" type="xsd:double"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ResectionStandardErrors">
    <xsd:annotation>
      <xsd:documentation>The ResectionStandardErrors element provides the computed standard errors for a resected position</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="NorthStandardError" type="doubleOrNullType"/>
        <xsd:element name="EastStandardError" type="doubleOrNullType"/>
        <xsd:element name="ElevationStandardError" type="doubleOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CircularObject">
    <xsd:annotation>
      <xsd:documentation>The CircularObject element provides the raw observation values and computed radius for the circular object observation method</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Method" type="circularObjectMethodType"/>  <!-- New for V5.58 of schema -->
        <xsd:element name="FirstRawObservation" type="circleType"/>
        <xsd:element name="SecondRawObservation" type="circleType"/>
        <xsd:element name="ThirdRawObservation" type="circleType" minOccurs="0"/>  <!-- New for V5.58 of schema -->
        <xsd:element name="Radius" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RemoteObject">
    <xsd:annotation>
      <xsd:documentation>The RemoteObject element provides the raw observation values and computed values for the remote object observation method</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="RawObservation" type="circleType"/>
        <xsd:element name="ObjectHeight" type="distOrNullType"/>
        <xsd:element name="ObjectWidth" type="distOrNullType"/>
        <xsd:element name="BasePointName" type="nameType"/>
        <xsd:element name="BasePointID" type="idType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EnteredData">
    <xsd:annotation>
      <xsd:documentation>The EnteredData element records the values entered for a Cogo computation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Method" type="pointCreationType"/>
        <xsd:element name="PointOne" type="nameType"/>         <!-- If the Method is ProjectPointToLine | ProjectPointToArc this element contains the name of the point projected onto the line or arc -->
        <xsd:element name="PointTwo" type="xsd:string"/>       <!-- May not be used in computation so can be null - use xsd:string (If the Method is ProjectPointToLine | ProjectPointToArc this element contains the name of the start point of the line or arc) -->
        <xsd:element name="PointThree" type="xsd:string"/>     <!-- May not be used in computation so can be null - use xsd:string (If the Method is ProjectPointToLine and a line between 2 points was defined this element contains the name of the end point of the line) -->
        <xsd:element name="PointFour" type="xsd:string"/>      <!-- May not be used in computation so can be null - use xsd:string -->
        <xsd:element name="AzimuthOne" type="angleOrNullType"/>
        <xsd:element name="DistanceOne" type="distOrNullType"/>
        <xsd:element name="AzimuthTwo" type="angleOrNullType"/>
        <xsd:element name="DistanceTwo" type="distOrNullType"/>
        <xsd:element name="DistanceThree" type="distOrNullType" minOccurs="0"/> <!-- New in Version 5.0 of schema - for Trimble Survey Manager - FromABaselineWithErrorDistribution - inital point abscissa --> 
        <xsd:element name="DistanceFour" type="distOrNullType" minOccurs="0"/>  <!-- New in Version 5.0 of schema - for Trimble Survey Manager - FromABaselineWithErrorDistribution - inital point ordinate --> 
        <xsd:element name="DistanceFive" type="distOrNullType" minOccurs="0"/>  <!-- New in Version 5.0 of schema - for Trimble Survey Manager - FromABaselineWithErrorDistribution - endpoint abscissa --> 
        <xsd:element name="DistanceSix" type="distOrNullType" minOccurs="0"/>   <!-- New in Version 5.0 of schema - for Trimble Survey Manager - FromABaselineWithErrorDistribution - endpoint ordinate --> 
        <xsd:element name="TurnedAngle" type="deltaAngleOrNullType"/>
        <xsd:element name="VerticalOffset" type="distOrNullType"/>
        <xsd:element name="AzimuthType" type="azimuthType"/>
        <xsd:element name="DistanceType" type="distanceType"/>
        <xsd:element name="DirectionAlongLine" type="directionAlongLineType"/>
        <xsd:element name="ReferenceLinearEntity" type="xsd:string" minOccurs="0"/>       <!-- New for V5.4 of schema (this element contains the name of the line or arc the point was projected onto) -->
        <xsd:element name="DistanceAlong" type="distOrNullType" minOccurs="0"/>           <!-- New for V5.4 of schema (this element contains the distance from the start point of the line or arc (PointTwo) to the projected position) -->
        <xsd:element name="ProjectedDistance" type="distOrNullType" minOccurs="0"/>       <!-- New for V5.4 of schema (this element contains the projection distance from the selected projection point (PointOne) to the line or arc) -->
        <xsd:element name="VerticalPlaneIntersectionStationID" type="idOrNullType"/>      <!-- New for V5.2 of schema -->
        <xsd:element name="VerticalPlaneIntersectionBackBearingID" type="idOrNullType"/>  <!-- New for V5.2 of schema -->
        <xsd:element name="VerticalPlaneIntersectionRay" type="circleType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DualPrismOffset">
    <xsd:annotation>
      <xsd:documentation>The DualPrismOffset element records the raw observation details and parameters for a dual prism measurement method observation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="PrismAObservation" type="circleType"/>
        <xsd:element name="PrismBObservation" type="circleType"/>
        <xsd:element name="PrismConstantA" type="distType"/>
        <xsd:element name="PrismConstantB" type="distType"/>
        <xsd:element name="DistanceAB" type="distOrNullType"/>
        <xsd:element name="DistanceBC" type="distType"/>
        <xsd:element name="ToleranceAB" type="distType"/>  <!-- New for V5.2 of schema -->
        <xsd:element name="CalculatedDistanceAB" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AngleOffset">
    <xsd:annotation>
      <xsd:documentation>The AngleOffset element records the raw observation values for an angle offset type measurement method</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TargetID" type="idType"/>  <!-- New for V5.2 of schema -->
        <xsd:element name="FirstRawObservation" type="circleType"/>
        <xsd:element name="SecondRawObservation" type="circleType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DistanceOffset">
    <xsd:annotation>
      <xsd:documentation>The DistanceOffset element records the raw observation and entered values for a distance offset measurement.
                         The offset direction is ALWAYS recorded in the JobXML file as viewed FROM the instrument station and as 
                         measured from the prism position TO the required offset point position.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="RawObservation" type="circleType"/>
        <xsd:element name="Direction" type="offsetDirectionType" minOccurs="0"/> <!-- Left in for compatability with pre-Version 11.30 files but now unused -->
        <xsd:element name="Distance" type="distType" minOccurs="0"/>             <!-- Left in for compatability with pre-Version 11.30 files but now unused -->
        <xsd:element name="LeftRightOffset" type="distType"/>
        <xsd:element name="InOutOffset" type="distType"/>
        <xsd:element name="DownUpOffset" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Attribute">
    <xsd:annotation>
      <xsd:documentation>The Attribute element records an attribute associated with (requested for) a Feature assigned to a point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Value" type="xsd:string"/>  <!-- This is a string element that may contain a numeric value, text, a date (in the format YYYY-MM-DD as per xsd:dateTime type), a time (in the format hh:mm:ss as per xsd:dateTime) or a file name -->
        <xsd:element name="Type" type="attributeType"/>
        <xsd:element name="ItemIndex" type="xsd:integer" minOccurs="0"/> <!-- Present if Type = Menu -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Feature">
    <xsd:annotation>
      <xsd:documentation>The Feature element holds the set attributes associated with a Feature assigned to a point.  The
                         Feature definition comes from the Feature and Attribute library associated with the job.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="Attribute" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="Name" type="nameType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Features">
    <xsd:annotation>
      <xsd:documentation>The Features element holds all the Features assigned to an entity</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="Feature" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AverageResidual">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The AverageResidual element holds the residuals details relating to an 
                         observation that has been used in the computation of an averaged position.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ObservationID" type="idType"/>
        <xsd:element name="IncludedInAverageComputation" type="booleanType"/>
        <xsd:element name="GridResidual" type="gridDeltaType"/>
        <xsd:element ref="AngleResidual"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="AverageResiduals">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The AverageResiduals element holds an AverageResidual element for each 
                         point used in the computation of an averaged position.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AverageResidual" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="LineTransformationDefinition">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The LineTransformationDefinition element holds the details of a line transformation
                         used for transforming between 'Grid (local)' and database grid positions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="StartPointName" type="nameType"/>
        <xsd:element name="StartPointGrid" type="horizGridPosType"/>
        <xsd:element name="StartPointLocalGrid" type="horizGridPosType"/>
        <xsd:element name="EndPointName" type="nameType"/>
        <xsd:element name="EndPointGrid" type="horizGridPosType"/>
        <xsd:element name="EndPointLocalGrid" type="horizGridPosType"/>
        <xsd:element name="ComputedGroundDistance" type="distType"/>
        <xsd:element name="ComputedLocalGridDistance" type="distType"/>
        <xsd:element name="DeltaComputedHorizontalDistance" type="distType"/>
        <xsd:element name="ScaleApplicationMethod" type="scaleApplicationMethodType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="HorizontalParameters">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>A horizontal parameters element contains the horizontal adjustment 
                         parameters for a Helmert transformation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="OriginNorth" type="coordType" minOccurs="0"/>
        <xsd:element name="OriginEast" type="coordType" minOccurs="0"/>
        <xsd:element name="TranslationNorth" type="distType" minOccurs="0"/>
        <xsd:element name="TranslationEast" type="distType" minOccurs="0"/>
        <xsd:element name="Rotation" type="deltaAngleType" minOccurs="0"/>
        <xsd:element name="ScaleFactor" type="xsd:double" minOccurs="0"/>
        <xsd:element name="TranslationNorthStandardError" type="distType" minOccurs="0"/>
        <xsd:element name="TranslationEastStandardError" type="distType" minOccurs="0"/>
        <xsd:element name="RotationStandardError" type="deltaAngleType" minOccurs="0"/>
        <xsd:element name="ScaleFactorStandardError" type="xsd:double" minOccurs="0"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="VerticalParameters">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>A vertical parameters element contains the vertical adjustment 
                         parameters for a Helmert transformation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="OriginNorth" type="coordType" minOccurs="0"/>
        <xsd:element name="OriginEast" type="coordType" minOccurs="0"/>
        <xsd:element name="SlopeNorthPerUnit" type="xsd:double" minOccurs="0"/>
        <xsd:element name="SlopeEastPerUnit" type="xsd:double" minOccurs="0"/>
        <xsd:element name="ConstantAdjustment" type="distType" minOccurs="0"/>
        <xsd:element name="SlopeNorthPerUnitStandardError" type="xsd:double" minOccurs="0"/>
        <xsd:element name="SlopeEastPerUnitStandardError" type="xsd:double" minOccurs="0"/>
        <xsd:element name="ConstantAdjustmentStandardError" type="distType" minOccurs="0"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PointPair">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The PointPair element holds the details for a pair of points (Grid point and 
                         'Grid (local)' point) used in a Helmert transformation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="GridPointName" type="nameType"/>
        <xsd:element name="LocalGridPointName" type="nameType"/>
        <xsd:element name="Dimension" type="calibDimensionType"/>
        <xsd:element name="HorizontalResidual" type="distOrNullType"/>
        <xsd:element name="VerticalResidual" type="distOrNullType"/>
        <xsd:element name="Residual3D" type="distOrNullType"/>
        <xsd:element name="GridResidual" type="gridDeltaType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PointPairs">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The PointPairs element holds all the PointPair elements providing 
                         details for of the points used in a Helmert transformation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="PointPair" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="HelmertTransformationParameters">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The HelmertTransformationParameters element holds the details of a Helmert transformation
                         used for transforming between 'Grid (local)' and database grid positions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="HorizontalParameters" minOccurs="0"/>
        <xsd:element ref="VerticalParameters" minOccurs="0"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="HelmertTransformationDefinition">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The HelmertTransformationDefinition element holds the details of a Helmert transformation
                         (the resultant parameters plus the definition point pair details) used for  
                         transforming between 'Grid (local)' and database grid positions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="HelmertTransformationParameters"/>
        <xsd:element ref="PointPairs"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="SevenParameterTransformationParameters">  <!-- New for V5.3 of schema -->
    <xsd:annotation>
      <xsd:documentation>A seven paremter transformation parameters element contains the 
                         adjustment parameters for a Seven Parameter transformation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="RotationX" type="deltaAngleType" minOccurs="0"/>
        <xsd:element name="RotationY" type="deltaAngleType" minOccurs="0"/>
        <xsd:element name="RotationZ" type="deltaAngleType" minOccurs="0"/>
        <xsd:element name="TranslationX" type="distType" minOccurs="0"/>
        <xsd:element name="TranslationY" type="distType" minOccurs="0"/>
        <xsd:element name="TranslationZ" type="distType" minOccurs="0"/>
        <xsd:element name="ScaleFactor" type="xsd:double" minOccurs="0"/>  <!-- Unity based scale factor value -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SevenParameterTransformationDefinition">  <!-- New for V5.3 of schema -->
    <xsd:annotation>
      <xsd:documentation>The SevenParameterTransformationDefinition element holds the details of a Seven 
                         Parameter transformation (the resultant parameters plus the definition point pair 
                         details) used for transforming between 'Grid (local)' and database grid positions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="SevenParameterTransformationParameters"/>
        <xsd:element ref="PointPairs"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="LocalTransformationRecord">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The LocalTransformation element holds the details a transformation defined 
                         for transforming between 'Grid (local)' and database grid positions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TransformationName" type="xsd:string"/>
        <xsd:element name="Method" type="transformationMethodType"/>
        <xsd:element name="VerticalAdjustmentMethod" type="transformationVerticalAdjustmentMethodType" minOccurs="0"/> <!-- Made optional in V5.3 of schema (Doesn't apply to a Seven Parameter transformation) -->
        <xsd:element name="Deleted" type="booleanType"/>
        <xsd:element name="Source" type="xsd:string" minOccurs="0"/> <!-- New for V5.2 of schema. File name for source of imported transformation definition (only present on imported transformation definitions). -->
        <xsd:element ref="LineTransformationDefinition" minOccurs="0"/>
        <xsd:element ref="HelmertTransformationDefinition" minOccurs="0"/>
        <xsd:element ref="SevenParameterTransformationDefinition" minOccurs="0"/>
        <xsd:element ref="Notes" minOccurs="0"/>      <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="EchoSounderConfigurationRecord">  <!-- New for V5.51 of schema -->
    <xsd:annotation>
      <xsd:documentation>The EchoSounderConfigurationRecord element records details relating to the echo sounder 
                         being used and its setup. Provides details of the Echo sounder type (as read from the 
                         definition file), the Echo sounder latency in seconds and the Echo sounder draft value.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Type" type="xsd:string"/>
        <xsd:element name="Latency" type="timeIntervalType"/>
        <xsd:element name="Draft" type="doubleOrNullType"/>   <!-- Depth of echo sounder transponder below water surface - assumes that the antenna/target height will provide a continuous point position at water surface -->
        <xsd:element ref="Notes" minOccurs="0"/>    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Data">  <!-- New for V5.51 of schema -->
    <xsd:annotation>
      <xsd:documentation>Data elements hold extra details that have been recorded along with an echo sounding</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>     <!-- The name for the recorded value that comes from the Echo sounder definition file -->
        <xsd:element name="Value" type="xsd:string"/>  <!-- This is a string element that may contain a numeric or text value -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EchoSounding">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The EchoSounding element holds the depth returned from a commented echo sounder</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Depth" type="distType"/>
        <xsd:element name="ExtraData" minOccurs="0">  <!-- New for V5.51 of schema -->
          <xsd:complexType> 
            <xsd:choice>
              <xsd:element ref="Data" maxOccurs="unbounded"/>
            </xsd:choice>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="OrthogonalIntersectionPointDetails">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The OrthogonalIntersectionPointDetails element is used in the Measure3DAxes  
                         element to record details of the computed orthogonal intersection position</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="HorizontalOffset" type="distType"/>
        <xsd:element name="PerpendicularOffset" type="distType"/>
        <xsd:element name="RadialOffset" type="distType"/>
        <xsd:element name="DistanceToPoint1" type="distType"/>
        <xsd:element name="DistanceToPoint2" type="distType"/>
        <xsd:element name="IntersectionPoint" type="gridPosType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="VerticalIntersectionPointDetails">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The VerticalIntersectionPointDetails element is used in the Measure3DAxes  
                         element to record details of the computed vertical intersection position</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="HorizontalOffset" type="distType"/>
        <xsd:element name="VerticalOffset" type="distOrNullType"/>
        <xsd:element name="RadialOffset" type="distType"/>
        <xsd:element name="DistanceToPoint1" type="distType"/>
        <xsd:element name="DistanceToPoint2" type="distType"/>
        <xsd:element name="IntersectionPoint" type="gridPosType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="Measure3DAxes">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>The Measure3DAxes element records the axis definition points, the measurement 
                         details and the computed offsets for the Measure 3D axes measurement option</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Point1" type="gridPointType"/>
        <xsd:element name="Point2" type="gridPointType"/>
        <xsd:element ref="OrthogonalIntersectionPointDetails"/>
        <xsd:element ref="VerticalIntersectionPointDetails"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="MiningAutostake">    <!-- New for the V5.5 schema -->
    <xsd:annotation>
      <xsd:documentation>The MiningAutostake element provides the delta details for mining 
                         auto-staked mining points. It provides the HorizontalDelta, VerticalDelta and 
                         Delta values for each staked position as well as the Station, Horizontal and 
                         Vertical offsets and the definition point names as appropriate for the chosen method.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Method" type="miningAutostakeMethodType"/>
        <xsd:element name="Station" type="distOrNullType"/>          <!-- The station along the defined line for the staked point - reported for the AutostakeCenterline and AutostakeGradeline methods -->
        <xsd:element name="StationOffset" type="distOrNullType"/>    <!-- New for the V5.58 schema: The specified station offset from the defined line - reported for the AutostakeCenterline method -->  
        <xsd:element name="HorizontalOffset" type="distOrNullType"/> <!-- The specified horizontal offset from the defined line - reported for the AutostakeCenterline, AutostakeGradeline and AutostakeProjectline methods -->  
        <xsd:element name="VerticalOffset" type="distOrNullType"/>   <!-- The specified vertical offset from the defined line - reported for the AutostakeCenterline, AutostakeGradeline and AutostakeProjectline methods -->
        <xsd:element name="StationDelta" type="distOrNullType"/>     <!-- New for the V5.58 schema: The station delta from the defined line to the measured position - reported for the AutostakeCenterline, AutostakeGradeline, AutostakeLaserline and AutostakeLaserlineFromCenterline method -->
        <xsd:element name="HorizontalDelta" type="distOrNullType"/>  <!-- The horizontal offset from the defined line to the measured position - reported for the AutostakeCenterline, AutostakeGradeline, AutostakeLaserline, AutostakeLaserlineFromCenterline, AutostakeBlasthole and AutostakeProjectline methods --> <!-- New for the V5.56 schema -->
        <xsd:element name="VerticalDelta" type="distOrNullType"/>    <!-- The vertical offset from the defined line to the measured position - reported for the AutostakeCenterline, AutostakeGradeline, AutostakeLaserline, AutostakeLaserlineFromCenterline, AutostakeBlasthole and AutostakeProjectline methods --> <!-- New for the V5.56 schema -->
        <xsd:element name="Delta" type="distType"/>                  <!-- This is the actual delta distance from the required position to the observed position - reported for all methods -->
        <xsd:element name="Point1" type="xsd:string"/>               <!-- The line start point for the AutostakeCenterline and AutostakeGradeline methods, the collar point for the AutostakeBlasthole method, the pivot point for the AutostakePivotpoint method and the laser line point nearest to the measured point for the AutostakeLaserline method -->
        <xsd:element name="Point2" type="xsd:string"/>               <!-- The line end point for the AutostakeCenterline and AutostakeGradeline methods, the toe point for the AutostakeBlasthole method and the laser line point furthest from the measured point for the AutostakeLaserline method (no second point for the AutostakePivotpoint method) -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CustomContent">    <!-- New for the V5.57 schema -->
    <xsd:complexType> 
      <xsd:sequence>
        <xsd:any namespace='##any' minOccurs='0' maxOccurs='unbounded' processContents='skip'/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CustomXmlSubRecord">    <!-- New for the V5.57 schema -->
    <xsd:annotation>
      <xsd:documentation>The CustomXmlSubRecord element provides an element for inclusion in PointRecord 
                         elements allowing a SCAPI application to record custom data associated with points.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="CustomTypeRecordId" type="idType"/> <!-- An ID referencing the CustomTypeRecord that defines the record type anme and version -->
        <xsd:element name="TypeVersion" type="xsd:integer"/>   <!-- An integer value assigned by the SCAPI application to identify the custom record version. -->
        <xsd:element name="Creator" type="xsd:string"/>        <!-- The name of the SCAPI application that wrote this custom sub-record -->  
        <xsd:element ref="CustomContent"/>                     <!-- The actual XML record structure containing the custom data written by the SCAPI application. -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PointOnPlane">  <!-- New for V5.62 of schema -->
    <xsd:annotation>
      <xsd:documentation>The PointOnPlane element records the details relating to the 
                         measurement of a point with respect to a defined plane</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="PerpendicularDelta" type="distOrNullType"/>  <!-- The perpendicular delta from the measured position to the defined plane -->
        <xsd:element name="IntersectionWithPlane" type="booleanType"/>  <!-- A flag that indicates whether or not the point was computed by intersection of an angles only observation with the defined plane -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="Layer">  <!-- New for V5.64 of schema - used by Survey Pro system -->
    <xsd:annotation>
      <xsd:documentation>The LayerRecord element contains the properties of a layer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="xsd:string"/>
        <xsd:element name="Visible" type="booleanType"/>
        <xsd:element name="Locked" type="booleanType"/>
        <xsd:element name="Selectable" type="booleanType"/>
        <xsd:element name="Active" type="booleanType"/>
        <xsd:element name="Control" type="booleanType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PointRecord">
    <xsd:annotation>
      <xsd:documentation>The PointRecord element holds all details for a point, including the details of how its position was fixed</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="Description1" type="pointDescType" minOccurs="0"/>         <!-- Present if the option for extra point descriptions has been switched on -->
        <xsd:element name="Description2" type="pointDescType" minOccurs="0"/>         <!-- Present if the option for extra point descriptions has been switched on -->
        <xsd:element name="Method" type="pointCreationType"/>
        <xsd:element name="SurveyMethod" type="surveyMethodType"/>
        <xsd:element name="Classification" type="classificationType"/>
        <xsd:element name="Deleted" type="booleanType"/>
        <xsd:element name="Source" type="xsd:string" minOccurs="0"/>                  <!-- New for V5.2 of schema. File name for source of imported point. -->
        <xsd:element name="Grid" type="gridPosType" minOccurs="0"/>                   <!-- Present if the point has grid coordinates only (no observation data): Method = Coordinates | FromABaseline | BearngVerticalPlaneIntersection | BearingDistanceIntersection | BearingBearingIntersection | DistanceDistanceIntersection | CogoCalculated | Resection | CopiedPoint (if the copied point was a grid point) | TraverseAdjusted | ReflineSetup -->
        <xsd:element name="LocalGrid" type="localGridPosType" minOccurs="0"/>         <!-- New for V5.2 of schema --> <!-- Present if the point has been entered (keyed in or imported/copied) as a 'Grid (local)' point - the ID of the transformation used to transform the local grid coordinates to database grid coordinates is included -->
        <xsd:element name="ComputedGrid" type="gridPosType" minOccurs="0"/>           <!-- Present if a grid coordinate could be computed from the observed data.  Will never be present if the Projection Type = NoProjection.  Will not be present for an incomplete observation eg. an angles only observation. -->
        <xsd:element name="ComputedLocalGrid" type="localGridPosType" minOccurs="0"/> <!-- New for V5.2 of schema --> <!-- Present if the current coordinate display is 'Grid (local)' and there is a valid local grid transformation selected (the ID of the transformation is included in the record) -->
        <xsd:element ref="WGS84" minOccurs="0"/>                                      <!-- Present if the point has WGS-84 coordinates only: Method = Coordinates | FourPointIntersection | Gps2PointOffset | Gps4PointIntersection | GpsSingleAutonomousPoint | GpsAverageAutonomousPoint | GpsFastStaticObservation -->
        <xsd:element ref="Local" minOccurs="0"/>                                      <!-- Present if the point has local ellipsoid coordinates only: Method = Coordinates | CopiedPoint -->
        <xsd:element ref="ECEF" minOccurs="0"/>                                       <!-- Present if Method = GpsFastStaticObservation | GpsStaticObservation | GpsRTKControlPoint | GpsRTKQuickMark | GpsCalibrationPoint | GpsContinuousDistancePoint | GpsContinuousTimePoint | GpsContinuousDistanceAndTimePoint | GpsContinuousDistanceOrTimePoint all with SurveyMethod = NetworkFloat | NetworkFix (VRS data) -->
        <xsd:element ref="RTXECEF" minOccurs="0"/>                                    <!-- Present if SurveyMethod = RTX - new for V5.64 of schema -->
        <xsd:element ref="StationAndOffset" minOccurs="0"/>                           <!-- Present if the point has been created using keyed in station and offset values (Method = Coordinates, SurveyMethod = KeyedIn).  This element's presence indicates how the point was keyed in. -->
        <xsd:element name="Circle" type="circleType" minOccurs="0"/>                  <!-- Present if Method = AngleOnly | HorizontalAngleOnly | DirectReading | AverageMeasurements | AngleOffset | HorizontalAngleOffset | VerticalAngleOffset | DistanceOffset | DualPrismOffset | CircularObject | RemoteObject i.e. present for terrestrial observations -->
        <xsd:element ref="MTA" minOccurs="0"/>                                        <!-- Present if Method = MeanTurnedAngle -->
        <xsd:element ref="LevelData" minOccurs="0"/>                                  <!-- Present if Method = LevelObservation -->
        <xsd:element ref="ReducedLevelData" minOccurs="0"/>                           <!-- Present if Method = ReducedLevelObservation - replaced by ReducedLevelObsReference and plan to deprecate in future -->
        <xsd:element ref="ReducedLevelObsReferences" minOccurs="0"/>                  <!-- Present if Method = ReducedLevelObservation -->
        <xsd:element ref="LaserOffset" minOccurs="0"/>                                <!-- Present if Method = LaserOffset -->
        <xsd:element name="Polar" type="polarType" minOccurs="0"/>                    <!-- Present if Method = BearingAndDistanceFromAPoint | BearingDistancePointIntersection | TurnedAngleAndDistance | GpsContinuousOffsetLeftPoint | GpsContinuousOffsetRightPoint -->
        <xsd:element name="ObservationPolarDeltas" type="polarType" minOccurs="0"/>   <!-- Present if the PointRecord includes a Circle element and the Classification = Backsight | Check -->
        <xsd:element name="GridStandardDeviations" type="gridStdDeviationsType" minOccurs="0"/> <!-- Added for v11.20 --> <!-- Present if the point is an averaged position computed using the Cogo/Compute average option - Classification = Averaged -->
        <xsd:element ref="AverageResiduals" minOccurs="0"/>                           <!-- New for V5.2 of schema --> <!-- Present if the point is an averaged position computed using the Cogo/Compute average option - Classification = Averaged -->
        <xsd:element ref="ECEFDeltas" minOccurs="0"/>                                 <!-- Present if Method = GpsStaticObservation | GpsRTKControlPoint | GpsRTKQuickMark | GpsCalibrationPoint | GpsContinuousDistancePoint | GpsContinuousTimePoint | GpsContinuousDistanceAndTimePoint | GpsContinuousDistanceOrTimePoint all with SurveyMethod = Fix (RTK data) -->
        <xsd:element ref="Precision" minOccurs="0"/>                                  <!-- Present for RTK or VRS observations - ECEF or ECEFDeltas elements are present in PointRecord -->
        <xsd:element ref="QualityControl1" minOccurs="0"/>                            <!-- Present if Trimble Access was set to record QC1 records -->
        <xsd:element ref="QualityControl2" minOccurs="0"/>                            <!-- Present if Trimble Access was set to record QC2 records -->
        <xsd:element ref="QualityControl3" minOccurs="0"/>                            <!-- Present if Trimble Access was set to record QC3 records -->
        <xsd:element name="StationID" type="idType" minOccurs="0"/>                   <!-- Present if the PointRecord includes a Circle or MTA element - it is a terrestrial observation point -->
        <xsd:element name="BackBearingID" type="idType" minOccurs="0"/>               <!-- Present if the PointRecord includes a Circle or MTA element - it is a terrestrial observation point -->
        <xsd:element name="TargetID" type="idType" minOccurs="0"/>                    <!-- Present if the PointRecord includes a Circle or MTA element - it is a terrestrial observation point -->
        <xsd:element name="Pressure" type="doubleOrNullType" minOccurs="0"/>          <!-- Present if the PointRecord includes a Circle or MTA element - it is a terrestrial observation point -->
        <xsd:element name="Temperature" type="doubleOrNullType" minOccurs="0"/>       <!-- Present if the PointRecord includes a Circle or MTA element - it is a terrestrial observation point -->
        <xsd:element name="ReferenceRecordID" type="idType" minOccurs="0"/>           <!-- Present if the PointRecord includes an ECEFDeltas or ECEF element - it is a RTK or RTK/VRS GPS point. New for the v4.0 schema definition (Survey Controller v11.30) -->
        <xsd:element name="RTK_Base" type="nameType" minOccurs="0"/>                  <!-- Present if the PointRecord includes an ECEFDeltas element - it is a RTK or RTK/VRS GPS point -->
        <xsd:element name="AntennaID" type="idType" minOccurs="0"/>                   <!-- Present if the PointRecord includes an ECEFDeltas or ECEF element - it is a RTK or RTK/VRS GPS point -->
        <xsd:element name="SourcePoint" type="nameType" minOccurs="0"/>               <!-- Present when Polar, Ground or Azimuth elements are present in PointRecord -->
        <xsd:element name="BasePoint" type="nameType" minOccurs="0"/>                 <!-- Present when Method = LaserOffset -->
        <xsd:element name="LaserHeight" type="distType" minOccurs="0"/>               <!-- Present when Method = LaserOffset -->
        <xsd:element name="LaserTargetHeight" type="distType" minOccurs="0"/>         <!-- Present when Method = LaserOffset -->
        <xsd:element name="MagneticDeclination" type="deltaAngleType" minOccurs="0"/> <!-- Present when Method = LaserOffset -->
        <xsd:element name="Azimuth" type="angleType" minOccurs="0"/>                  <!-- Present when Method = AzimuthOnly -->
        <xsd:element name="AzimuthType" type="azimuthType" minOccurs="0"/>            <!-- Present when Polar, Ground or Azimuth elements are present in PointRecord -->
        <xsd:element name="DistanceType" type="distanceType" minOccurs="0"/>          <!-- Present when Polar or Ground elements are present in PointRecord -->
        <xsd:element ref="Stakeout" minOccurs="0"/>                                   <!-- Present when Classification = Stakeout (points that have been staked out) -->
        <xsd:element ref="ResectionStandardErrors" minOccurs="0"/>                    <!-- Present if Method = Resection (a resected station point) -->
        <xsd:element ref="DualPrismOffset" minOccurs="0"/>                            <!-- Present if Method = DualPrismOffset -->
        <xsd:element ref="CircularObject" minOccurs="0"/>                             <!-- Present if Method = CircularObject -->
        <xsd:element ref="RemoteObject" minOccurs="0"/>                               <!-- Present if Method = RemoteObject -->
        <xsd:element ref="EnteredData" minOccurs="0"/>                                <!-- Present when point has been computed from a Cogo calculation -->
        <xsd:element ref="AngleOffset" minOccurs="0"/>                                <!-- Present if Method = AngleOffset -->
        <xsd:element ref="DistanceOffset" minOccurs="0"/>                             <!-- Present if Method = DistanceOffset -->
        <xsd:element ref="EchoSounding" minOccurs="0"/>                               <!-- New for V5.2 of schema --> <!-- Present if there has been an echo sounder depth stored with the point - only available in GPS continuous topo functions -->
        <xsd:element ref="Measure3DAxes" minOccurs="0"/>                              <!-- New for V5.2 of schema --> <!-- Present if the 'Measure 3D axes' Survey menu option has been used -->
        <xsd:element ref="PositionInTunnel" minOccurs="0"/>                           <!-- New for V5.4 of schema --> <!-- Present if the 'Position' option in the Tunnels module has been used -->
        <xsd:element ref="MiningAutostake" minOccurs="0"/>                            <!-- New for V5.5 of schema --> <!-- Present if the Mining auto stake functionality has been used -->
        <xsd:element name="DeviceAxisOrientation" type="deviceAxisOrientationType" minOccurs="0"/> <!-- New for V5.56 of schema --> <!-- Data from which the orientation of the device axis can be determined -->
        <xsd:element ref="CustomXmlSubRecord" minOccurs="0"/>                         <!-- New for V5.57 of schema --> <!-- Present when a SCAPI application has added custom data to a PointRecord -->
        <xsd:element name="PhotogrammetryObs" type="photoObsType" minOccurs="0"/>     <!-- New for V5.6 of schema --> <!-- Present when the position has been determined by photogrammetric means -->
        <xsd:element ref="PointOnPlane" minOccurs="0"/>                               <!-- New for V5.62 of schema --> <!-- Present if the point has been measured with respect to a defined plane -->
        <xsd:element ref="Layer" minOccurs="0"/>                                      <!-- New for V5.64 of schema - used by Survey Pro system -->
        <xsd:element ref="Notes" minOccurs="0"/>                                      <!-- Present when note added to this item in 'Review current job'  -->
        <xsd:element ref="Features" minOccurs="0"/>                                   <!-- Present when Feature and Attribute details have been assigned to the point when it was recorded (based on the Features and Attributes defined in the Feature and Attribute library associated with the job) -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LineRecord">
    <xsd:annotation>
      <xsd:documentation>The LineRecord element contains the details relating to a line and how it was defined</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="Method" type="lineMethodType"/>
        <xsd:element name="Deleted" type="booleanType"/>
        <xsd:element name="StartPoint" type="nameType"/>
        <xsd:element name="EndPoint" type="nameType" minOccurs="0"/>     <!-- Present when Method = TwoPoints -->
        <xsd:element name="Azimuth" type="angleType" minOccurs="0"/>     <!-- Present when Method = BearingAndDistance -->
        <xsd:element name="Length" type="distType" minOccurs="0"/>       <!-- Present when Method = BearingAndDistance -->
        <xsd:element name="Grade" type="gradeOrNullType" minOccurs="0"/> <!-- Present when Method = BearingAndDistance -->
        <xsd:element name="StartStation" type="coordType"/>
        <xsd:element name="StationInterval" type="distOrNullType"/>
        <xsd:element ref="CustomXmlSubRecord" minOccurs="0"/>            <!-- New for V5.57 of schema --> <!-- Present when a SCAPI application has added custom data to a PointRecord -->
        <xsd:element ref="Notes" minOccurs="0"/>                         <!-- Present when note added to this item in 'Review current job'  -->
        <xsd:element ref="Features" minOccurs="0"/>                      <!-- Present when Feature and Attribute details have been assigned to the line when it was recorded (based on the Features and Attributes defined in the Feature and Attribute library associated with the job) -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ArcRecord">
    <xsd:annotation>
      <xsd:documentation>The ArcRecord element contains the details relating to an arc and how it was defined</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="Method" type="arcMethodType"/>
        <xsd:element name="Deleted" type="booleanType"/> 
        <xsd:element name="StartPoint" type="nameType" minOccurs="0"/>          <!-- Present when Method = TwoPointsAndRadius | ArcLengthAndRadius | DeltaAngleAndRadius | TwoPointsAndCenterPoint | ThreePoints -->
        <xsd:element name="EndPoint" type="nameType" minOccurs="0"/>            <!-- Present when Method = TwoPointsAndRadius | TwoPointsAndCenterPoint | ThreePoints -->
        <xsd:element name="Direction" type="directionType" minOccurs="0"/>      <!-- Present for all Methods except for ThreePoints Method -->
        <xsd:element name="Radius" type="distType" minOccurs="0"/>              <!-- Present when Method = TwoPointsAndRadius | ArcLengthAndRadius | DeltaAngleAndRadius | IntersectionPointAndTangents -->
        <xsd:element name="StartAzimuth" type="angleType" minOccurs="0"/>       <!-- Present when Method = ArcLengthAndRadius | DeltaAngleAndRadius | IntersectionPointAndTangents -->
        <xsd:element name="Length" type="distType" minOccurs="0"/>              <!-- Present when Method = ArcLengthAndRadius -->
        <xsd:element name="Grade" type="gradeOrNullType" minOccurs="0"/>        <!-- Present when Method = ArcLengthAndRadius | DeltaAngleAndRadius -->
        <xsd:element name="DeltaAngle" type="deltaAngleType" minOccurs="0"/>    <!-- Present when Method = DeltaAngleAndRadius -->
        <xsd:element name="IntersectionPoint" type="nameType" minOccurs="0"/>   <!-- Present when Method = IntersectionPointAndTangents -->
        <xsd:element name="EndAzimuth" type="angleType" minOccurs="0"/>         <!-- Present when Method = IntersectionPointAndTangents -->
        <xsd:element name="CenterPoint" type="nameType" minOccurs="0"/>         <!-- Present when Method = TwoPointsAndCenterPoint -->
        <xsd:element name="OtherPointOnArc" type="nameType" minOccurs="0"/>     <!-- Present when Method = ThreePoints -->
        <xsd:element name="StartStation" type="coordType"/>
        <xsd:element name="StationInterval" type="distOrNullType"/>
        <xsd:element ref="CustomXmlSubRecord" minOccurs="0"/>                   <!-- New for V5.57 of schema --> <!-- Present when a SCAPI application has added custom data to a PointRecord -->
        <xsd:element ref="Notes" minOccurs="0"/>                                <!-- Present when note added to this item in 'Review current job'  -->
        <xsd:element ref="Features" minOccurs="0"/>                             <!-- Present when Feature and Attribute details have been assigned to the arc when it was recorded (based on the Features and Attributes defined in the Feature and Attribute library associated with the job) -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CalibrationPointRecord">
    <xsd:annotation>
      <xsd:documentation>The CalibrationPointRecord element holds the details for a pair of points (GPS point and grid point)
                         used in a GPS calibration</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="GridPointName" type="nameType"/>
        <xsd:element name="WGS84PointName" type="nameType"/>
        <xsd:element name="Dimension" type="calibDimensionType"/>
        <xsd:element name="HorizontalResidual" type="distOrNullType"/>
        <xsd:element name="VerticalResidual" type="distOrNullType"/>
        <xsd:element name="Residual3D" type="distOrNullType"/>
        <xsd:element name="GridResidual" type="gridDeltaType"/>
        <xsd:element ref="Notes" minOccurs="0"/>    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="GPSEquipmentRecord">
    <xsd:annotation>
      <xsd:documentation>The GPSEquipmentRecord element holds the details for the GPS receiver used and its associated antenna</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="ReceiverType" type="xsd:string"/>
        <xsd:element name="ReceiverSerialNumber" type="xsd:string"/>
        <xsd:element name="ReceiverFirmwareVersion" type="doubleOrNullType"/>
        <xsd:element name="AntennaType" type="xsd:string"/>
        <xsd:element name="AntennaSerialNumber" type="xsd:string"/>
        <xsd:element name="AntennaNumber" type="xsd:integer"/>
        <xsd:element name="AntennaMeasurementMethod" type="xsd:string"/>
        <xsd:element name="AntennaMethod" type="xsd:integer"/>
        <xsd:element name="AntennaTapeAdjustment" type="distOrNullType"/>
        <xsd:element name="AntennaHorizontalOffset" type="distOrNullType"/>
        <xsd:element name="AntennaVerticalOffset" type="distOrNullType"/>
        <xsd:element name="AntennaMeasurementKey" type="antennaMeasurementKey" minOccurs="0"/>  <!-- New for V5.64 of schema. -->
        <xsd:element ref="MeasurementStack" minOccurs="0"/>   <!-- New for V5.64 of schema.  Present when a V10 AntennaMeasurementMethod is selected -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AntennaRecord">
    <xsd:annotation>
      <xsd:documentation>The AntennaRecord element holds the details for the GPS receiver used and its associated antenna</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="MeasuredHeight" type="distOrNullType"/>
        <xsd:element name="MeasurementType" type="antennaMeasType"/>
        <xsd:element name="GPSEquipmentID" type="idOrNullType"/>
        <xsd:element name="ReducedHeight" type="distOrNullType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RoverSurveyOptionsRecord" type="surveyOptionsType"/>

  <xsd:element name="BaseSurveyOptionsRecord" type="surveyOptionsType"/>

  <xsd:element name="SurveyEventRecord">
    <xsd:annotation>
      <xsd:documentation>The SurveyEventRecord element records the occurrence of a given survey event</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="Event" type="surveyEventType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ContinuousParametersRecord">
    <xsd:annotation>
      <xsd:documentation>The ContinuousParametersRecord element holds the settings defined for a continuous survey operation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="TriggerMethod" type="contTriggerMethodType"/>
        <xsd:element name="TriggerTime" type="xsd:double" minOccurs="0"/>     <!-- Present only if the trigger method type is based on a time interval -->
        <xsd:element name="TriggerDistance" type="distType" minOccurs="0"/>   <!-- Present only if the trigger method type is based on a distance interval -->
        <xsd:element name="StopTime" type="timeIntervalType" minOccurs="0"/>  <!-- Present only if the trigger method type is StopAndGo - new from Survey Controller v11.30 on -->
        <xsd:element name="OffsetType" type="contOffsetType"/>
        <xsd:element name="OffsetOne" type="contOffsetDirectionType" minOccurs="0"/>  <!-- Present only if an Offset 1 is specified in Trimble Access -->
        <xsd:element name="OffsetTwo" type="contOffsetDirectionType" minOccurs="0"/>  <!-- Present only if an Offset 2 is specified in Trimble Access -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="InitialisationRecord">
    <xsd:annotation>
      <xsd:documentation>The InitialisationRecord element records the occurrence of a GPS initialisation event</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="InitialisationEvent" type="initEventType"/>
        <xsd:element name="Time" type="GPSTimeType"/>
        <xsd:element name="InitialisationType" type="initialisationType"/>
        <xsd:element name="SurveyType" type="gpsSurveyType"/>
        <xsd:element name="InitialisationCounter" type="integerOrNullType"/>
        <xsd:element name="PointName" type="xsd:string"/>  <!-- Don't use nameType as this is allowed to be empty (depending on initialisation type) -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ReferenceRecord">
    <xsd:annotation>
      <xsd:documentation>The ReferenceRecord element records the name of the current GPS RTK base station</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="PointName" type="nameType"/>
        <xsd:element name="AntennaID" type="idType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PostProcessRecord">
    <xsd:annotation>
      <xsd:documentation>The PostProcessRecord element records the name of the postprocessed file being 
                         collected and the start time for the data in this file</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="FileName" type="xsd:string"/>
        <xsd:element name="StartTime" type="GPSTimeType"/>
        <xsd:element ref="Notes" minOccurs="0"/>          <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelLoopOpenRecord">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The LevelLoopOpenRecord element indicates the start of a level run</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="LoopName" type="xsd:string"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="LoopMethod" type="levelLoopMethodType" minOccurs="0"/>  <!-- New for the V5.2 schema - made optional in V5.57 schema-->
        <xsd:element name="StartPoint" type="levelLoopStartEndPointType"/>
        <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelLoopCloseRecord">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The LevelLoopCloseRecord element records the closure of a level run</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="LoopID" type="idType"/>
        <xsd:element name="EndPoint" type="levelLoopStartEndPointType"/>
        <xsd:element name="Misclose" type="distOrNullType"/>
        <xsd:element name="MaxLoopMiscloseTolerance" type="distOrNullType" minOccurs="0"/>
        <xsd:element name="AllowableMisclose" type="distOrNullType" minOccurs="0"/>
        <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LevelTestResultsRecord">  <!-- New in Version 4.0 of schema - for TDS SurveyPro level data -->
    <xsd:annotation>
      <xsd:documentation>The LevelTestResultsRecord element holds details of a level test that has been carried out</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="MidPositionReadings" type="levelTestReadingsType"/>
        <xsd:element name="EndPositionReadings" type="levelTestReadingsType"/>
        <xsd:element name="HorizontalDistance" type="distType"/>
        <xsd:element name="ComputedError" type="xsd:double"/>
        <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PointDesign">
    <xsd:annotation>
      <xsd:documentation>The PointDesign element contains details of the design point that has been staked</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="StakeoutMethod" type="stakeoutMethodType"/>
        <xsd:element name="DesignElevation" type="coordOrNullType"/>
        <xsd:element name="OriginalDesignElevation" type="coordOrNullType" minOccurs="0"/> <!-- Present if the design elevation has been edited in Trimble Access prior to staking out -->
        <xsd:element name="ReferenceAzimuth" type="angleOrNullType" minOccurs="0"/> <!-- New for V5.2 of schema. Reference azimuth used for point stakeout (only present when StakeoutMethod is PointRelativeToAzimuth). -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ExtraPointDesignElevation">  <!-- New for the v12.20 Survey Controller -->
    <xsd:annotation>
      <xsd:documentation>The ExtraPointDesignElevation element contains any additional design elevation values that 
                         have been entered by using the functionality available in the custom as staked deltas</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DesignElevation" type="coordOrNullType"/>
        <xsd:element name="VerticalOffset" type="coordOrNullType"/>
        <xsd:element name="DeltaElevation" type="coordOrNullType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ExtraPointDesignElevations">  <!-- New for the v12.20 Survey Controller -->
    <xsd:annotation>
      <xsd:documentation>The ExtraPointDesignElevations element contains one or more ExtraPointDesignElevation elements 
                         have been entered by using the functionality available in the custom as staked deltas</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="1" maxOccurs="unbounded">
        <xsd:element ref="ExtraPointDesignElevation"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SideSlopeFromAlignmentHingeDefinition">
    <xsd:annotation>
      <xsd:documentation>The SideSlopeFromAlignmentHingeDefinition element contains the side slope definition for a
                         side slope staked out from an alignment</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Method" type="sideSlopeFromAlignMethodType"/>
        <xsd:element name="Offset" type="coordType"/>
        <xsd:element name="Grade" type="gradeType" minOccurs="0"/>           <!-- Present when Method = OffsetAndGrade -->
        <xsd:element name="VerticalDistance" type="distType" minOccurs="0"/> <!-- Present when Method = OffsetAndVerticalDistance -->
        <xsd:element name="Elevation" type="coordType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EquatedStation">  <!-- New for Version 12.20 Survey Controller -->
    <xsd:annotation>
      <xsd:documentation>The EquatedStation element supplies the equated station for a given true station (available in 
                         the Station element) and the equated station zone number</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Station" type="coordType"/>
        <xsd:element name="Zone" type="xsd:integer"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RoadDesign">
    <xsd:annotation>
      <xsd:documentation>The RoadDesign element contains the road definition details that apply to a staked road point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="StakeoutMethod" type="stakeoutMethodType"/>
        <xsd:element name="Station" type="coordType"/>
        <xsd:element ref="EquatedStation" minOccurs="0"/>                                            <!-- New for Version 12.20 Survey Controller - only present if equated stations are defined for the road -->
        <xsd:element name="Offset" type="coordOrNullType"/>
        <xsd:element name="Elevation" type="coordOrNullType"/>
        <xsd:element name="OriginalDesignElevation" type="coordOrNullType" minOccurs="0"/>           <!-- Present if the design elevation has been edited in Trimble Access prior to staking out -->
        <xsd:element name="HorizontalOffsetMethod" type="horizontalOffsetMethodType" minOccurs="0"/> <!-- Present if StakeoutMethod = StationOnString -->
        <xsd:element name="HorizontalConstructionOffsetApplied" type="constOffsetAppliedType"/>
        <xsd:element name="VerticalConstructionOffsetApplied" type="constVertOffsetAppliedType"/>    <!-- This element is new for the Survey Controller v11.30 -->
        <xsd:element name="HorizontalConstructionOffsetCalculated" type="booleanType"/>              <!-- New for V5.0 schema - indicates whether or not the horizontal construction offset is a calculated value -->
        <xsd:element name="HorizontalConstructionOffset" type="distOrNullType"/>
        <xsd:element name="VerticalConstructionOffset" type="distOrNullType"/>
        <xsd:element name="StationConstructionOffset" type="distOrNullType"/>
        <xsd:element name="CrossSlopeDelta" type="distOrNullType" minOccurs="0"/>                    <!-- Present if the computed cross slope delta elevation value is not null in Trimble Access - a cross slope value will only be computed if the user has defined a cross slope element as part of their staking out process. -->
        <xsd:element name="CrossSlopeOffset" type="distType" minOccurs="0"/>                         <!-- Present if the specified cross slope offset value is not null in Trimble Access - a cross slope offset value will only be output if the user has defined a cross slope with a non-zero offset as part of their staking out process. -->
        <xsd:element name="SubgradeDepth" type="distType" minOccurs="0"/>                            <!-- Present if the specified subgrade depth value is not null in Trimble Access - a subgrade depth value will only be computed if the user has defined a subgrade depth as part of their staking out process. -->
        <xsd:element name="StakingSubgradeIntersection" type="booleanType" minOccurs="0"/>           <!-- Present if the specified subgrade depth value is not null in Trimble Access and a subgrade intersection point has been staked - the value will be true. -->
        <xsd:element name="StationClass" type="stationClassType"/>
        <xsd:element name="SideSlopeGrade" type="gradeType" minOccurs="0"/>                          <!-- Present if the user is staking out a side slope position -->
        <xsd:element name="DitchOffset" type="distType" minOccurs="0"/>                              <!-- Present if the user is staking out a side slope point and the position is in cut -->
        <xsd:element name="NewOffsetForSideSlope" type="distType" minOccurs="0"/>                    <!-- Present if the user has defined that a side slope should start at a different position in the cross-section definition from that in the original definition (they have selected a new position for the hinge point). -->
        <xsd:element ref="SideSlopeFromAlignmentHingeDefinition" minOccurs="0"/>                     <!-- Present if the user is staking out a side slope position -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AlignmentDesign">
    <xsd:annotation>
      <xsd:documentation>The alignmentDesign element contains the alignment definition details that apply to a staked alignment point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="StakeoutMethod" type="stakeoutMethodType"/>
        <xsd:element name="Station" type="coordType"/>
        <xsd:element ref="EquatedStation" minOccurs="0"/>                                            <!-- Only present if equated station equations are defined for the alignment -->
        <xsd:element name="StationClass" type="stationClassType"/>
        <xsd:element name="Offset" type="coordOrNullType"/>
        <xsd:element name="Elevation" type="coordOrNullType"/>
        <xsd:element name="OriginalDesignElevation" type="coordOrNullType" minOccurs="0"/>           <!-- Present if the design elevation has been edited in Trimble Access prior to staking out -->
        <xsd:element name="HorizontalConstructionOffsetApplied" type="constOffsetAppliedType"/>      <!-- New in the AlignmentDesign element for V5.56 schema -->
        <xsd:element name="VerticalConstructionOffsetApplied" type="constVertOffsetAppliedType"/>    <!-- New in the AlignmentDesign element for V5.56 schema -->
        <xsd:element name="HorizontalConstructionOffsetCalculated" type="booleanType"/>              <!-- New in the AlignmentDesign element for V5.56 schema -->
        <xsd:element name="HorizontalConstructionOffset" type="distOrNullType"/>
        <xsd:element name="VerticalConstructionOffset" type="distOrNullType"/>
        <xsd:element name="StationConstructionOffset" type="distOrNullType"/>                        <!-- New in the AlignmentDesign element for V5.56 schema -->
        <xsd:element name="SideSlopeGrade" type="gradeType" minOccurs="0"/>                          <!-- New in the AlignmentDesign element for V5.56 schema - present if the user is staking out a side slope position -->
        <xsd:element name="DitchOffset" type="distType" minOccurs="0"/>                              <!-- New in the AlignmentDesign element for V5.56 schema - present if the user is staking out a side slope point and the position is in cut -->
        <xsd:element ref="SideSlopeFromAlignmentHingeDefinition" minOccurs="0"/>                     <!-- New in the AlignmentDesign element for V5.56 schema - present if the user is staking out a side slope position -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DTMDesign">
    <xsd:annotation>
      <xsd:documentation>The DTMDesign element contains the DTM name and specified vertical offset used for staked DTM points</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="nameType"/>
        <xsd:element name="VerticalOffset" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SlopeDesign">
    <xsd:annotation>
      <xsd:documentation>The SlopeDesign element provides the design left and right slope details</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SlopeLeft" type="leftRightSlopeDesignType"/>
        <xsd:element name="SlopeRight" type="leftRightSlopeDesignType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LinearDeltas">
    <xsd:annotation>
      <xsd:documentation>The LinearDeltas element reports the deltas from the as-staked position to the design position
                         as values in terms of the line/arc/road or alignment definition.  Note that if the original design 
                         elevation is edited during the stakeout process, the DeltaElevation value reported in the LinearDeltas 
                         element will still provide the delta elevation with respect to the line/arc/road or alignment as
                         defined. This means that the DeltaElevation value will not reflect the change made to the design
                         elevation as this has effectively become a point staking operation. The DeltaElevation element in the
                         GridDeltas element will provide the delta elevation to the edited design elevation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DeltaStation" type="distOrNullType"/>
        <xsd:element name="DeltaOffset" type="distOrNullType"/>
        <xsd:element name="DeltaElevation" type="distOrNullType"/>
        <xsd:element name="GradeToLine" type="gradeOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DTMDeltas">
    <xsd:annotation>
      <xsd:documentation>The DTMDeltas element reports the delta elevation from the measured elevation to the DTM elevation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DeltaElevation" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DeltasRelativeToDTM">
    <xsd:annotation>
      <xsd:documentation>The DeltasRelativeToDTM element reports the DTM name, vertical offset and delta elevation from the measured 
                         elevation to the DTM elevation. This element is output if the Stakeout Roads option has been used while a DTM 
                         is also available, and the 'Display cut/fill to DTM' option is set</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DTMName" type="nameType"/>
        <xsd:element name="VerticalOffset" type="distType"/>
        <xsd:element name="DeltaElevation" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SecondaryRoadDeltas">
    <xsd:annotation>
      <xsd:documentation>The SecondaryRoadDeltas element reports the construction deltas from the current GENIO road staking 
                         group to a selected a secondary group</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="RoadName" type="nameType"/>
        <xsd:element name="Code" type="xsd:string"/>  <!-- New for V5.2 of schema -->
        <xsd:element name="CalculatedHorizontalConstructionOffset" type="distType"/>
        <xsd:element name="DeltaElevation" type="distType"/>
        <xsd:element name="OffsetToSecondaryMasterAlignment" type="distType"/>
        <xsd:element name="Station" type="coordType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CatchPoint">
    <xsd:annotation>
      <xsd:documentation>The CatchPoint element reports the stakeout details for a staked catch point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Offset" type="coordType"/>
        <xsd:element name="Elevation" type="coordType"/>
        <xsd:element name="AsStakedSideSlopeGrade" type="gradeOrNullType"/>
        <xsd:element name="HorizontalDistanceToHingePoint" type="distType"/>
        <xsd:element name="VerticalDistanceToHingePoint" type="distType"/>
        <xsd:element name="SlopeDistanceToHingePoint" type="distType"/>
        <xsd:element name="SlopeDistanceToHingePointWithConstructionOffset" type="distOrNullType"/>
        <xsd:element name="HorizontalDistanceToCenterLine" type="distType" minOccurs="0"/> <!-- New for the v5.54 schema definition - for Survey Pro system -->
        <xsd:element name="VerticalDistanceToCenterLine" type="distType" minOccurs="0"/>   <!-- New for the v5.54 schema definition - for Survey Pro system -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CatchPointTemplateReport">
    <xsd:annotation>
      <xsd:documentation>The CatchPointTemplateReport element reports a cross-section element as part of a series of elements that
                         provide all the design cross-section elements from the catch point back to the alignment (centreline)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="HorizontalDistance" type="distType"/>
        <xsd:element name="DeltaElevation" type="distOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CenterPoint">
    <xsd:annotation>
      <xsd:documentation>The CenterPoint element provides the details for the alignment position in a CrossSectionReport element</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Code" type="xsd:string"/>
        <xsd:element name="Station" type="distType"/>
        <xsd:element ref="EquatedStation" minOccurs="0"/>     <!-- New for Version 12.20 Survey Controller - only present if equated stations are defined for the road -->
        <xsd:element name="RightAzimuth" type="angleType"/>
        <xsd:element name="ComputedGrid" type="gridPosType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CrossSectionReport">
    <xsd:annotation>
      <xsd:documentation>The CrossSectionReport element provides details of all the design points for a complete design 
                         cross-section at the station of the staked point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element ref="CenterPoint"/>
        <xsd:element name="LeftSide" type="crossSectionElementsType" minOccurs="0"/>
        <xsd:element name="RightSide" type="crossSectionElementsType" minOccurs="0"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="Stakeout">
    <xsd:annotation>
      <xsd:documentation>The Stakeout element is a collection of all the stakeout elements for a given point</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="PointDesign" minOccurs="0" maxOccurs="1"/>                          <!-- Present if the Stakeout Points option has been used for the point. -->
        <xsd:element ref="ExtraPointDesignElevations" minOccurs="0" maxOccurs="1"/>           <!-- Present if the functionality available in the custom as staked deltas system has been used to add extra design elevations -->
        <xsd:element name="LineDesign" type="lineArcDesignType" minOccurs="0" maxOccurs="1"/> <!-- Present if the Stakeout Lines option has been used for the point. -->
        <xsd:element name="ArcDesign" type="lineArcDesignType" minOccurs="0" maxOccurs="1"/>  <!-- Present if the Stakeout Arcs option has been used for the point. -->
        <xsd:element ref="RoadDesign" minOccurs="0" maxOccurs="1"/>                           <!-- Present if the Stakeout Roads option has been used for the point. -->
        <xsd:element ref="AlignmentDesign" minOccurs="0" maxOccurs="1"/>                      <!-- New for V5.1 of Schema - Present if the Stakeout Alignments option has been used for the point. -->
        <xsd:element ref="DTMDesign" minOccurs="0" maxOccurs="1"/>                            <!-- Present if the Stakeout DTMs option has been used for the point. -->
        <xsd:element ref="SlopeDesign" minOccurs="0" maxOccurs="1"/>                          <!-- Present if staking a slope from a line (StakeoutMethod = SlopeFromLine) -->
        <xsd:element ref="LinearDeltas" minOccurs="0" maxOccurs="1"/>                         <!-- Present if the Stakeout Lines, Stakeout Arcs or Stakeout Roads options have been used -->
        <xsd:element name="GridDeltas" type="gridDeltaType" minOccurs="0" maxOccurs="1"/>     <!-- Present if the 'Store grid deltas' option in the Stakeout options in Trimble Access is checked. -->
        <xsd:element name="PolarDeltas" type="polarType" minOccurs="0" maxOccurs="1"/>        <!-- Present if the 'Store grid deltas' option in the Stakeout options in Trimble Access not is checked. -->
        <xsd:element ref="DTMDeltas" minOccurs="0" maxOccurs="1"/>                            <!-- Present if the Stakeout DTMs option has been used -->
        <xsd:element ref="DeltasRelativeToDTM" minOccurs="0" maxOccurs="1"/>                  <!-- Present if the Stakeout Roads option has been used while a DTM is also available and the 'Display cut/fill to DTM' option is set. -->
        <xsd:element ref="SecondaryRoadDeltas" minOccurs="0" maxOccurs="1"/>                  <!-- Present if GENIO road staking is being used and the user has selected a secondary group for construction deltas reporting. -->
        <xsd:element ref="CatchPoint" minOccurs="0" maxOccurs="1"/>                           <!-- Present when a road/alignment catch point has been staked. -->
        <xsd:element ref="CatchPointTemplateReport" minOccurs="0" maxOccurs="unbounded"/>     <!-- Present when a road/alignment catch point has been staked - a record is output for each cross-section element back to the centreline. -->
        <xsd:element ref="CrossSectionReport" minOccurs="0" maxOccurs="1"/>                   <!-- Present if the Stakeout Roads option or Alignments stake Side slope from alignment option has been used for the point. -->
        <xsd:element ref="Notes" minOccurs="0"/>                                              <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="MeanTurnedAngleResiduals">
    <xsd:annotation>
      <xsd:documentation>The MeanTurnedAngleResiduals element records the residual details for a given observation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Round" type="xsd:integer"/>
        <xsd:element name="ObservationID" type="idType"/>
        <xsd:element name="PointName" type="nameType"/>
        <xsd:element name="HorizontalAngleResidual" type="deltaAngleOrNullType"/>
        <xsd:element name="VerticalAngleResidual" type="deltaAngleOrNullType"/>
        <xsd:element name="SlopeDistanceResidual" type="distOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RoundsMeanTurnedAngleResiduals">
    <xsd:annotation>
      <xsd:documentation>The RoundsMeanTurnedAngleResiduals element is a collection of MeanTurnedAngleResiduals for a given round</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="MeanTurnedAngleResiduals" maxOccurs="unbounded"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ListOfStations">
    <xsd:annotation>
      <xsd:documentation>The ListOfStations element lists all the stations selected for a traverse definition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="Item" type="itemType" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TraverseDefinitionRecord">
    <xsd:annotation>
      <xsd:documentation>The TraverseDefinitionRecord element records the traverse start and end details 
                         as well as the list of stations in the traverse</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TraverseName" type="nameType"/>
        <xsd:element name="Deleted" type="booleanType"/>
        <xsd:element name="BacksightPoint" type="nameType"/>
        <xsd:element name="BacksightAzimuth" type="angleType"/>
        <xsd:element name="BacksightAzimuthKeyedIn" type="booleanType"/>
        <xsd:element ref="ListOfStations"/>
        <xsd:element name="ForesightPoint" type="nameType"/>
        <xsd:element name="ForesightAzimuth" type="angleType"/>
        <xsd:element name="ForesightAzimuthKeyedIn" type="booleanType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TraverseAdjustmentRecord">
    <xsd:annotation>
      <xsd:documentation>The TraverseAdjustmentRecord element records the adjustment methods selected for traverse adjustment</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="AdjustmentMethod" type="adjustmentMethodType"/>
        <xsd:element name="AngularErrorDistribution" type="errorDistributionType"/>
        <xsd:element name="ElevationErrorDistribution" type="errorDistributionType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TraverseLegs">
    <xsd:annotation>
      <xsd:documentation>The TraverseLegs element lists all the adjusted traverse legs for an adjusted traverse</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="Leg" type="traverseLegType" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TraverseClosureRecord">
    <xsd:annotation>
      <xsd:documentation>The TraverseClosureRecord element records the result of a traverse adjustment, including the list of the adjusted traverse legs</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TraverseDefinitionID" type="idType"/>
        <xsd:element name="TraverseAdjustmentRecordID" type="idType"/>  <!-- New for the v4.0 schema definition (Survey Controller v11.30) -->
        <xsd:element name="ClosureStatus" type="closureStatusType"/>
        <xsd:element name="AzimuthMisclose" type="deltaAngleType"/>
        <xsd:element name="DistanceMisclose" type="distType"/>
        <xsd:element name="TraverseLength" type="distType"/>
        <xsd:element name="Precision" type="xsd:double"/>
        <xsd:element name="DeltaNorth" type="distType"/>
        <xsd:element name="DeltaEast" type="distType"/>
        <xsd:element name="DeltaElevation" type="distOrNullType"/>
        <xsd:element ref="TraverseLegs"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ListOfPoints">
    <xsd:annotation>
      <xsd:documentation>The ListOfPoints element lists all the points used in an area computation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="Item" type="itemType" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="AreaRecord">
    <xsd:annotation>
      <xsd:documentation>The AreaRecord element holds the result of an area computation, including the list of the points chosen for the computation</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Area" type="xsd:double"/>
        <xsd:element name="Perimeter" type="distType"/>
        <xsd:element name="DistanceType" type="distDisplayType"/>
        <xsd:element name="NumberOfPoints" type="xsd:integer"/>
        <xsd:element ref="ListOfPoints"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StartRoundsRecord">
    <xsd:annotation>
      <xsd:documentation>The StartRoundsRecord element indicates the start of a series of rounds of observations</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StartRoundRecord">
    <xsd:annotation>
      <xsd:documentation>The StartRoundRecord element indicates the start of a sequence of observations comprising a round and provides the round number</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Round" type="xsd:integer"/>
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EndRoundsRecord">
    <xsd:annotation>
      <xsd:documentation>The EndRoundsRecord element indicates the completion of a series of rounds of observations</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EndRoundRecord">
    <xsd:annotation>
      <xsd:documentation>The EndRoundRecord element indicates the end of a sequence of observations comprising a round</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ComputeAzimuthRecord">
    <xsd:annotation>
      <xsd:documentation>The ComputeAzimuthRecord element holds all the details related to an azimuth computed and stored in the job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="AzimuthMethod" type="computeAzimuthMethodType"/>
        <xsd:element name="Point1" type="nameType" minOccurs="0"/>                  <!-- Present if AzimuthMethod = BetweenTwoPoints | BisectedCorner -->
        <xsd:element name="Point2" type="nameType" minOccurs="0"/>                  <!-- Present if AzimuthMethod = BetweenTwoPoints | BisectedCorner -->
        <xsd:element name="Azimuth1" type="angleType" minOccurs="0"/>               <!-- Present if AzimuthMethod = BisectedAzimuths | AzimuthPlusTurnedAngle -->
        <xsd:element name="Azimuth2" type="angleType" minOccurs="0"/>               <!-- Present if AzimuthMethod = BisectedAzimuths -->
        <xsd:element name="CornerPoint" type="nameType" minOccurs="0"/>             <!-- Present if AzimuthMethod = BisectedCorner -->
        <xsd:element name="TurnedAngle" type="angleType" minOccurs="0"/>            <!-- Present if AzimuthMethod = AzimuthPlusTurnedAngle -->
        <xsd:element name="ReferenceLinearEntity" type="xsd:string" minOccurs="0"/> <!-- Present if AzimuthMethod = AzimuthToLineOffset -->
        <xsd:element name="Station" type="coordType" minOccurs="0"/>                <!-- Present if AzimuthMethod = AzimuthToLineOffset -->
        <xsd:element name="Offset" type="coordType" minOccurs="0"/>                 <!-- Present if AzimuthMethod = AzimuthToLineOffset -->
        <xsd:element name="ComputedAzimuth" type="angleType"/>
        <xsd:element name="ComputedAngle" type="angleType" minOccurs="0"/>          <!-- New for the v5.4 schema definition - Present if AzimuthMethod = BisectedAzimuths | BisectedCorner | AzimuthToLineOffset -->
        <xsd:element name="AzimuthType" type="azimuthType"/>
        <xsd:element ref="Notes" minOccurs="0"/>                                    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ComputeDistanceRecord">
    <xsd:annotation>
      <xsd:documentation>The ComputeDistanceRecord element holds all the details related to a distance computed and stored in the job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DistanceMethod" type="computeDistMethodType"/>
        <xsd:element name="Point1" type="nameType"/>
        <xsd:element name="Point2" type="nameType" minOccurs="0"/>                  <!-- Present if DistanceMethod = BetweenTwoPoints -->
        <xsd:element name="ReferenceLinearEntity" type="xsd:string" minOccurs="0"/> <!-- Present if DistanceMethod = BetweenPointAndLine | BetweenPointAndArc -->
        <xsd:element name="DistanceAlong" type="distOrNullType" minOccurs="0"/>     <!-- Present if DistanceMethod = BetweenPointAndLine | BetweenPointAndArc -->
        <xsd:element name="ComputedDistance" type="distType"/>
        <xsd:element name="DistanceType" type="distDisplayType"/>
        <xsd:element ref="Notes" minOccurs="0"/>                                    <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ComputeAngleRecord">  <!-- New for v5.0 schema -->
    <xsd:annotation>
      <xsd:documentation>The ComputeAngleRecord element records the points used and the angle computed</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Point1" type="nameType"/>
        <xsd:element name="Point2" type="nameType"/>
        <xsd:element name="Point3" type="nameType"/>
        <xsd:element name="ComputedAngle" type="angleType"/>
        <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="InverseRecord">
    <xsd:annotation>
      <xsd:documentation>The InverseRecord element holds all the details related to an inverse computed and stored in the job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="FromPoint" type="nameType"/>
        <xsd:element name="ToPoint" type="nameType"/>
        <xsd:element name="Azimuth" type="angleOrNullType"/>
        <xsd:element name="HorizontalDistance" type="distOrNullType"/>
        <xsd:element name="VerticalDistance" type="distOrNullType"/>
        <xsd:element name="SlopeDistance" type="distOrNullType"/>
        <xsd:element name="Grade" type="gradeOrNullType"/>
        <xsd:element name="AzimuthType" type="azimuthType"/>
        <xsd:element name="DistanceType" type="distDisplayType"/>
        <xsd:element ref="Notes" minOccurs="0"/>                         <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CogoTransformationRecord">
    <xsd:annotation>
      <xsd:documentation>The CogoTransformationRecord element records all the details related to cogo transformation carried out in a job.
                         The points that have been transformed using the parameters recorded in this element will preceed the element as 
                         a series of PointRecord elements with the Method CogoCalculated and Grid elements holding the transformed coordinates.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="NumberOfPointsTransformed" type="xsd:integer"/>
        <xsd:element name="Rotation" type="rotationDefnType" minOccurs="0"/>       <!-- Present if a rotation was defined in the transformation -->
        <xsd:element name="Scale" type="scaleDefnType" minOccurs="0"/>             <!-- Present if a scale factor was applied in the transformation -->
        <xsd:element name="Translation" type="translationDefnType" minOccurs="0"/> <!-- Present if a translation was defined in the transformation -->
        <xsd:element ref="Notes" minOccurs="0"/>                                   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ScanningRecord">  <!-- New for the V5.0 schema -->
    <xsd:annotation>
      <xsd:documentation>The ScanningRecord element records the details for a scan that has been carried out. It provides
                         references to the station setup used and the name of the file that the scanned point cloud is stored in.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="StationID" type="idType"/>
        <xsd:element name="BackBearingID" type="idType"/>
        <xsd:element name="Type" type="scanType"/>
        <xsd:element name="FileName" type="xsd:string"/>
        <xsd:element name="NumberOfPoints" type="xsd:integer"/>
        <xsd:element name="HorizontalStep" type="deltaAngleType"/>
        <xsd:element name="VerticalStep" type="deltaAngleType"/>
        <xsd:element ref="Notes" minOccurs="0"/>                         <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ImageRecord">  <!-- New for the V5.0 schema -->
    <xsd:annotation>
      <xsd:documentation>The ImageRecord element records the details for an image that has been recorded. It provides references to the
                         station setup at which the image was taken and the name of the image file that has been recorded.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="StationID" type="idOrNullType"/>     <!-- Reference to a StationRecord *OR* a PhotoStationRecord -->
        <xsd:element name="BackBearingID" type="idOrNullType"/>
        <xsd:element name="CameraID" type="idType"/>            <!-- Reference to a CameraRecord *OR* a CameraRecord2 -->
        <xsd:element name="PointRecordID" type="idOrNullType"/> <!-- Reference to Measured Point ID -->
        <xsd:element name="FileName" type="xsd:string"/>
        <xsd:element name="HorizontalAngle" type="angleOrNullType"/>  <!-- Sighting direction horizontal angle -->
        <xsd:element name="VerticalAngle" type="angleOrNullType"/>    <!-- Sighting direction vertical angle -->
        <xsd:element name="Width" type="xsd:integer"/>          <!-- Image width in pixels -->
        <xsd:element name="Height" type="xsd:integer"/>         <!-- Image height in pixels -->
        <xsd:element name="SourceX" type="xsd:integer"/>        <!-- Origin X of image in camera sensor array -->
        <xsd:element name="SourceY" type="xsd:integer"/>        <!-- Origin Y of image in camera sensor array -->
        <xsd:element name="SourceWidth" type="xsd:integer"/>    <!-- Width of image in camera sensor array -->
        <xsd:element name="SourceHeight" type="xsd:integer"/>   <!-- Height of image in camera sensor array -->
        <xsd:element name="ImageType" type="imageType"/>        <!-- New for V5.64 of schema.  Distinguishes HDR/non-HDR images -->
        <xsd:element ref="Notes" minOccurs="0"/>                <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="MediaFileRecord">  <!-- New for the V5.53 schema -->
    <xsd:annotation>
      <xsd:documentation>The MediaFileRecord element records the details for a media file that has been recorded. It provides references to the
                         record that the media file is associated with and the name of the media file that has been recorded.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>        
        <xsd:element name="RecordID" type="idOrNullType"/> <!-- Reference to Record ID -->
        <xsd:element name="FileName" type="xsd:string"/> <!-- Name of the media file -->
        <xsd:element ref="Notes" minOccurs="0"/>                <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CameraRecord">  <!-- New for the V5.0 schema -->
    <xsd:annotation>
      <xsd:documentation>The CameraRecord element records the details relating to the camera used for images</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="X0" type="xsd:double"/>             <!-- X coordinate of point where the sighting axis intersects the image plane -->
        <xsd:element name="Y0" type="xsd:double"/>             <!-- Y coordinate of point where the sighting axis intersects the image plane -->
        <xsd:element name="CK" type="xsd:double"/>             <!-- Distance from principle point to camera projection centre -->
        <xsd:element name="Kappa" type="deltaAngleType"/>      <!-- Rotation parameter of the image plane -->
        <xsd:element name="Phi" type="deltaAngleType"/>        <!-- Rotation parameter of the image plane -->
        <xsd:element name="Omega" type="deltaAngleType"/>      <!-- Rotation parameter of the image plane -->
        <xsd:element name="S0" type="xsd:double"/>             <!-- Distance component of vector from origin to camera projection centre -->
        <xsd:element name="C0" type="deltaAngleType"/>         <!-- Angle component of vector from origin to camera projection centre -->
        <xsd:element name="Z0" type="deltaAngleType"/>         <!-- Angle component of vector from origin to camera projection centre -->
        <xsd:element name="I" type="deltaAngleType"/>          <!-- Trunnion error -->
        <xsd:element name="K1" type="xsd:integer"/>            <!-- Distortion parameter -->
        <xsd:element name="P1" type="xsd:integer"/>            <!-- Distortion parameter -->
        <xsd:element name="P2" type="xsd:integer"/>            <!-- Distortion parameter -->
        <xsd:element name="PixelSizeX" type="xsd:double"/>     <!-- Physical pixel X dimension -->
        <xsd:element name="PixelSizeY" type="xsd:double"/>     <!-- Physical pixel Y dimension -->
        <xsd:element name="PixelCountX" type="xsd:integer"/>   <!-- Size of sensor array (in pixels) -->
        <xsd:element name="PixelCountY" type="xsd:integer"/>   <!-- Size of sensor array (in pixels) -->
        <xsd:element ref="Notes" minOccurs="0"/>               <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TunnelSettingsRecord">  <!-- New for the V5.1 schema -->
    <xsd:annotation>
      <xsd:documentation>The TunnelSettingsRecord element provides the measurement tolerances for the specified tunnel (by name)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice maxOccurs="unbounded">
        <xsd:element name="TunnelName" type="nameType"/>                      <!-- The name of the tunnel -->
        <xsd:element name="ScanInterval" type="xsd:double"/>                  <!-- The interval (or resolution) for the measurements taken at each tunnel profile -->
        <xsd:element name="ScanEDMTimeout" type="xsd:double"/>                <!-- Renamed in V5.2 schema -->  <!-- The timeout period in seconds set in the instrument for scanning operations on this tunnel -->
        <xsd:element name="ScanStationTolerance" type="doubleOrNullType"/>    <!-- Renamed in V5.2 schema -->  <!-- The specified station tolerance value (maximum difference between the nominal station and the measured station for each point), before a warning is issued -->
        <xsd:element name="ScanOverbreakTolerance" type="doubleOrNullType"/>  <!-- Renamed in V5.2 schema -->  <!-- The specified maximum distance, radial from the tunnel alignment, that the measured position can exceed the design position for each point, before a warning is issued -->
        <xsd:element name="ScanUnderbreakTolerance" type="doubleOrNullType"/> <!-- Renamed in V5.2 schema -->  <!-- The specified maximum distance, radial from the tunnel alignment, that the design position can exceed the measured position for each point, before a warning is issued -->
        <xsd:element name="ScanIterations" type="xsd:integer"/>               <!-- New for the V5.2 schema --> <!-- The specified maximum number of iterations that the system will attempt when scanning a position before the operation is abandoned -->
        <xsd:element name="SetoutTolerance" type="distType"/>                 <!-- New for the V5.2 schema --> <!-- The specified position tolerance allowed for a setout position -->
        <xsd:element name="SetoutIterations" type="xsd:integer"/>             <!-- New for the V5.2 schema --> <!-- The specified maximum number of iterations that the system will attempt when setting out a position before the operation is abandoned -->
        <xsd:element name="SetoutEDMTimeout" type="xsd:double"/>              <!-- New for the V5.2 schema --> <!-- The timeout period in seconds set in the instrument for setting out operations on this tunnel -->
        <xsd:element ref="Notes" minOccurs="0"/>                              <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TunnelPointDeltaRecord">    <!-- New for the V5.1 schema -->
    <xsd:annotation>
      <xsd:documentation>The TunnelPointDeltaRecord element provides the radial delta (undercut/overcut) for the tunnel point as well 
                         as its position as a horizontal and vertical offset from the tunnel alignment and the delta station value. 
                         The ObservationID provides a link to the actual observation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ObservationID" type="idType"/>                   <!-- Provides the ID of the observation (PointRecord) that these delta values relate to -->
        <xsd:element name="Delta" type="distOrNullType"/>                   <!-- This is the actual undercut/overcut value for the observed position - a -ve value indicates an undercut and a +ve value an overcut -->
        <xsd:element name="HorizontalOffset" type="distOrNullType"/>        <!-- The horizontal offset from the tunnel alignment to the measured position -->
        <xsd:element name="VerticalOffset" type="distOrNullType"/>          <!-- The vertical offset from the tunnel alignment to the measured position -->
        <xsd:element name="DeltaStation" type="distOrNullType"/>            <!-- The difference in station (-ve if before and +ve if after) from the nominal station to the station of the measured position -->
        <xsd:element name="RotatedHorizontalOffset" type="distOrNullType"/> <!-- The horizontal offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) --> <!-- New for the V5.51 schema -->
        <xsd:element name="RotatedVerticalOffset" type="distOrNullType"/>   <!-- The vertical offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) -->   <!-- New for the V5.51 schema -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TunnelCrossSectionRecord">  <!-- New for the V5.1 schema -->
    <xsd:annotation>
      <xsd:documentation>The TunnelCrossSectionRecord element provides the details of tunnel measurements carried out at a given station (tunnel
                         profile). The element holds a list of the TunnelPointDeltaRecord elements for the measured points.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice maxOccurs="unbounded">
        <xsd:element name="TunnelName" type="nameType"/>                      <!-- The name of the tunnel -->
        <xsd:element name="Station" type="xsd:double"/>                       <!-- The station of the measured tunnel profile -->
        <xsd:element ref="EquatedStation" minOccurs="0"/>                     <!-- New for the V5.52 schema - only present if equated stations are defined for the tunnel -->
        <xsd:element name="TemplateSurface" type="xsd:string"/>               <!-- New for the V5.58 schema - the currently active surface within the template that has been scanned. -->
        <xsd:element ref="TunnelPointDeltaRecord" maxOccurs="unbounded"/>     <!-- A list of all the tunnel deltas for this measured tunnel profile -->
        <xsd:element ref="Notes" minOccurs="0"/>                              <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TunnelSetoutPointRecord">    <!-- New for the V5.2 schema -->
    <xsd:annotation>
      <xsd:documentation>The TunnelSetoutPointRecord element for a tunnel setout point provides its position as a horizontal and vertical
                         offset from the tunnel alignment plus the delta station value. The ObservationID provides a link to the actual observation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ObservationID" type="idType"/>                   <!-- Provides the ID of the observation (PointRecord) that these delta values relate to -->
        <xsd:element name="HorizontalOffset" type="distOrNullType"/>        <!-- The horizontal offset from the tunnel alignment to the measured setout position -->
        <xsd:element name="VerticalOffset" type="distOrNullType"/>          <!-- The vertical offset from the tunnel alignment to the measured setout position -->
        <xsd:element name="DeltaStation" type="distOrNullType"/>            <!-- The difference in station (-ve if before and +ve if after) from the nominal station to the station of the measured setout position -->
        <xsd:element name="RotatedHorizontalOffset" type="distOrNullType"/> <!-- The horizontal offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) --> <!-- New for the V5.51 schema -->
        <xsd:element name="RotatedVerticalOffset" type="distOrNullType"/>   <!-- The vertical offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) -->   <!-- New for the V5.51 schema -->
        <xsd:element name="DeltaOffset" type="distOrNullType"/>             <!-- The computed projection distance of the measured position onto the setout intersection line --> <!-- New for the V5.51 schema -->
        <xsd:element name="DistanceFromVertex" type="distOrNullType"/>      <!-- The distance around the design profile from the projection of the tunnel alignment (rotated if rotation applied) onto the design profile (-ve if to the left and +ve if to the right) --> <!-- New for the V5.51 schema -->
        <xsd:element name="ProfileDistance" type="distOrNullType"/>         <!-- The distance around the design profile from the profile start point to the projected position of the measured point on the profile --> <!-- New for the V5.51 schema -->
        <xsd:element name="TemplateSurface" type="xsd:string"/>             <!-- New for the V5.58 schema - the surface specified for use with the setout point. -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TunnelSetoutPointsRecord">  <!-- New for the V5.2 schema -->
    <xsd:annotation>
      <xsd:documentation>The TunnelSetoutPointsRecord element provides the details of tunnel setout point measurements carried out at a given
                         station (tunnel profile). The element holds a list of TunnelSetoutPointRecord elements for the measured setout points.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice maxOccurs="unbounded">
        <xsd:element name="TunnelName" type="nameType"/>                      <!-- The name of the tunnel -->
        <xsd:element name="Station" type="xsd:double"/>                       <!-- The station of the measured tunnel setout points -->
        <xsd:element ref="EquatedStation" minOccurs="0"/>                     <!-- New for the V5.52 schema - only present if equated stations are defined for the tunnel -->
        <xsd:element ref="TunnelSetoutPointRecord" maxOccurs="unbounded"/>    <!-- A list of all the positions for this measured tunnel setout points -->
        <xsd:element ref="Notes" minOccurs="0"/>                              <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:choice>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PositionInTunnel">    <!-- New for the V5.4 schema -->
    <xsd:annotation>
      <xsd:documentation>The PositionInTunnel element provides the radial delta (undercut/overcut), horizontal 
                         and vertical offsets from the tunnel alignment, the station value and profile distance 
                         (distance around the design profile to the position) for points measured in a tunnel
                         using the Position option in the Tunnels module.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Delta" type="distOrNullType"/>                   <!-- This is the actual undercut/overcut value for the observed position - a -ve value indicates an undercut and a +ve value an overcut -->
        <xsd:element name="HorizontalOffset" type="distOrNullType"/>        <!-- The horizontal offset from the tunnel alignment to the measured position -->
        <xsd:element name="VerticalOffset" type="distOrNullType"/>          <!-- The vertical offset from the tunnel alignment to the measured position -->
        <xsd:element name="Station" type="distOrNullType"/>                 <!-- The computed station for the measured position -->
        <xsd:element ref="EquatedStation" minOccurs="0"/>                   <!-- New for the V5.52 schema - only present if equated stations are defined for the tunnel -->
        <xsd:element name="ProfileDistance" type="distOrNullType"/>         <!-- The distance around the design profile from the profile start point to the projected position of the measured point on the profile -->
        <xsd:element name="TunnelName" type="nameType"/>                    <!-- The name of the tunnel -->
        <xsd:element name="RotatedHorizontalOffset" type="distOrNullType"/> <!-- The horizontal offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) --> <!-- New for the V5.51 schema -->
        <xsd:element name="RotatedVerticalOffset" type="distOrNullType"/>   <!-- The vertical offset (in the rotation plane) from the location of the tunnel alignment after rotation to the measured position (will be null if there is no rotation) -->   <!-- New for the V5.51 schema -->
        <xsd:element name="DistanceFromVertex" type="distOrNullType"/>      <!-- The distance around the design profile from the projection of the tunnel alignment (rotated if rotation applied) onto the design profile (-ve if to the left and +ve if to the right) --> <!-- New for the V5.51 schema -->
        <xsd:element name="TemplateSurface" type="xsd:string"/>             <!-- New for the V5.58 schema - the currently active surface within the template that the position is reported with respect to. -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="NTRIPDataSourceRecord">  <!-- New for the V5.1 schema -->
    <xsd:annotation>
      <xsd:documentation>The NTRIPDataSourceRecord element provides the details of the selected NTRIP (Networked Transport of RTCM via Internet Protocol) source</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="MountPoint" type="xsd:string"/>  <!-- This is effectively the name of the RTCM data source -->
        <xsd:element name="DataFormat" type="xsd:string"/>  <!-- The format used for the received data stream -->
        <xsd:element name="Solution" type="xsd:string"/>    <!-- This indicates the type of solution provided - typically Single base or Network -->
        <xsd:element name="IPAdress" type="xsd:string"/>    <!-- The IP address of the server -->
        <xsd:element name="IPPort" type="xsd:integer"/>     <!-- The IP port used by the server -->
        <xsd:element ref="Notes" minOccurs="0"/>            <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ImportDataBeginRecord">                <!-- New for V5.2 of schema. -->
    <xsd:annotation>
      <xsd:documentation>The ImportDataBeginRecord element indicates the start of a series of imported coordinate system records or points or lines</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Source" type="xsd:string"/>                  <!-- File name for source of imported data -->
        <xsd:element ref="Notes" minOccurs="0"/>            <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ImportDataEndRecord">                <!-- New for V5.2 of schema. -->
    <xsd:annotation>
      <xsd:documentation>The ImportDataEndRecord element indicates the end of a series of imported coordinate system records or points or lines</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="NumberOfEntitiesImported" type="xsd:integer"/>   <!-- Number of point and line entities imported (Doesn't include count of coordinate system records) -->
        <xsd:element ref="Notes" minOccurs="0"/>            <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ComputeAreaRecord">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>This type of record holds the result of an area computation, including the list of entities defining the area</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="xsd:string"/>  <!-- Don't use nameType - the computed area may not be given a name -->
        <xsd:element name="DistanceType" type="distDisplayType"/>
        <xsd:element name="Area" type="xsd:double"/>
        <xsd:element name="Perimeter" type="distType"/>
        <xsd:element name="NumberOfEntities" type="xsd:integer"/>
        <xsd:element name="ListOfEntities" type="entityListType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SubdivideAreaRecord">  <!-- New for V5.2 of schema -->
    <xsd:annotation>
      <xsd:documentation>This type of record holds the result of an area computation, including the list of entities defining the area</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Name" type="xsd:string"/>  <!-- Don't use nameType - the computed area may not be given a name -->
        <xsd:element name="DistanceType" type="distDisplayType"/>
        <xsd:element name="Area" type="xsd:double"/>
        <xsd:element name="Perimeter" type="distType"/>
        <xsd:element name="NumberOfEntities" type="xsd:integer"/>
        <xsd:element name="ListOfEntities" type="entityListType"/>
        <xsd:element name="SubArea" type="xsd:double"/>
        <xsd:element name="SubPerimeter" type="distType"/>
        <xsd:element name="NumberOfSubEntities" type="xsd:integer"/>
        <xsd:element name="ListOfSubEntities" type="entityListType"/>
        <xsd:element name="CreatedPoints">
          <xsd:complexType> 
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
              <xsd:element name="Item" type="itemType" maxOccurs="unbounded"/>
            </xsd:choice>
          </xsd:complexType>
        </xsd:element>  
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TriangleSolutionRecord">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>This type of record holds the entered and calculated values for a triangle solution computation. 
                         The boolean Calculated attribute on each element indicates whether the value was calculated (true)
                         or entered (false) as part of the solution computation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Method" type="triangleSolutionMethodType"/>
        <xsd:element name="Sidea" type="calcEnteredDistanceType"/>
        <xsd:element name="Sideb" type="calcEnteredDistanceType"/>
        <xsd:element name="Sidec" type="calcEnteredDistanceType"/>
        <xsd:element name="AngleA" type="calcEnteredAngleType"/>
        <xsd:element name="AngleB" type="calcEnteredAngleType"/>
        <xsd:element name="AngleC" type="calcEnteredAngleType"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="LayoutData">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element contains the details of a layout calculation 
                         carried out as part of an arc solution computation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="LayoutMethod" type="arcLayoutMethodType"/>
        <xsd:element name="Station" type="distType"/>
        <xsd:element name="PCStation" type="distType"/>
        <xsd:element name="DeflectionAngle" type="deltaAngleType" minOccurs="0"/> <!-- Present if LayoutMethod = PCDeflection | PIDeflection -->
        <xsd:element name="ChordDistance" type="distType" minOccurs="0"/>         <!-- Present if LayoutMethod = PCDeflection | ChordOffset -->
        <xsd:element name="PreviousStation" type="distType" minOccurs="0"/>       <!-- Present if LayoutMethod = PCDeflection and the computation immediately follows a PCDeflection computation with a different station value -->
        <xsd:element name="ShortChord" type="distType" minOccurs="0"/>            <!-- Present if LayoutMethod = PCDeflection and the computation immediately follows a PCDeflection computation with a different station value -->
        <xsd:element name="PI-StationDistance" type="distType" minOccurs="0"/>    <!-- Present if LayoutMethod = PIDeflection -->
        <xsd:element name="TangentDistance" type="distType" minOccurs="0"/>       <!-- Present if LayoutMethod = TangentOffset -->
        <xsd:element name="TangentOffset" type="distType" minOccurs="0"/>         <!-- Present if LayoutMethod = TangentOffset -->
        <xsd:element name="ChordOffset" type="distType" minOccurs="0"/>           <!-- Present if LayoutMethod = ChordOffset -->
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ArcSolutionRecord">  <!-- New for V5.4 of schema -->
    <xsd:annotation>
      <xsd:documentation>This type of record holds the entered and calculated values for an arc solution computation. 
                         The boolean Calculated attribute the arc value elements indicates whether the value was 
                         calculated (true) or entered (false) as part of the solution computation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ParameterOneMethod" type="arcSolutionParamOneMethodType"/>
        <xsd:element name="ParameterTwoMethod" type="arcSolutionParamTwoMethodType"/>
        <xsd:element name="Radius" type="calcEnteredDistanceType"/>
        <xsd:element name="Delta" type="calcEnteredAngleType"/>
        <xsd:element name="DegreeArc" type="calcEnteredAngleType"/>
        <xsd:element name="DegreeChord" type="calcEnteredAngleType"/>
        <xsd:element name="ArcLength" type="calcEnteredDistanceType"/>
        <xsd:element name="ChordLength" type="calcEnteredDistanceType"/>
        <xsd:element name="TangentLength" type="calcEnteredDistanceType"/>
        <xsd:element name="ExternalDistance" type="calcEnteredDistanceType"/>
        <xsd:element name="MidOrdinateDistance" type="calcEnteredDistanceType"/>
        <xsd:element name="SegmentArea" type="areaType"/>
        <xsd:element name="SectorArea" type="areaType"/>
        <xsd:element name="FilletArea" type="areaType"/>
        <xsd:element ref="LayoutData" minOccurs="0"/>
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="VolumeRecord">  <!-- New for V5.52 of schema -->
    <xsd:annotation>
      <xsd:documentation>This type of record holds the entered and calculated values for a volume computation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="VolumeMethod" type="volumeComputationMethodType"/>
        <xsd:element name="SurfaceFileName" type="xsd:string"/>
        <xsd:element name="BaseSurfaceFileName" type="xsd:string" minOccurs="0"/> <!-- This element will only be present when the VolumeMethod in SurfaceToSurface -->
        <xsd:element name="ReferenceElevation" type="distOrNullType"/> <!-- This element will be null when VolumeMethod = SurfaceToSurface or VolumeMethod = StockpileDepression -->
        <xsd:element name="HaulBulkagePercentage" type="xsd:double"/>  <!-- The bulking percentage that may have been applied to cut volumes -->
        <xsd:element name="ShrinkagePercentage" type="xsd:double"/>    <!-- The shrinkage percentage that may have been applied to fill volumes -->
        <xsd:element name="VolumeResults" type="volumeResultsType"/>
        <xsd:element name="AdjustedVolumeResults" type="volumeResultsType" minOccurs="0"/> <!-- This element will only be present if the HaulBulkagePercentage and/or the ShrinkagePercentage is non-zero --> 
        <xsd:element name="CutArea" type="areaOrNullType"/>
        <xsd:element name="FillArea" type="areaOrNullType"/>
        <xsd:element name="SurfaceArea" type="areaOrNullType"/>              <!-- New for V5.57 of schema -->
        <xsd:element name="SurfaceDepth" type="areaOrNullType"/>             <!-- New for V5.57 of schema -->
        <xsd:element name="SurfaceAreaByDepthVolume" type="areaOrNullType"/> <!-- New for V5.57 of schema -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SBASOffsetRecord">  <!-- New for V5.55 of schema -->
    <xsd:annotation>
      <xsd:documentation>This Satellite Based Augmentation Service (SBAS) offset record holds the current offset relationship 
                         being applied between RTK positions and OmniSTAR positions. The offset relationship is determined by 
                         measuring the same position with both RTK and OmniSTAR methods. The offset relationship can be stored 
                         on the device and then used at a later stage. Once an offset relationship is selected for use the offset 
                         is applied in each OmniSTAR position recorded in the job following the selection of the offset 
                         relationship until a different offset relationship is selected.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="BasePoint" type="nameType"/>         <!-- The name of the RTK base point being used at the time the offset relationship was determined -->
        <xsd:element name="RoverPoint" type="nameType"/>        <!-- The name of the RTK rover point at which the offset relationship was determined -->
        <xsd:element name="Date" type="timeStampType"/>         <!-- The time at which the offset relationship was determined -->
        <xsd:element name="ObservationNote" type="xsd:string"/> <!-- User entered text that can be associated with the offset record -->
        <xsd:element name="SigmaEast" type="distType"/>         <!-- The standard error in the east direction for the computed offset -->
        <xsd:element name="SigmaNorth" type="distType"/>        <!-- The standard error in the north direction for the computed offset -->
        <xsd:element name="SigmaElevation" type="distType"/>    <!-- The vertical standard error for the computed offset -->
        <xsd:element name="DeltaLatitude" type="deltaAngleType"/>  <!-- The latitude component of the offset relationship -->
        <xsd:element name="DeltaLongitude" type="deltaAngleType"/> <!-- The longitude component of the offset relationship -->
        <xsd:element name="DeltaHeight" type="distType"/>          <!-- The height component of the offset relationship -->
        <xsd:element ref="Notes" minOccurs="0"/>   <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TiltSensorCalibrationRecord">  <!-- New for V5.56 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element records details of when the last calibration was
                         carried out and how long remains until the calibration expires</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="SerialNumber" type="xsd:string"/>                    <!-- The device serial number -->
        <xsd:element name="TiltEvent" type="tiltEventType"/>                    <!-- A flag that indicates why this TiltSensorCalibrationRecord has been recorded. -->
        <xsd:element name="CalibrationTimeRemaining" type="doubleOrNullType"/>  <!-- The time remaining (in seconds) until the tilt sensor calibration expires. -->
        <xsd:element name="CalibrationTimeoutPeriod" type="xsd:double"/>        <!-- The total period (in seconds) that the tilt sensor calibration is valid for. -->
        <xsd:element name="SensorStatus" type="tiltSensorStatusType"/>          <!-- Indicates the current state of the tilt sensor. -->
        <xsd:element name="CalibrationTime" type="GPSTimeType"/>                <!-- The GPS time at which the current tilt calibration was carried out - if the TiltEvent is Calibration then it will be the time of this calibration -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="CustomTypeRecord">  <!-- New for V5.57 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element records details of a custom element type that has been
                         defined and used in a job file to record data specific to a SCAPI 
                         application program running on the Survey Core system.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TypeName" type="xsd:string"/>                    <!-- A unique name assigned by the SCAPI application to identify the custom record type. -->
        <xsd:element name="TypeVersion" type="xsd:integer"/>                <!-- An integer value assigned by the SCAPI application to identify the custom record version. -->
        <xsd:element name="Guid" type="xsd:string"/>                        <!-- The GUID relating to the custom data - typically (and by default) the GUID of the SCAPI application. -->
        <xsd:element name="Reviewable" type="booleanType"/>                 <!-- A flag indicating whether or not this type of custom record is reviewable within the system. -->
        <xsd:element name="Creator" type="xsd:string"/>                     <!-- Provides the name of the SCAPI application that has defined and uses this custom record type. -->
        <xsd:element name="EntityKeyPath" type="xsd:string" minOccurs="0"/> <!-- Provides the full path to the element or attribute that contains the record's key - used for CustomEntityRecord elements only and identifies element used for searching. -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="CustomXmlRecord">  <!-- New for V5.57 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element is used to hold custom elements that have been
                         appended to the job file in chronological sequence by a SCAPI 
                         application program running on the Survey Core system.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="CustomTypeRecordId" type="idType"/> <!-- An ID referencing the CustomTypeRecord that defines the record type anme and version -->
        <xsd:element name="TypeVersion" type="xsd:integer"/>   <!-- An integer value assigned by the SCAPI application to identify the custom record version. -->
        <xsd:element name="Creator" type="xsd:string"/>        <!-- The name of the SCAPI application that wrote this custom record -->  
        <xsd:element ref="CustomContent"/>                     <!-- The actual XML record structure containing the custom data written by the SCAPI application. -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="CustomEntityRecord">  <!-- New for V5.57 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element is used to hold custom elements that have been added to the 
                         job file by a SCAPI application program running on the Survey Core system.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="CustomTypeRecordId" type="idType"/> <!-- An ID referencing the CustomTypeRecord that defines the record type anme and version -->
        <xsd:element name="TypeVersion" type="xsd:integer"/>   <!-- An integer value assigned by the SCAPI application to identify the custom record version. -->
        <xsd:element name="Creator" type="xsd:string"/>        <!-- The name of the SCAPI application that wrote this custom record -->  
        <xsd:element name="Deleted" type="booleanType"/>
        <xsd:element name="KeyValue" type="xsd:string"/>       <!-- A unique name (or key) that the SCAPI application that created this custom record used for fast record searching and retrieval. -->  
        <xsd:element ref="CustomContent"/>                     <!-- The actual XML record structure containing the custom data written by the SCAPI application. -->
        <xsd:element ref="Features" minOccurs="0"/>            <!-- Present when Feature and Attribute details have been assigned to the custome entity when it was recorded -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="PhotoInstrumentRecord">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>The PhotoInstrumentRecord element provides details on the instrument being used for the following observations</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="Type" type="instrumentType"/>
        <xsd:element name="Model" type="xsd:string" minOccurs="0"/>
        <xsd:element name="Serial" type="xsd:string" minOccurs="0"/>
        <xsd:element name="FirmwareVersion" type="xsd:string" minOccurs="0"/>
        <xsd:element name="UserDefinedName" type="xsd:string" minOccurs="0"/>
        <xsd:element ref="Notes" minOccurs="0"/>
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DeviceAxisStandardDeviation">
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="Vector3D" type ="vector3DType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DeviceAxisOrientationData">  <!-- New for V5.6 of schema -->
    <xsd:complexType>
      <xsd:choice>
        <xsd:element name="PointRecordID" type="idType"/>
          <xsd:sequence>
              <xsd:element name="DeviceAxisOrientation" type="deviceAxisOrientationType"/>
              <xsd:element ref="DeviceAxisStandardDeviation" minOccurs="0"/>
          </xsd:sequence>
      </xsd:choice>
    </xsd:complexType>          
  </xsd:element>

  <xsd:element name="PhotoStationRecord">
    <xsd:annotation>
      <xsd:documentation>
        The PhotoStationRecord element provides details of the photogrammetry station setup. The InstrumentID
        provides a link to the appropriate PhotoInstrumentRecord. The AtmosphereID provides a link to the appropriate
        AtmosphereRecord. RawInstrumentHeight is a measurement from the  ground mark to the reference mark/location on
        the instrument regardless of how the instrument is tilted. I.e. the word height is misleading but is the best
        word we have at the moment. 
        The DeviceAxisOrientationData element holds either an ID reference to a PointRecord that provides has sensor values 
        available in it or the sensor values from the instrument stored in a DeviceAxisOrientation biVector which is 
        defined as follows:
        The XX,YY and ZZ terms form a vector that defines the axis of rotation and the magnitude of the vector = the rotation
        angle in radians.

        Simple case: when the photogrammetry instrument is stand-alone and a measurement is made, a PhotoStationRecord
        is created with a DeviceAxisOrientationData element containing a DeviceAxisOrientation element describing how the 
        instrument is tilted and oriented.
        Complex case: when the instrument is combined with something like a prism or RTK receiver, two things happen at
        the same time when a measurement is made: 1) a PointRecord is created with the appropriate Method (pointCreationType)
        along with a DeviceAxisOrientation which is needed to compute the position of the ground point. 2) a PhotoStationRecord
        is created from which photogrammetry observations may eventually flow. To compute accurately, this station needs to
        use the same DeviceAxisOrientation parameters that are stored in the PointRecord of #1 above. Rather than copying the 
        same values to both records, it's wiser to have just the one DeviceAxisOrientation element in the PointRecord and make the
        PhotoStationRecord reference it via the PointRecordID in the DeviceAxisOrientationData element. No where is the utility of 
        this linkage more apparent than when photogrammetry techniques are used to refine the tilt and orientation of the 
        instrument; the new resulting tilt and compass vector can now update the ground position calculations of #1.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="StationName" type="nameType"/>
        <xsd:element name="InstrumentHeight" type="distOrNullType"/> <!--Reduced value-->
        <xsd:element name="RawInstrumentHeight" type="rawInstTargetHeightType"/>
        <xsd:element name="InstrumentID" type="idOrNullType"/>
        <xsd:element name="AtmosphereID" type="idOrNullType"/>
        <xsd:element name="StationType" type="photoStationType"/>
        <xsd:element ref="DeviceAxisOrientationData"/>
          
        <xsd:element ref="Notes" minOccurs="0"/>
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>
    
  <xsd:element name="CameraDesignRecord">  <!-- New for V5.6 of schema -->
    <xsd:complexType>
      <xsd:annotation>
        <xsd:documentation>
          Defines the design parameters of a camera which are common for all cameras of the same model. The design
          expresses the detector in terms of the size of the detector in pixels and the size of each pixel in meters
          It expresses the lens in terms of the lens model and nominal focal length.
          Because some instruments may use multiple cameras, there may only need to be one record of this type.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:all>
        <xsd:element name="Type" type="xsd:string" />
        <xsd:element name="HeightPixels" type="xsd:unsignedInt" />
        <xsd:element name="WidthPixels" type="xsd:unsignedInt" />
        <xsd:element name="PixelSize" type="xsd:double" />
        <xsd:element name="LensModel" type="lensModelType" />
        <xsd:element name="NominalFocalLength" minOccurs="0" type="xsd:double" />
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required" />
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CameraRecord2">  <!-- New for V5.6 of schema -->
    <xsd:annotation>
      <xsd:documentation>
        Defines internal camera calibration parameters for the detector, lens type, lens distortion parameters, the principal point
        of autocollimation, the principal distance, and the external calibration consisting of translation and rotation vectors
        of the lens with respectto the instrument's frame of reference. Note that there is no rotation specified between the
        lens and the detector because it would be redundant with the external rotation.  This is more general than the
        old CameraRecord which was used by the VX.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="CameraDesignID" type="idType" />
        <xsd:element name="CameraPosition" type="xsd:string" />
        <!-- Simple way for the operator to know which camera is which -->
        <xsd:element name="Optics">
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="IdealAngularMagnification" type="xsd:double"/>
              <!-- Typically 1.0 -->
              <xsd:element name="AngleSymmetricDistortion">
                <xsd:annotation>
                  <xsd:documentation>These are coefficients of a linearization polynomial.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:all>
                    <xsd:element name="Order3" type="xsd:double" />
                    <xsd:element name="Order5" type="xsd:double" />
                    <xsd:element name="Order7" type="xsd:double" />
                    <xsd:element name="Order9" type="xsd:double" />
                  </xsd:all>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="AngleDecenteringDistortion">
                <xsd:annotation>
                  <xsd:documentation>These two coefficients of a BiVector angle (row axis rotation, column axis rotation, 0).</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:all>
                    <xsd:element name="Column" type="xsd:double" />
                    <xsd:element name="Row" type="xsd:double" />
                  </xsd:all>
                </xsd:complexType>
              </xsd:element>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Geometry">
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="PerspectiveCenterPixels">
                <xsd:annotation>
                  <xsd:documentation>
                    The combination of the principal point of auto collimation and the principal distance, these values
                    represent the translation from the 0,0 pixel on the detector to the lens exit pupil in units of pixels.
                  </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:all>
                    <xsd:element name="PrincipalPointColumn" type="xsd:double" />
                    <xsd:element name="PrincipalPointRow" type="xsd:double" />
                    <xsd:element name="PrincipalDistance" type="xsd:double" />
                  </xsd:all>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="VectorOffset" type="vector3DType">
                <xsd:annotation>
                  <xsd:documentation>
                    External calibration: Translation vector describing the lens position in the instrument's frame of reference.
                    Used to perform rigid body transformations between the optics (lens) and instruments frame of reference. When
                    there will be a horizontal and vertical circle readings, the Y-axis is assumed to represent the look direction
                    of the instrument before applying the circle readings.
                  </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="BiVectorAngle" type="biVector3DType">
                <xsd:annotation>
                  <xsd:documentation>
                    External calibration: Rotation biVector describing the lens rotation in the instrument's frame of reference.
                    Used to perform rigid body transformations between the optics (lens) and instruments frame of reference. When
                    there will be a horizontal and vertical circle readings, the Y-axis is assumed to represent the look direction
                    of the instrument before applying the circle readings.
                  </xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="Covariance">
                <xsd:complexType>
                  <xsd:all>
                    <xsd:element name="VectorOffset" type="symmetrical3DMatrixType" />
                    <xsd:element name="BiVectorAngle" type="symmetrical3DMatrixType" />
                    <xsd:element name="OffsetAngleCrossVariance" type="full3DMatrixType" />
                  </xsd:all>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" name="LookDirection" type="vector3DType" />
              <xsd:element minOccurs="0" name="RollDirection" type="vector3DType" />
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element ref="Notes" minOccurs="0"/>
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FlightMissionRecord">  <!-- New for V5.6 of schema -->
    <xsd:complexType>
      <xsd:annotation>
        <xsd:documentation>
          A flight mission represents a snapshot of a given site at a given time as an interconnected series of images
          along with their camera positions and orientations. It is intended to be adjusted for photogrammetry as a single unit.
          One single mission may be comprised of multiple flight blocks flown by one or multiple planes to cover complex areas.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
        <xsd:element name="Name" type="nameType" minOccurs="1" maxOccurs="1" />   <!-- Name is no longer optional in v5.63 -->
        <xsd:element name ="Boundary" minOccurs="0" maxOccurs="1">                <!-- New for v5.63 -->
          <xsd:annotation>
            <xsd:documentation>The boundary represents the original mission perimeter as defined by the user during the planning phase.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:sequence minOccurs="3" maxOccurs="unbounded">
                <xsd:element name ="Node">
                  <xsd:complexType>
                    <xsd:annotation>
                      <xsd:documentation>Latitude and longitude coordinates</xsd:documentation>
                    </xsd:annotation>
                    <xsd:attribute name="latitude" type="latitudeType" use="required"/>
                    <xsd:attribute name="longitude" type="longitudeType" use="required"/>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="FlightBlock" type="flightBlockType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="ID" type="idType" use="required" />
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="GNSSTiltCompensatedPointSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The GNSSTiltCompensatedPointSettings element provides the settings details for
                         GNSS tilt compensated points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="NumberOfMeasurements" type="xsd:integer"/>
              <xsd:element name="OccupationTime" type="xsd:integer"/>           <!-- In seconds -->
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="AutoStorePoint" type="booleanType"/>  <!-- New for the V5.63 schema -->
              <xsd:element name="AutoPrecisions" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="PointOptions">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="PointNameIncrement" type="xsd:integer"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tilt">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="TiltWarnings" type="booleanType"/>
              <xsd:element name="AutoAbandon" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TopoPointSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The TopoPointSettings element provides the settings details for
                         topo points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="NumberOfMeasurements" type="xsd:integer"/>
              <xsd:element name="OccupationTime" type="xsd:integer"/>           <!-- In seconds -->
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="AutoStorePoint" type="booleanType"/>  <!-- New for the V5.63 schema -->
              <xsd:element name="AutoPrecisions" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="PointOptions">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="PointNameIncrement" type="xsd:integer"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tilt">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="TiltWarnings" type="booleanType"/>
              <xsd:element name="TiltLimit" type="distOrNullType"/>
              <xsd:element name="AutoAbandon" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RTKControlSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The RTKControlSettings element provides the settings details for
                         observed control points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="NumberOfMeasurements" type="xsd:integer"/>
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="AutoStorePoint" type="booleanType"/>  <!-- New for the V5.63 schema -->
              <xsd:element name="AutoPrecisions" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tilt">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="TiltWarnings" type="booleanType"/>
              <xsd:element name="TiltLimit" type="distOrNullType"/>
              <xsd:element name="AutoAbandon" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="QuickMarkSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The QuickMarkSettings element provides the settings details for
                         rapid points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="AutoPrecisions" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tilt">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="TiltWarnings" type="booleanType"/>
              <xsd:element name="TiltLimit" type="distOrNullType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ContinuousPointsSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The ContinuousPointsSettings element provides the settings details for
                         GNSS continuous points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="AutoPrecisions" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Tilt">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="TiltWarnings" type="booleanType"/>
              <xsd:element name="TiltLimit" type="distOrNullType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StakeOutSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The StakeOutSettings element provides the settings details for
                         stakeout points from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="DistanceType" type="distanceDisplayType"/>
        <xsd:element name="DisplayPointType" type="coordinateType"/>
        <xsd:element name="Measurement">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="OccupationTime" type="xsd:integer"/>           <!-- In seconds -->
              <xsd:element name="NumberOfMeasurements" type="xsd:integer"/>
              <xsd:element name="HorizontalPrecision" type="distOrNullType"/>
              <xsd:element name="VerticalPrecision" type="distOrNullType"/>
              <xsd:element name="Tolerance">
                <xsd:complexType> 
                  <xsd:all>
                    <xsd:element name="HorizontalDistance" type="distOrNullType"/>
                    <xsd:element name="VerticalDistance" type="distOrNullType"/>
                    <xsd:element name="HorizontalAngle" type="angleOrNullType"/>
                    <xsd:element name="VerticalAngle" type="angleOrNullType"/>
                    <xsd:element name="SlopeDistance" type="distOrNullType"/>
                  </xsd:all>
                </xsd:complexType>
              </xsd:element>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Display">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="PointLabels" type="stakeoutPointLabelsType"/>
              <xsd:element name="OrientTo" type="stakeoutOrientToType"/>
              <xsd:element name="GridDeltas" type="booleanType"/>
              <xsd:element name="GNSSDeltas" type="coordinateType"/>
              <xsd:element name="TotalStationDeltas" type="stakeoutTotalStationDeltasType"/>
              <xsd:element name="TotalStationAngleDeltaTolerance" type="angleOrNullType"/>
              <xsd:element name="TotalStationDistanceDeltaTolerance" type="distOrNullType"/>
              <xsd:element name="MapMode" type="stakeoutMapModeType"/>
              <xsd:element name="TotalStationViewMode" type="stakeoutTotalStationViewModeType"/>
              <xsd:element name="DoToleranceCheck" type="booleanType"/>
              <xsd:element name="UseCompass" type="booleanType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SiteCalibrationSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The CalibrationSettings element provides the settings details for
                         automated GNSS calibration from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="FixHorizontalScaleToOne" type="booleanType"/>
        <xsd:element name="AutomaticallyCalibrate" type="booleanType"/>
        <xsd:element name="VerticalAdjustmentMethod" type="verticalAdjustmentType"/>
        <xsd:element name="CalibrationObservationType" type="calibrationObservationType"/>
        <xsd:element name="Limits">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="MaximumHorizontalResiduals" type="distOrNullType"/>
              <xsd:element name="MaximumVerticalResiduals" type="distOrNullType"/>
              <xsd:element name="MaximumHorizontalScale" type="xsd:double"/>
              <xsd:element name="MinimumHorizontalScale" type="xsd:double"/>
              <xsd:element name="MaximumVerticalIncline" type="xsd:double"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="DuplicatePointToleranceSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The DuplicatePointToleranceSettings element provides the settings details for
                         handling duplicate point comparisons from the survey style.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="HorizontalDistance" type="distOrNullType"/>
        <xsd:element name="VerticalDistance" type="distOrNullType"/>
        <xsd:element name="HorizontalAngular" type="angleOrNullType"/>
        <xsd:element name="VerticalAngular" type="angleOrNullType"/>
        <xsd:element name="F1F2HorizontalAngle" type="angleOrNullType"/>
        <xsd:element name="F1F2VerticalAngle" type="angleOrNullType"/>
        <xsd:element name="F1F2SlopeDistance" type="distOrNullType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TotalStationSettings">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The TotalStationSettings element provides the details 
                         specific to total station instruments.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="ViewBeforeStorage" type="booleanType"/>
        <xsd:element name="PointNameIncrement" type="xsd:integer"/>
        <xsd:element name="PointDisplay" type="coordinateType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="IntegratedSurveyStyle">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The IntegratedSurveyStyle element provides the details linking
                         the referenced GNSS and conventional survey styles.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="IntegratedStyle" type="booleanType"/>
        <xsd:element name="PreciseElevation" type="booleanType"/>
        <xsd:element name="Dependencies">
          <xsd:complexType> 
            <xsd:all>
              <xsd:element name="GNSSStyleName" type="nameType"/>
              <xsd:element name="TSStyleName" type="nameType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="IntegratedSurveyTarget">    <!-- New for the V5.61 schema -->
    <xsd:annotation>
      <xsd:documentation>The IntegratedSurveyTarget element provides the measured vertical
                         offset between the center of the prism and the antenna phase center.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="OffsetFromCenterOfPrismToAntenna" type="distType"/>
      </xsd:all>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SurveyStyleRecord">  <!-- New for V5.61 of schema -->
    <xsd:complexType>
      <xsd:annotation>
        <xsd:documentation>The SurveyStyleRecord is designed to provide specific survey style data when requested on an
                           individual survey style database record by a Plug In Survey Core application. This means that
                           this record type will never be seen in a full job export to a JobXML file.</xsd:documentation>
      </xsd:annotation>
      <xsd:all>
        <xsd:element name="SurveyStyle">
          <xsd:complexType>
            <xsd:all>
              <xsd:element name="Name" type="nameType"/>
              <xsd:element name="DeviceType" type="surveyStyleDeviceType"/>
            </xsd:all>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RoverSurveyOptions" type="fullSurveyOptionsType" minOccurs="0"/>
        <xsd:element name="BaseSurveyOptions" type="fullSurveyOptionsType" minOccurs="0"/>
        <xsd:element ref="GNSSTiltCompensatedPointSettings" minOccurs="0"/>
        <xsd:element ref="TopoPointSettings" minOccurs="0"/>
        <xsd:element ref="RTKControlSettings" minOccurs="0"/>
        <xsd:element name="QuickMarkSettings" type="quickMarkAndContinuousSettingsType" minOccurs="0"/>
        <xsd:element name="ContinuousPointsSettings" type="quickMarkAndContinuousSettingsType" minOccurs="0"/>
        <xsd:element ref="StakeOutSettings" minOccurs="0"/>
        <xsd:element ref="SiteCalibrationSettings" minOccurs="0"/>
        <xsd:element ref="DuplicatePointToleranceSettings" minOccurs="0"/>
        <xsd:element name="Instrument" type="instrumentRecordType" minOccurs="0"/>
        <xsd:element ref="TotalStationSettings" minOccurs="0"/>
        <xsd:element ref="IntegratedSurveyStyle" minOccurs="0"/>
        <xsd:element ref="IntegratedSurveyTarget" minOccurs="0"/>
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required" />
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="MagneticSensorCalibrationRecord">  <!-- New for V5.61 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element is used to record the magnetic sensor calibration 
                         status of a of a GNSSReceiver or V10 Imaging Rover</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="Device" type="magneticCalibrationDeviceType"/>           <!-- Indicates the device type that has been calibrated. -->
        <xsd:element name="SerialNumber" type="xsd:string"/>                        <!-- The device serial number -->
        <xsd:element name="CalibrationType" type="magneticSensorCalibDimensionType" minOccurs="0"/> <!-- Indicates whether thecalibration was 2D or 3D. -->
        <xsd:element name="Event" type="tiltEventType"/>                            <!-- A flag that indicates why this MagneticSensorCalibrationRecord has been recorded. -->
        <xsd:element name="SensorStatus" type="tiltSensorStatusType"/>              <!-- Indicates the current state of the magnetic sensor. -->
        <xsd:element name="HardIronOffsetX" type="doubleOrNullType" minOccurs="0"/> <!-- Correction applied (in milliGauss) to the magnetometer to allow for fixed ferrous effects in unit -->
        <xsd:element name="HardIronOffsetY" type="doubleOrNullType" minOccurs="0"/> <!-- Correction applied (in milliGauss) to the magnetometer to allow for fixed ferrous effects in unit -->
        <xsd:element name="HardIronOffsetZ" type="doubleOrNullType" minOccurs="0"/> <!-- Correction applied (in milliGauss) to the magnetometer to allow for fixed ferrous effects in unit -->
        <xsd:element name="CalibrationTime" type="GPSTimeType" minOccurs="0"/>      <!-- The GPS time at which the current calibration was carried out - if the Event is Calibration then it will be the time of this calibration -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="CalibrationCheckRecord">  <!-- New for V5.62 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element is used to record the details for a V10 Imaging Rover
                         camera calibration check operation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="InstrumentHeight" type="distOrNullType"/>          <!-- Reduced value - should be close to 1.45m -->
        <xsd:element name="RawInstrumentHeight" type="rawInstTargetHeightType"/>
        <xsd:element name="InstrumentID" type="idType"/>
        <xsd:element name="DistanceToWall" type="distOrNullType"/>            <!-- Measured distance from the instrument setup position to the wall on which the wall target has been placed - should be close to 2m -->
        <xsd:element name="FloorTargetDistanceToWall" type="distOrNullType"/> <!-- Measured distance from the floor target to the wall on which the wall target has been placed - should be close to 0.4m -->
        <xsd:element name="WallTargetHeight" type="distOrNullType"/>          <!-- Measured height of the wall target above the floor - should be close to 1.45m (level with instrument) -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="ImagePairRecord">  <!-- New for V5.62 of schema -->
    <xsd:annotation>
      <xsd:documentation>This element is used to record the matching pairs of images used 
                         in a V10 Imaging Rover camera calibration check operation.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="CalibrationCheckID" type="idType"/>                <!-- References the CalibrationCheckRecord this record 'belongs to' -->
        <xsd:element name="LeftImage" type="calibrationImageDetailsType"/>    <!-- Details for the left image of the pair -->
        <xsd:element name="RightImage" type="calibrationImageDetailsType"/>   <!-- Details for the left image of the pair -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType> 
  </xsd:element>

  <xsd:element name="PlaneResiduals">  <!-- New for the V5.62 schema -->
    <xsd:annotation>
      <xsd:documentation>The PlaneResiduals element holds all the PlaneResidual elements</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="PlaneResidual" type="planeResidualType" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="PlaneDefinitionRecord">  <!-- New for the V5.62 schema -->
    <xsd:annotation>
      <xsd:documentation>The PlaneDefinitionRecord element provides the parameters for a plane definition based on
                         selected points and includes a list of the residuals to the points used in defining the plane.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="PlaneA" type="xsd:double"/>                      <!-- The plane parameter A -->
        <xsd:element name="PlaneB" type="xsd:double"/>                      <!-- The plane parameter B -->
        <xsd:element name="PlaneC" type="xsd:double"/>                      <!-- The plane parameter C -->
        <xsd:element name="PlaneD" type="xsd:double"/>                      <!-- The plane parameter D -->
        <xsd:element ref="PlaneResiduals"/>
        <xsd:element ref="Notes" minOccurs="0"/>                            <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="RTXRTKOffsetRecord">  <!-- New for the V5.64 schema -->
    <xsd:annotation>
      <xsd:documentation>The RTXRTKOffsetRecord element provides parameters to transform RTXECEF coordinates into the local RTK reference frame.
	                     The translations are used to produce the coordinates included in the Reductions element.
                         The sigmas are combined with the RTX measurement precisions to produce precision estimates for the transformed RTX coordinates. </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType> 
      <xsd:all>
        <xsd:element name="TranslationX" type="distType"/>                  <!-- Cartesian X axis translation applied to RTXECEF X coordinate to transform to RTK reference frame -->
        <xsd:element name="TranslationY" type="distType"/>                  <!-- Cartesian Y axis translation applied to RTXECEF Y coordinate to transform to RTK reference frame -->
        <xsd:element name="TranslationZ" type="distType"/>                  <!-- Cartesian Z axis translation applied to RTXECEF Z coordinate to transform to RTK reference frame -->
        <xsd:element name="SigmaE" type="distType"/>                        <!-- Estimated error in east axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="SigmaN" type="distType"/>                        <!-- Estimated error in north axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="SigmaU" type="distType"/>                        <!-- Estimated error in up axis translation derived from error estimate of measured points the translation is computed from -->
        <xsd:element name="BaseCheckResult" type="checkResultType"/>        <!-- Result of check on computed RTK-RTX offset compared to in-use offset at the base -->
        <xsd:element name="RoverCheckResult" type="checkResultType"/>       <!-- Result of check on computed RTK-RTX offset compared to in-use offset at the rover -->
        <xsd:element name="BRCheckResult" type="checkResultType"/>          <!-- Result of check on RTK-RTX offset transmitted from base compared to in-use offset at the rover -->
        <xsd:element name="Source" type="RTXRTKoffsetSourceType"/>          <!-- The source of the RTK-RTX offset parameters, the device which computed or obtained the offset -->
        <xsd:element name="ITRFEpoch" type="xsd:string"/>                   <!-- ITRF2008 epoch the offset was computed for, and to be applied to -->
        <xsd:element name="RtkPointId" type="xsd:string"/>                  <!-- The name of the RTK point used in determining the offset values -->
        <xsd:element name="RtxPointId" type="xsd:string"/>                  <!-- The name of the RTX point used in determining the offset values -->
        <xsd:element name="RtkBasePointId" type="xsd:string"/>              <!-- The name of the RTK base point used in the RTK survey -->
        <xsd:element ref="Notes" minOccurs="0"/>                            <!-- Present when note added to this item in 'Review current job'  -->
      </xsd:all>
      <xsd:attribute name="ID" type="idType" use="required"/>
      <xsd:attribute name="TimeStamp" type="timeStampType" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FieldBook">  <!-- The FieldBook element can contain any number of the elements referenced below according to the data that has been collected in the Survey Controller or Trimble Access job -->
    <xsd:annotation>
      <xsd:documentation>The FieldBook element contains the set of chronological elements providing details of all the data collected in the field job</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="FeatureCodingRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="UnitsRecord" type="unitsRecWithIDType" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element ref="CorrectionsRecord" maxOccurs="unbounded"/>
        <xsd:element ref="CoordinateSystemRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="EllipsoidRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ProjectionRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DatumRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HorizontalAdjustmentRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="VerticalAdjustmentRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="JobPropertiesRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="InstrumentRecord" type="instrumentRecWithIDType" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PointRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AtmosphereRecord" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="StationRecord" minOccurs="0" maxOccurs="unbounded"/>
